<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>b28d4021-90fc-4b86-9f90-6da64e11bd9a</id>   <name>PC GCD-R3 Order</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose>For ordering a(n) GCD-R3</purpose>       <use />       <misuse />     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value />       </item>       <item>         <key>Business Process Level</key>         <value />       </item>       <item>         <key>Care setting</key>         <value />       </item>       <item>         <key>Client group</key>         <value />       </item>       <item>         <key>Clinical Record Element</key>         <value />       </item>       <item>         <key>Copyright</key>         <value />       </item>       <item>         <key>Issues</key>         <value />       </item>       <item>         <key>Owner</key>         <value />       </item>       <item>         <key>Sign off</key>         <value />       </item>       <item>         <key>Speciality</key>         <value />       </item>       <item>         <key>User roles</key>         <value />       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]">     <items>       <item>         <key>Technical.EPIC ID</key>         <value>&lt;p&gt;COD4&lt;/p&gt;</value>       </item>       <item>         <key>Catalogue Pattern.Ordering Clinician Instructions</key>         <value>&lt;p&gt;R3 = Patient is expected to benefit from and is accepting of any appropriate investigations/interventions that can be offered including attempted resuscitation and ICU care, but excluding intubation and chest compression. • Resuscitation: is undertaken for acute deterioration but intubation and chest compression should not be performed • Life Support Interventions: may be offered without intubation and chest compression • Life Sustaining Measures: are used when appropriate • Major Surgery: is considered when appropriate • Transfer: is considered for diagnosis and treatment, if required&lt;/p&gt;</value>       </item>       <item>         <key>Catalogue Pattern.Synonyms</key>         <value>&lt;p&gt;GCD-R3; GCD; CODE STATUS; STATUS&lt;/p&gt;</value>       </item>       <item>         <key>Technical.ﻩ Technical Traceabililty</key>         <value>&lt;p&gt;{~AHSID~b28d4021-90fc-4b86-9f90-6da64e11bd9a~NAME~PC GCD-R3 Order}&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='Patients location of care where this GCD order was ordered']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Mandatory</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1]/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='Has this GCD been ordered after relevant conversation with the patient or alternate decision maker?']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Mandatory;  A response to this question may trigger a cascading question</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1]/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='Is this GCD order and interim order awating the outcome of a dispute resolution process?']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Mandatory: Cascading question available only when  ' No- this is an interim order prior to conversation' is selected</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='Clarifications']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Free text</value>       </item>     </items>   </annotations>   <definition xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" concept_name="Service request" max="1" name="GCD-R3">     <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="Patients location of care where this GCD order was ordered">       <Rule path="/items[at0002]">         <constraint xsi:type="textConstraint">           <includedValues>Home</includedValues>           <includedValues>Clinic/Facility/Hosptial</includedValues>         </constraint>       </Rule>     </Items>     <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1" concept_name="Adhoc Cluster Heading" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="Has this GCD been ordered after relevant conversation with the patient or alternate decision maker?">       <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Has this GCD been ordered after relevant conversation with the patient or alternate decision maker?">         <Rule path="/items[at0002]">           <constraint xsi:type="textConstraint">             <includedValues