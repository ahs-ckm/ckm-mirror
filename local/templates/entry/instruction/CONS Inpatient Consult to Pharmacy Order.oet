        <Rule path="/items[at0002]" name="Free text">
          <constraint xsi:type="textConstraint">
            <includedValues>Therapeutic drug monitoring</includedValues>
            <includedValues>Medication review</includedValues>
            <includedValues>Patient medication education</includedValues>
            <includedValues>Other</includedValues>
          </constraint>
        </Rule>
      </Items>
