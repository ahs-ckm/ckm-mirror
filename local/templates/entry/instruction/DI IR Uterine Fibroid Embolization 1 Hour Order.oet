        <value>&lt;table border=&quot;0&quot; cellpadding=&quot;0&quot; cellspacing=&quot;0&quot; style=&quot;width:272px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;Thoracic/Abdominal Embolization (150 WLU)&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;</value>
      </item>
