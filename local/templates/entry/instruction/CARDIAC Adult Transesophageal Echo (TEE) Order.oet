<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>eb60e7c3-6e40-48f8-a68e-d0d67fccb464</id>   <name>CARDIAC Adult Transesophageal Echo (TEE) Order</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose>For ordering a(n) Adult Transesophageal Echo (TEE)</purpose>       <use />       <misuse />     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value />       </item>       <item>         <key>Business Process Level</key>         <value />       </item>       <item>         <key>Care setting</key>         <value />       </item>       <item>         <key>Client group</key>         <value />       </item>       <item>         <key>Clinical Record Element</key>         <value />       </item>       <item>         <key>Copyright</key>         <value />       </item>       <item>         <key>Issues</key>         <value />       </item>       <item>         <key>Owner</key>         <value />       </item>       <item>         <key>Sign off</key>         <value />       </item>       <item>         <key>Speciality</key>         <value />       </item>       <item>         <key>User roles</key>         <value />       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]">     <items>       <item>         <key>Technical.EPIC ID</key>         <value>&lt;p&gt;ECH160&lt;/p&gt;</value>       </item>       <item>         <key>Imaging.Body Part</key>         <value>&lt;p&gt;%=CA&lt;/p&gt;</value>       </item>       <item>         <key>Imaging.Laterality</key>         <value>&lt;p&gt;No&lt;/p&gt;</value>       </item>       <item>         <key>Catalogue Pattern.Clinical Decision Support Requirements</key>         <value>&lt;p&gt;Agitated saline injection studies (Bubble Study) are not routinely performed on patients &amp;gt;65 years of age. For patients over 65, please page Echo attending physician on service to discuss.&lt;/p&gt;</value>       </item>       <item>         <key>Catalogue Pattern.Patient Requirements</key>         <value>&lt;p&gt;Nutrition: Nothing to eat or drink 6 hours prior to exam.; IV requirements: 20 gauge minimum.; Medications: Continue medications as usual with sip of water.&lt;/p&gt;</value>       </item>       <item>         <key>Catalogue Pattern.Clinical Communication</key>         <value>&lt;p&gt;Inpatient Clinical Communication: Complete Ultrasound exams before Nuclear Medicine imaging if possible. Patient transported by stretcher to the department. Patient should be dressed in hospital gown. Intact hospital and applicable allergy ID bands required. Height and weight must be recorded in CIS for US Cardiac exams. If patient requires sedation, please inform Ultrasound department. If difficult IV start please call Ultrasound, maximum 2 attempts on Unit.; Outpatient Clinical Communication: Intact hospital and applicable allergy ID bands required and provided at time of registration. Patient will be required to change into hospital gown upon arrival. Please follow nutrition requirements: Nothing to eat or drink 6 hours prior to exam. Continue medications as usual with sip of water.&lt;/p&gt;</value>       </item>       <item>         <key>Catalogue Pattern.Recommended Use</key>         <value>&lt;p&gt;If patient is diabetic, indicate to book early appointment.&lt;/p&gt;</value>       </item>       <item>         <key>Catalogue Pattern.Synonyms</key>         <value>&lt;p&gt;Echocardiogram;&lt;/p&gt;&lt;p&gt;Cardiac;&lt;/p&gt;&lt;p&gt;US;&lt;/p&gt;&lt;p&gt;Ultrasound;&lt;/p&gt;&lt;p&gt;TEE&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>       <item>         <key>Technical.ï»© Technical Traceabililty</key>         <value>&lt;p&gt;{~AHSID~eb60e7c3-6e40-48f8-a68e-d0d67fccb464~NAME~CARDIAC Adult Transesophageal Echo (TEE) Order}&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1 and name/value='Frequency']/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='Starting:']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Calendar pick default to today</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1 and name/value='Frequency']/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='At:']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Time entry default to now</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='Clinical Indication?']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Mandatory; A response to this question may trigger a cascading question; Free text box for comments</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]/description[at0009]/items[openEHR-EHR-CLUSTER.service_request_information.v1 and name/value='Specify']">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>Cascading question available if "Other: Specify" selected for "reason for exam?"; Free text; Free text box for comments</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-INST