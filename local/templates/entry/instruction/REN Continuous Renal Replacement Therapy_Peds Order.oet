       <Rule path="/items[at0002]" default="Routine">
         <constraint xsi:type="textConstraint" allowTerminologyLookUp="false">
           <includedValues>Add-On</includedValues>
           <includedValues>Routine (default)</includedValues>
           <includedValues>STAT</includedValues>
           <includedValues>Timed</includedValues>
           <includedValues>Today</includedValues>
           <includedValues>Urgent</includedValues>
         </constraint>
       </Rule>
     </Items>
     <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1" concept_name="Adhoc Cluster Heading" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="CRRT mode: CVVHDF, Anticoagulation type, Heparin or No Anticoagulation">
       <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="CRRT mode">
         <Rule path="/items[at0002]">
           <constraint xsi:type="textConstraint">
             <includedValues>CVVHDF</includedValues>
             <includedValues>CVVHD</includedValues>
             <includedValues>CVVH</includedValues>
           </constraint>
         </Rule>
       </Items>
       <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1" concept_name="Adhoc Cluster Heading" max="1" path="/items[at0002]" name="Anticoagulation type">
         <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Anticoagulation type">
           <Rule path="/items[at0002]">
             <constraint xsi:type="textConstraint">
               <includedValues>Heparin or No Anticoagulation</includedValues>
