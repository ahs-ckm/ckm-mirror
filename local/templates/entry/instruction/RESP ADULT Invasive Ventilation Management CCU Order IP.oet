        <Rule path="/items[at0002]">
          <constraint xsi:type="textConstraint">
            <includedValues>4 - 6 (ml/kg of predicted body weight</includedValues>
            <includedValues>6 - 10 (ml/kg of predicted body weight) (default)</includedValues>
            <includedValues>Other</includedValues>
          </constraint>
        </Rule>
      </Items>
