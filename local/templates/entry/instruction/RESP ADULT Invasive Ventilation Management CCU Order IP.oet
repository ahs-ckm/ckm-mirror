    <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1" concept_name="Adhoc Cluster Heading" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="Titrate to Maintain Target Saturation Range">
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Titrate to Maintain Target Saturation Range">
        <Rule path="/items[at0002]">
          <constraint xsi:type="textConstraint">
            <includedValues>90 - 95% (CCU) (default)</includedValues>
            <includedValues>92 - 96%</includedValues>
            <includedValues>88 to 92% (known CO2 retainer)</includedValues>
            <includedValues>Other (Specify)</includedValues>
          </constraint>
        </Rule>
      </Items>
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Target Saturation min"/>
