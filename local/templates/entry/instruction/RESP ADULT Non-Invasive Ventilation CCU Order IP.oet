  <definition xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" concept_name="Service request" max="1" name="Non-Invasive Ventilation, CCU">
    <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="Priority">
      <Rule path="/items[at0002]">
        <constraint xsi:type="textConstraint">
          <includedValues>Routine (default)</includedValues>
          <includedValues>STAT</includedValues>
        </constraint>
      </Rule>
    </Items>
    <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="Mode">
      <Rule path="/items[at0002]">
        <constraint xsi:type="textConstraint">
          <includedValues>NIV (BiPAP)</includedValues>
          <includedValues>CPAP</includedValues>
        </constraint>
      </Rule>
    </Items>
    <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1" concept_name="Adhoc Cluster Heading" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="NIV (BiPAP) Ventilation Goals">
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Ventilation Goals">
        <Rule path="/items[at0002]">
          <constraint xsi:type="textConstraint">
            <includedValues>pH 7.35 - 7.45</includedValues>
            <includedValues>pH range</includedValues>
            <includedValues>PaCO2 35 - 45 mmHg</includedValues>
            <includedValues>PaCO2 range (mmHg)</includedValues>
            <includedValues>Work of Breathing</includedValues>
            <includedValues>Other (Specify)</includedValues>
          </constraint>
        </Rule>
      </Items>
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="pH min"/>
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="pH max"/>
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="PaCO2 (min) mmHg"/>
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="PaCO2 (max) mmHg"/>
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Other"/>
    </Items>
    <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.adhoc_cluster_heading.v1" concept_name="Adhoc Cluster Heading" max="1" path="/activities[at0001]/description[at0009]/items[at0132]" name="NIV (BiPAP) Titrate to Maintain Target Saturation Range">
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Titrate to Maintain Target Saturation Range">
        <Rule path="/items[at0002]">
          <constraint xsi:type="textConstraint">
            <includedValues>90 - 95% (CCU) (default)</includedValues>
            <includedValues>92 - 96%</includedValues>
            <includedValues>88 to 92% (known CO2 retainer)</includedValues>
            <includedValues>Other (Specify)</includedValues>
          </constraint>
        </Rule>
      </Items>
      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1" concept_name="Service request information" max="1" path="/items[at0002]" name="Target Saturation min"/>
