    <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine EYE drop 0.5%; preservative free&apos;]/activities[at0001]">
      <items>
        <item>
          <key>CKT.Content Details</key>
          <value>&lt;p&gt;&amp;nbsp;1 drop, BOTH eyes, once, as needed, ROP Exam, Have available at bedside for Ophthalmology to administer&lt;/p&gt;</value>
        </item>
