      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>
      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="fed492db-9a3c-4739-a42f-6ad38f1386f0" concept_name="NURCOM CDS Technical Item - Do Not Order [NURCDS1] IP Order" max="1" path="/items" name="CDS Technical Item - Do Not Order"/>
