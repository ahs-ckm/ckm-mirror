<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>f75f182e-efa4-43fa-8297-c448d9820261</id>   <name>CVVHDF Heparin</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;*&amp;nbsp; CRRT mode: &lt;strong&gt;CVVHDF&lt;/strong&gt;; CVVHDF Intraoperative&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Anticoagulation type: &lt;strong&gt;Citrate&lt;/strong&gt;, Heparin; No Anticoagulation&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Prismaflex filter set: &lt;strong&gt;ST150 (Standard&lt;/strong&gt;); Other&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Blood flow rate (mL/min): &lt;strong&gt;150&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Patient weight:&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Prescribed effluent dose = weight (kg) x Dose (mL/kg/hr), rounded up to nearest 100 mL/hr: &lt;strong&gt;Standard 25 mL/kg/hr; &lt;/strong&gt;Other&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Total effluent dose (mL/hr):&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Replacement Solution – &lt;strong&gt;Prism0CAL with compartment A&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Pre-filter replacement rate via Replacement Pump (mL/hr): &lt;strong&gt;Standard: 50% of prescribed effluent dose (rounded up to the nearest 100 mL) ; &lt;/strong&gt;Other&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Pre-filter replacement rate via PBP (mL/hr):&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Dialysate Solution – &lt;strong&gt;Prism0CAL without compartment A&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Dialysate rate (mL/hr) = &lt;strong&gt;Prescribed effluent dose – pre-filter replacement (PBP) rate (mL/h);&lt;/strong&gt; Other (specify)&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Dialysate Flow Rate (mL/hr):&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Anticoagulation via pre Blood Pump (PBP): Sodium Citrate (IV Continuous, Usual starting rate 180 mL/h and 140 mL/h in patients with liver failure. Maximum rate is 220 mL/hr&lt;/p&gt;&lt;p&gt;*&amp;nbsp; Patient fluid removal rate (mL/hr):&lt;/p&gt;&lt;p&gt;Include: Urine, Gastric, Drains&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Dialysate and Replacement Potassium&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Potassium for addition to dialysate and replacement solution, as per CRRT Order Set.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Dialysate and Replacement Potassium&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Priming Solution - Heparin&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sodium chloride 0.9% infusion; 50 mL/hr, continuous&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Priming Solution - Heparin&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sodium chloride 0.9% infusion; 50 mL/hr, continuous&apos;]/activities[at0156]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Priming Solution - Heparin&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;heparin, preservative free injection; 1000 units/mL; continuous as needed, CRRT&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;5000 units should be mixed in NaCl 0.9% 1 L as needed for CRRT circuit priming.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Priming Solution - Heparin&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;heparin, preservative free injection; 1000 units/mL; continuous as needed, CRRT&apos;]/activities[at0156]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Heparin Infusion&apos;]">     <items>       <item>         <key>Transform.Technical Notes</key>         <value>&lt;p&gt;When checked, need to auto-select heparin infusion with infusion with initial bolus and re-bolus, and auto-select PTT&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Heparin Infusion&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;img src=&quot;https://ahsckm.ca/ckm/document?cid=1175.123.38&quot; alt=&quot;&quot; vspace=&quot;0&quot; hspace=&quot;0&quot; border=&quot;0&quot; style=&quot;border:0px solid black; height:147px; margin-bottom:0px; margin-left:0px; margin-right:0px; margin-top:0px; width:754px&quot;&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Heparin Infusion&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;PLACEHOLDER; heparin 250 units/mL syringe&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Start at 12 units/kg/hour (*** units/hr) &amp;nbsp;to a maximum 1000 units/hour.&amp;nbsp;&lt;br&gt;Follow titration nomogram for rate changes.&lt;/p&gt;&lt;p&gt;Interpretation &amp;nbsp; &amp;nbsp; Immediate Action &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Infusion Rate Change &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Repeat PTT&lt;/p&gt;&lt;p&gt;Very Low &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Rebolus 3000 units &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;Increase by 100 units/hr &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;4 hours&lt;br&gt;Low &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; -- &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;Increase by 50 units/hr &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;4 hours&lt;br&gt;Therapeutic &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;-- &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;-- &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;Next AM&lt;br&gt;High &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;Hold infusion 30 min &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Decrease by 100 units/hr &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;4 hours&lt;br&gt;Very High &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Notify MD &amp;amp; hold infusion 60 min &amp;nbsp; &amp;nbsp; &amp;nbsp;Decrease by 200 units/hr &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;4 hours&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Heparin Infusion&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;PLACEHOLDER; heparin 250 units/mL syringe&apos;]/activities[at0156]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Heparin Infusion&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;heparin, preservative free injection; 1000 units/mL, 3000 units&apos;]/activities[at0156]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Heparin Infusion&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Adjust infusion according to nomogram.&lt;/p&gt;&lt;p&gt;PTT(results in seconds)&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;BOLUS&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;RATE CHANGE&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;REPEAT PTT&lt;br&gt;Less than or equal to 50 &amp;nbsp;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;3000 units&amp;nbsp;&amp;nbsp; &amp;nbsp;+ 100 units/hour&amp;nbsp;&amp;nbsp; &amp;nbsp;4 hours&lt;br&gt;50-64&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; 0&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;+50 units/hour&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;4 hours&lt;br&gt;65-90 &amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; 0&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;0 (no change)&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;Next morning&lt;br&gt;91-125&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; 0&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;-100 units/hour&amp;nbsp;&amp;nbsp; &amp;nbsp;4 hours&lt;br&gt;Greater than 125 secs&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; 0&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;-200 units/hour&amp;nbsp;&amp;nbsp; &amp;nbsp;4 hours&lt;/p&gt;&lt;p&gt;PTT Sampling Process:&amp;nbsp;&lt;br&gt;1. PTT obtained from venous side (post-filter) of Prismaflex circuit&lt;br&gt;2. Document time for next PTT test&lt;br&gt;3. Document changes in dosage.&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Heparin Infusion&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]"><items><item><key>Technical.ﻩ Technical Traceability</key><value>{~AHSID~f75f182e-efa4-43fa-8297-c448d9820261~NAME~CVVHDF Heparin.oet}</value></item></items></annotations><definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="CVVHDF Heparin">     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="fcfa8558-f829-4127-96b0-452c7dca01c3" concept_name="REN Continuous Renal Replacement Therapy - Adult Order" max="1" path="/items" name="Continuous Renal Replacement Therapy Adult; CRRT CVVHDF Citrate Adult"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderable_section.v1" concept_name="Orderable Section" max="1" path="/items" name="Dialysate and Replacement Potassium">       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="48e649b4-dd84-47cd-8590-9887f989d08c" concept_name="PHARM potassium chloride 40 mmol in sterile water for injection 100 mL injection Order" max="1" path="/items" name="potassium chloride 40 mmol in sterile water for injection 100 mL injection; continuous as needed, CRRT"/>     </Item>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderable_section.v1" concept_name="Orderable Section" max="1" path="/items" name="Priming Solution - Heparin">       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="a9ef28ba-22e1-4bac-aa9a-e6b3a4172eff" concept_name="PHARM sodium chloride 0.9% infusion Order" max="1" path="/items" name="sodium chloride 0.9% infusion; 50 mL/hr, continuous"/>       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1