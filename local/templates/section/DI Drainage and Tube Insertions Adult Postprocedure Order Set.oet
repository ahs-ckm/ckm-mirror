<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>53dc91df-56c6-433a-a948-ce765c12c12d</id>  <name>DI Drainage and Tube Insertions Adult Postprocedure Order Set</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose />      <keywords>        <item>xyz</item>      </keywords>      <use />      <misuse />    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value />      </item>      <item>        <key>Business Process Level</key>        <value />      </item>      <item>        <key>Care setting</key>        <value />      </item>      <item>        <key>Client group</key>        <value />      </item>      <item>        <key>Clinical Record Element</key>        <value />      </item>      <item>        <key>Copyright</key>        <value />      </item>      <item>        <key>Issues</key>        <value />      </item>      <item>        <key>Owner</key>        <value />      </item>      <item>        <key>Sign off</key>        <value />      </item>      <item>        <key>Speciality</key>        <value />      </item>      <item>        <key>User roles</key>        <value />      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ﻩ Technical Traceability</key>        <value>{~AHSID~53dc91df-56c6-433a-a948-ce765c12c12d~NAME~DI Drainage and Tube Insertions Adult Postprocedure Order Set}</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Clinical Decision Support']/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;&lt;em&gt;Consider opening and merging the following order sets if appropriate, &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;- Postprocedure Management of Patients with Diabetes Mellitus, Adult&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;- PCA Adult Standard&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;- Anesthesia Postprocedure Adult&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;- Transfusion Medicine Adult&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Diet and Nutrition']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Diet Instructions to Nursing; Resume Previous Diet as Tolerated Now']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Resume previous diet as tolerated, encourage fluids for patients with no fluid restriction&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Diet and Nutrition']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Diet Instructions to Nursing; Resume Previous Diet as Tolerated 1 hour Post']/activities[at0001]">    <items>      <item>        <key>Transform.Technical Notes</key>        <value>&lt;p&gt;Starting H+1&lt;/p&gt;</value>      </item>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Resume previous diet as tolerated, encourage fluids for patients with no fluid restriction&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Activity']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Activity; Strict bed rest']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·Until discontinued, Starting today, Activity level: Strict Bed Rest. Monitor procedure site for *** hours and during first ambulation. Then activity as tolerated as per discharge criteria, Recovery &amp;amp; On Unit&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Activity']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Activity; Bed rest with bathroom privileges']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;Until discontinued, Starting today, Activity Level: Bed Rest with Exceptions. Bathroom Exceptions: Bathroom Privileges. Monitor procedure site for *** hours and during first ambulation. Then activity as tolerated as per discharge criteria&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Activity']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Activity; Adjust Head of Bed to']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;Once, Starting today, For 1 occurrence. Head of bed adjustment: Flat.&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Activity']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Activity; As tolerated']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;Until discontinued, Starting today, Until Specified.&amp;nbsp;&lt;/p&gt;</value>      </item>      <item>        <key>CKT.Phase of Care</key>        <value>&lt;p&gt;Recovery &amp;amp; On Unit.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Vital Signs']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; – Aldrete Score NOTE: not applicable if no procedural sedation given']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Once, Starting today For 1 Occurrences, Every 15 minutes for a minimum of 30 minutes and patient achieves Aldrete score greater than or equal to 8 or baseline&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Vital Signs']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Vital Signs; with Pulse Oximetry']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Per protocol, - Every 15 minutes for 1 hour - Every 30 minutes for 1 hour - Every hour for 2 hours or until bedrest is complete - THEN per unit protocol until discharge&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Vital Signs']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Pain Score; Pain Score and Nausea']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Per protocol, Starting today, Until Specified&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Intake and Output']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Intake and Output']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Per protocol, Include strict oral intake&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Intake and Output']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Intake and Output; Chart Drainage every Shift']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Chart drainage every shift or as per unit protocol. Empty full drainage bag.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='POCT Glucose']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT; Recovery &amp; On Unit']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;As needed, Starting today, Until Specified&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='POCT Glucose']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Notify Most Responsible Health practioner(MRHP); Blood Glucose']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Until discontinued, Starting Today, Until Specified. Blood Glucose Greater than (mmol/L): 18, Blood Glucose Less than (mmol/L): 4, Do not hold insulin without prescriber order. Initiate appropriate AHS Glycemic Management Protocol when targets are not met and call MRHP, Recovery &amp;amp; On Unit&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Assessments']/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Assessments']/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Post procedure site assessment; Observe Procedural site for signs of hematoma and/or bleeding at each assessment, Q15MIN for 1 hour']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Every 15 Min, Starting today For 1 Hours, If bleeding or swelling occurs, apply manual pressure STAT until hemostasis achieved. Restart all post-procedure vital signs and assessments. Recovery &amp;amp; On Unit&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Assessments']/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Post procedure site assessment; Observe Procedural site for signs of hematoma and/or bleeding at each assessment, Q30MIN for 1 hour']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;Every 30 Min, Starting today For 1 Hours, If bleeding or swelling occurs, apply manual pressure STAT until hemostasis achieved. Restart all post-procedure vital signs and assessments. Recovery &amp;amp; On Unit&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Assessments']/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Post procedure site assessment; Observe Procedural site for signs of hematoma and/or bleeding at each assessment, Q1H for 2 hours or until bedrest is complete']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;Every 1 Hour, Starting H+2 Hours For 2 Hours If bleeding or swelling occurs, apply manual pressure STAT until hemostasis achieved. Restart all post-procedure vital signs and assessments. Recovery &amp;amp; On Unit&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Assessments']/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Post procedure site assessment; Observe procedural site for signs of hematoma and/or bleeding at each assessment, per unit protocol until discharge criteria met']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Per protocol, Starting today, Until Specified, Recovery &amp;amp; On unit&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Assessments']/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Dressing Instructions:; Leave dressing in situ; change if soiled']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; As needed, Starting today, Until Specified, Action: Change, Recovery &amp;amp; On Unit&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Assessments']/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Dressing Instructions:; : Leave dressing in situ; reinforce if required']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;As needed, Starting today, Until Specified, Recovery &amp;amp; On Unit&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Interventions']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; Home Medications']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Until discontinued, Starting today, Until Specified. Patient may take their own home medications following procedure unless otherwise ordered, Recovery &amp;amp; On Unit&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Interventions']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; Home Medications']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Postprocedure Care (Floor/On Unit)']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care Interventions']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; Interventional Radiologist']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;Until discontinued, Startin