      </Rule>
      <Rule path="/activities[at0001]/description[at0002]/items[at0011]" max="0" />
    </Item>
    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.patient_care.v0" concept_name="Fake Patient care" max="1" path="/items" name="Notify">
      <Rule path="/activities[at0001]" name="Order" />
      <Rule path="/activities[at0001]/description[at0002]" name="Notify" />
      <Rule path="/activities[at0001]/description[at0002]/items[at0009]" name="Who">
        <constraint xsi:type="textConstraint">
          <includedValues>Attending Physician</includedValues>
        </constraint>
      </Rule>
      <Rule path="/activities[at0001]/description[at0002]/items[at0010]" name="When">
        <constraint xsi:type="textConstraint">
          <includedValues>if patient develops tremors, or HR &gt; 130 bpm</includedValues>
