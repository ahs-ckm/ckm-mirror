      <Rule path="/activities[at0001]/description[at0002]" name="Vital Signs" />
      <Rule path="/activities[at0001]/description[at0002]/items[at0009]" name="Specific Vital Signs">
        <constraint xsi:type="textConstraint">
          <includedValues>Blood Pressure</includedValues>
          <includedValues>Pulse</includedValues>
        </constraint>
      </Rule>
      <Rule path="/activities[at0001]/description[at0002]/items[at0010]">
        <constraint xsi:type="textConstraint">
          <includedValues>q1h</includedValues>
        </constraint>
      </Rule>
      <Rule path="/activities[at0001]/description[at0002]/items[at0011]" max="0" />
    </Item>
    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.patient_care.v0" concept_name="Fake Patient care" max="1" path="/items" name="Vital Signs - Temp">
      <Rule path="/activities[at0001]" name="Order" />
      <Rule path="/activities[at0001]/description[at0002]" name="Vital Signs" />
      <Rule path="/activities[at0001]/description[at0002]/items[at0009]" name="Specific Vital Signs">
