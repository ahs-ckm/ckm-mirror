  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderset.v0" concept_name="Mini Order Set" max="1" name="OSSM Vitals, Bronchodilator Therapy fake">
    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.patient_care.v0" concept_name="Fake Patient care" max="1" path="/items" name="Vital Signs - pre &amp; post bronchodilator">
      <Rule path="/activities[at0001]" name="Order" />
      <Rule path="/activities[at0001]/description[at0002]" name="Vital Signs" />
      <Rule path="/activities[at0001]/description[at0002]/items[at0009]" name="Specific Vital Signs">
        <constraint xsi:type="textConstraint">
          <includedValues>Respirations</includedValues>
          <includedValues>Oxygen Saturation</includedValues>
          <includedValues>Pulse</includedValues>
        </constraint>
      </Rule>
      <Rule path="/activities[at0001]/description[at0002]/items[at0010]">
        <constraint xsi:type="textConstraint">
          <includedValues>q15min</includedValues>
        </constraint>
      </Rule>
      <Rule path="/activities[at0001]/description[at0002]/items[at0011]" max="0" />
      <Rule path="/activities[at0001]/description[at0002]/items[at0012]">
        <constraint xsi:type="textConstraint">
          <includedValues>pre and 15 min post bronchodilator therapy</includedValues>
        </constraint>
      </Rule>
    </Item>
    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.patient_care.v0" concept_name="Fake Patient care" max="1" path="/items" name="Vital Signs - BP &amp; pulse">
