                  </annotations>
                  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -6 to Day -4&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Systemic Treatment #1&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;alemtuzumab injection; IV, over 2 hours, once, daily&apos;]/activities[at0001]">
                    <items>
                      <item>
                        <key>CKT.Clinical Guidance</key>
                        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Hypersensitivity precautions with alemtuzumab.&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;em&gt;&amp;nbsp;&amp;nbsp;&lt;/em&gt;Do not infuse other medications through the same IV line as alemtuzumab.&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Notify MD if patient experiences an infusion reaction to alemtuzumab.&lt;/p&gt;</value>
                      </item>
                      <item>
                        <key>CKT.Content Details</key>
                        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Dose:&amp;nbsp;0.2 mg/kg/dose&lt;/p&gt;&lt;p&gt;&amp;nbsp; Offset: 2.5 hours&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
                      </item>
                    </items>
                  </annotations>
                  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -1&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                    <items>
                      <item>
                        <key>CKT.Clinical Guidance</key>
                        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Start&amp;nbsp;on Day -1, maintaining a level of 200 to 300 mcg/L.&lt;/p&gt;&lt;p&gt;&amp;nbsp; The duration of cycloSPORINE treatment and tapering schedule will be patient dependent.&lt;/p&gt;</value>
                      </item>
                      <item>
                        <key>CKT.Content Details</key>
                        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Dose:&amp;nbsp;2.5 mg/kg/dose&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
                      </item>
                    </items>
                  </annotations>
                  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -1&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]">
                    <items>
                      <item>
                        <key>Transform.Preselected</key>
                        <value>&lt;p&gt;Y&lt;/p&gt;</value>
                      </item>
                    </items>
                  </annotations>
                  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day +1&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                    <items>
                      <item>
                        <key>CKT.Clinical Guidance</key>
                        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Give once daily until ANC is&amp;nbsp;greater than or equal to 5.0 x 10^9/L&amp;nbsp;for 2 days.&lt;/p&gt;</value>
                      </item>
                      <item>
                        <key>CKT.Content Details</key>
                        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Dose:&amp;nbsp;5 mcg/kg/dose&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
                      </item>
                    </items>
