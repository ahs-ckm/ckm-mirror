  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Oxygen Therapy - hypovolemia due to blood loss&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;If hypovolemia is due to blood loss, consider application of oxygen via a partial rebreather at 10-15 litres per minute&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Oxygen Therapy - hypovolemia due to blood loss&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;titrate to maintain target saturation range: 92 - 96%. Resevoir mask, 10 Litres per minute&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
