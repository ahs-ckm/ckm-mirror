<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>4d7a2b74-e0b6-4666-8717-197ce28c619a</id>   <name>Pediatric Emergency Antiemetics Order Panel</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.ï»© Technical Traceability</key>         <value>{~AHSID~6fe7e1a8-5951-4191-b450-4b0fe1286e59~NAME~Antiemetics Peds Order Panel.oet}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;strong&gt;Oral&lt;/strong&gt;: frequency: q6h. Patients less than 6 years, maximum dose 75mg per 24 hours, maximum single dose 25 mg; 6-12 years: 150 mg per 24 hours, or 50 mg per dose; greater than 12 years: 400 mg per 24 hours, or 100 mg per dose&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Injection&lt;/strong&gt;: frequency: q6h as needed. Patients less than 6 years, maximum dose 75 mg per 24 hours, maximum single dose 25 mg; 6-12 years: 150 mg per 24 hours, or 50 mg per dose; greater than 12 years: 400 mg per 24 hours, or 100 mg per dose&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Rectal&lt;/strong&gt;: frequency: q8h as needed. Patients less than 5 years: 25 mg maximum; 6-7 years: 25 mg every 8 hours; 8-11 years: 50 mg every 8 hours; over 12 years: 50 mg every 8 hours&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #2&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Ondansetron should be used as a single dose in most situations. Multiple doses, especially IV, can lead to QT prolongation.&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #3&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Ondansetron dose guidance:&lt;/p&gt;&lt;p&gt;8-15 kg: 2mg/dose&lt;/p&gt;&lt;p&gt;Greater than 15-30 kg: 4 mg/dose&lt;/p&gt;&lt;p&gt;Greater than 30 kg: 8 mg/dose&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Pediatric Emergency Antiemetics Order Panel">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_gu