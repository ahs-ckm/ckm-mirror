  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose Meter POCT; once&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;For patient with altered GCS.&lt;br&gt;If blood glucose is less than 4 mmol/L or greater than 18 mmol/L, notify the physician or NP and refer to the AHS Glycemic Management - Adult policy for appropriate management&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
