  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; border=&quot;1&quot; style=&quot;width:467px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;width:213px&quot;&gt;&lt;strong&gt;Dosing Weight&lt;/strong&gt;&lt;/td&gt;&lt;td style=&quot;width:251px&quot;&gt;&lt;strong&gt;&amp;nbsp;Suggested Dose&lt;/strong&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:213px&quot;&gt;&amp;nbsp;Less than or equal to 2 kg&lt;/td&gt;&lt;td style=&quot;width:251px&quot;&gt;&amp;nbsp;1 mL (400 mg)&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:213px&quot;&gt;&amp;nbsp;2.1 to 3 kg&lt;/td&gt;&lt;td style=&quot;width:251px&quot;&gt;&amp;nbsp;1.5 mL (600 mg)&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:213px&quot;&gt;&amp;nbsp;3.1 to 4 kg&lt;/td&gt;&lt;td style=&quot;width:251px&quot;&gt;&amp;nbsp;2 mL (800 mg)&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:213px&quot;&gt;&amp;nbsp;4.1 to 5 kg&lt;/td&gt;&lt;td style=&quot;width:251px&quot;&gt;&amp;nbsp;2.5 mL (1000 mg)&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:213px&quot;&gt;&amp;nbsp;5.1 to 6 kg&lt;/td&gt;&lt;td style=&quot;width:251px&quot;&gt;&amp;nbsp;3 mL (1200 mg)&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;glucose Gel 31g; *** mL buccal once as needed for hypoglycemia.&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Initial hypoglycemia treatment dose. Contact MRHP for any blood glucose level less than 1.8 mmol/L, any symptomatic patient with blood glucose level less than 2.6 mmol/L.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;glucose Gel 31g; *** mL buccal once as needed for hypoglycemia.&apos;]/activities[at0156]">
