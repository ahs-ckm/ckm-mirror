  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Renal&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;If presenting with acute kidney injury choose urinalysis, urine protein, urine microalbumin and urine creatinine&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Consider renal biolsy alongside consultation to nephrolopgy.&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
