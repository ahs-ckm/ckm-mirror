<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>f0958451-d8d6-4a32-9ea8-8f6dc4e7007d</id>  <name>Other Tests Newborn Order Group</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose />      <use />      <misuse />    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value />      </item>      <item>        <key>Business Process Level</key>        <value />      </item>      <item>        <key>Care setting</key>        <value />      </item>      <item>        <key>Client group</key>        <value />      </item>      <item>        <key>Clinical Record Element</key>        <value />      </item>      <item>        <key>Copyright</key>        <value />      </item>      <item>        <key>Issues</key>        <value />      </item>      <item>        <key>Owner</key>        <value />      </item>      <item>        <key>Sign off</key>        <value />      </item>      <item>        <key>Speciality</key>        <value />      </item>      <item>        <key>User roles</key>        <value />      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ï»© Technical Traceability</key>        <value>{~AHSID~f0958451-d8d6-4a32-9ea8-8f6dc4e7007d~NAME~Other Tests Newborn Order Group}</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Assess for jaundice as per provincial clinical practice guideline:&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;HYPERBILIRUBINEMIA SCREENING, ASSESSMENT AND TREATMENT (WELL NEWBORN GREATER THAN 35 WEEKS GESTATION) Guideline&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;https://extranet.ahsnet.ca/teams/policydocuments/1/clp-prov-womens-health-postpartum-hyperbilirubinemia-hcs-238-01.pdf&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Transform.Technical Notes</key>        <value>&lt;p&gt;Bilirubin Total and Newborn Jaundice Screen POCT Manual configured as single select options&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #1']">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Select one of the following Bilirubin tests&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Bilirubin, Total']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Within 36 hours of birth or prior to discharge.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Newborn Jaundice Screen POCT; Manual']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Daily, starting between 12 to 24 hours of age. &amp;nbsp;Babies who are clinically jaundiced can be assessed at any time.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT; As needed']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;for any symptomatic or at risk neonate as per Hypoglycemia Identification and Management in the Newborn Less than 72 hours of Age Protocol&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT; As needed']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Pulse oximetry: Critical Congenital Heart Disease Screen']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Obtain SpO2 on right wrist/hand [Pre-ductal] and one foot [Post-ductal] at 24 hours of age or prior to discharge. NOTE: Negative screen (pass) SpO2 greater than or equal to 95% in hand OR foot and the preductal to post ductal difference is less than or equal to 3%. Notify MRHP if pass is not achieved OR as per Facility guideline.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='EHDI Hearing Screening']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Do not screen infants with meningitis (proven or suspected) or bilateral aural atresia. &amp;nbsp;Refer directly to Audiology for a diagnostic auditory brainstem response (ABR) assessment. &amp;nbsp;&lt;br&gt;Babies are to be screened at greater than 15 hours (vaginal delivery) and greater than 22 hours (caesarean section) using the postpartum protocol for newborn hearing screening. &amp;nbsp;&lt;br&gt;Babies are to be screened as close to discharge as possible.&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='EHDI Hearing Screening']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Other Tests">    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Bilirubin Investigations - Routine">      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.section_supporting_context.v1" concept_name="Section supporting context" path="/items" />      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #1" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="7592e4e0-2356-414b-86d6-4abcfd9c18de" concept_name="LAB Bilirubin Total Order" max="1" path="/items" name="Bilirubin, Total" />      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="OR" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="3ece78db-190e-470d-9431-6f34fd2746e8" concept_name="LAB Newborn Jaundice Screen POCT Order" max="1" path="/items" name="Newborn Jaundice Screen POCT; Manual" />    </Item>    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="e8da34ee-6e55-438b-ba29-8a6c15655d40" concept_name="LAB Glucose Meter POCT Order" max="1" path="/items" name="Glucose Meter POCT; As needed" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="42391c72-1c4f-4d33-82b5-f3a864d711d3" concept_name="PC Pulse oximetry Order" max="1" path="/items" name="Pulse oximetry: Critical Congenital Heart Disease Screen" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="fe559d97-2912-41e3-94e9-97b7c7ea16c7" concept_name="PC EHDI Hearing Screening Order" max="1" path="/items" name="EHDI Hearing Screening" />  </definition>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.adhoc.v1">    <digest id="MD5-CAM-1.0.1">8AE70853975A75254E6D39A8DE3652AF</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1">    <digest id="MD5-CAM-1.0.1">4B2F0F6CD593E442DA9C30277A3EA9CF</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-INSTRUCTION.section_supporting_context.v1">    <digest id="MD5-CAM-1.0.1">A7EC4E4C870D8C3431B29F6514353416</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1">    <digest id="MD5-CAM-1.0.1">AD8DFB192E67715F0D9E11E95E674AD0</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1">    <digest id="MD5-CAM-1.0.1">230A8D964017E1FD7AC82EBDEE0202A6</digest>  </integrity_checks></template>