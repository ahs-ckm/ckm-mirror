  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Inpatient-modified PRAM Score Assessment&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Assessment by Inpatient-modified PRAM score every 1 to 2 hours as per pathway; Inpatient-modified PRAM assessment 15 minutes post salbutamol as per pathway&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
