<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>299720a2-1d8e-4442-a8db-f52686da4c86</id>  <name>Ketogenic Diet Pediatric Panel</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose />      <use />      <misuse />    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value />      </item>      <item>        <key>Business Process Level</key>        <value />      </item>      <item>        <key>Care setting</key>        <value />      </item>      <item>        <key>Client group</key>        <value />      </item>      <item>        <key>Clinical Record Element</key>        <value />      </item>      <item>        <key>Copyright</key>        <value />      </item>      <item>        <key>Issues</key>        <value />      </item>      <item>        <key>Owner</key>        <value />      </item>      <item>        <key>Sign off</key>        <value />      </item>      <item>        <key>Speciality</key>        <value />      </item>      <item>        <key>User roles</key>        <value />      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ﻩ Technical Traceability</key>        <value>{~AHSID~299720a2-1d8e-4442-a8db-f52686da4c86~NAME~Ketogenic Diet Pediatric Panel}</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Diet']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Pediatric diet; Ketogenic Diet']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Diet and intake per Neurology or Ketogenic Dietitian. No adjustments without their approval.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Diet']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Enteral Feeding with Tray NB/Inf/Ped']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Diet and intake per Neurology or Ketogenic Dietitian. No adjustments without their approval.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Diet']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Enteral Feeding without Tray NB/Inf/Ped']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Diet and intake per Neurology or Ketogenic Dietitian. No adjustments without their approval.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='POCT Blood Glucose Monitoring']/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Blood glucose monitoring should be ordered every 4 hours if patient is:&lt;/p&gt;&lt;ul&gt;&lt;li&gt;NPO&lt;/li&gt;&lt;li&gt;less than 1 year of age&lt;/li&gt;&lt;li&gt;tolerating less than 3/4 or more of allotted food or vomits 2 or more times in 24 hours&lt;/li&gt;&lt;/ul&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='POCT Blood Glucose Monitoring']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT every 4 hours']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;ul&gt;&lt;li&gt;If glucose 2.3 to&amp;nbsp;2.5 mmol/L and asymptomatic, recheck in 1 hour&lt;/li&gt;&lt;li&gt;If symptoms with glucose less than or equal to 2.5 mmol/L give 30 mL orange juice. If NPO refer to PRN IV dextrose orders.&lt;/li&gt;&lt;li&gt;If glucose less than or equal to 2.2 mmol/L give 30 mL orange juice. If NPO refer to PRN IV dextrose orders.&lt;/li&gt;&lt;/ul&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='POCT Blood Glucose Monitoring']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT PRN #1']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;ul&gt;&lt;li&gt;If glucose 2.3 to&amp;nbsp;2.5 mmol/L and asymptomatic, recheck in 1 hour&lt;/li&gt;&lt;li&gt;If symptoms with glucose less than or equal to 2.5 mmol/L give 30 mL orange juice. If NPO refer to PRN IV dextrose orders.&lt;/li&gt;&lt;li&gt;If glucose less than or equal to 2.2 mmol/L give 30 mL orange juice. If NPO refer to PRN IV dextrose orders.&lt;/li&gt;&lt;li&gt;Assess every 4 hours and PRN if patient is NPO or unable to tolerate 3/4 or more of allotted foods OR vomits 2 or more times in 24 hours&lt;/li&gt;&lt;/ul&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='POCT Blood Glucose Monitoring']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT PRN #1']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='POCT Blood Glucose Monitoring']/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='dextrose 10% in water infusion; 35 mL PRN']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;o&amp;nbsp;&amp;nbsp; PRN Reason: If patient NPO and has symptoms with glucose less than or equal to 2.5 mmol/L or if glucose less than or equal to 2.2 mmol/L&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='POCT Blood Glucose Monitoring']/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='dextrose 5% in water infusion; 70 mL PRN']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;o&amp;nbsp;&amp;nbsp; PRN Reason: If patient NPO and has symptoms with glucose less than or equal to 2.5 mmol/L or if glucose less than or equal to 2.2 mmol/L&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Notify']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Notify Most Responsible Health practioner (MRHP) - Blood Glucose']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Notify Neurologist if child unable to take ¾ or more of allocated food&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Notify']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Notify Most Responsible Health practioner (MRHP) - Blood Glucose']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Notify Neurologist if child vomits two or more times in a 12 hour period&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Notify']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Notify Most Responsible Health practioner (MRHP) - Blood Glucose']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Notify Neurologist if administering orange juice or IV dextrose for blood glucose &amp;lt; 2.5 mmol/L&lt;/p&gt;</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Ketogenic Diet Pediatric Panel">    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderable_section.v1" concept_name="Orderable Section" max="1" path="/items" name="Diet">      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="519e6cd8-a774-4226-b5ad-7d9ea53cf374" concept_name="NFS Pediatric Diet Order" max="1" path="/items" name="Pediatric diet; Ketogenic Diet" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="0c204318-b8dc-4aa0-aa1b-c7f4a608246b" concept_name="NFS Enteral Feeding with Tray NB/Inf/Ped Order" max="1" path="/items" name="Enteral Feeding with Tray NB/Inf/Ped" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="321159e3-47b2-4387-b4ff-ed70ee4f9967" concept_name="NFS Enteral Feeding without Tray NB_Inf_Ped Order" max="1" path="/items" name="Enteral Feeding without Tray NB/Inf/Ped" />    </Item>    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderable_section.v1" concept_name="Orderable Section" max="1" path="/items" name="POCT Blood Glucose Monitoring">      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />     