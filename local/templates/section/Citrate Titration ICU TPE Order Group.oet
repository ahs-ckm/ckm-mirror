<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>e0d653e0-40b0-4f1e-b2f7-d2ba02c8073a</id>  <name>Citrate Titration ICU TPE Order Group</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose />      <use />      <misuse />    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value />      </item>      <item>        <key>Business Process Level</key>        <value />      </item>      <item>        <key>Care setting</key>        <value />      </item>      <item>        <key>Client group</key>        <value />      </item>      <item>        <key>Clinical Record Element</key>        <value />      </item>      <item>        <key>Copyright</key>        <value />      </item>      <item>        <key>Issues</key>        <value />      </item>      <item>        <key>Owner</key>        <value />      </item>      <item>        <key>Sign off</key>        <value />      </item>      <item>        <key>Speciality</key>        <value />      </item>      <item>        <key>User roles</key>        <value />      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ﻩ Technical Traceability</key>        <value>{~AHSID~e0d653e0-40b0-4f1e-b2f7-d2ba02c8073a~NAME~Citrate Titration ICU TPE Order Group}</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>Transform.Technical Notes</key>        <value>&lt;p&gt;Nomograms ONLY&amp;nbsp;required in Sidebar Activity&lt;/p&gt;</value>      </item>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;&lt;strong&gt;&lt;u&gt;TPE Nomograms&lt;/u&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;4% sodium citrate&lt;/strong&gt;&lt;/p&gt;&lt;table border="0" cellspacing="0" cellpadding="0"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;table cellpadding="1" cellspacing="1" border="1"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;Circuit ionized calcium mmol/L (target 0.2 – 0.4 mmol/L)&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;4% sodium citrate Infusion Rate&lt;/strong&gt;&lt;/p&gt;&lt;strong&gt;(start at 180 mL/hr and 140 mL/hr in patients with liver failure)&lt;/strong&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;Greater than 0.5&lt;/td&gt;&lt;td&gt;Increase 30 mL/hr and call MD&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;0.41 to 0.5&lt;/td&gt;&lt;td&gt;Increase 20 mL/hr&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;0.2 to 0.4&lt;/td&gt;&lt;td&gt;No change&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;Less than 0.2&lt;/td&gt;&lt;td&gt;Decrease 20 mL/hr&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&lt;strong&gt;Calcium Chloride&lt;/strong&gt;&lt;/p&gt;&lt;table border="1" cellspacing="0" cellpadding="0"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;Systemic (arterial line) Ionized Calcium&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;Calcium CHLOride infusion rate&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;&amp;nbsp;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Repeat iCa&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;Less than 0.8 mmol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Notify MD. Stop citrate infusion for 30 minutes.&lt;/p&gt;&lt;p&gt;Give 10 mL 10% calcium CHLOride IV via central line over 1 hour and&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Increase&lt;/strong&gt;&amp;nbsp;calcium CHLOride infusion by 45 mL/hour.&lt;/p&gt;&lt;p&gt;Restart citrate infusion but&amp;nbsp;&lt;strong&gt;decrease&lt;/strong&gt;&amp;nbsp;rate by 30 mL/hour.&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;In 2 hours&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;0.8 – 0.85 mmol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;Increase&amp;nbsp;&lt;/strong&gt;by 45 mL/hour.&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;In 2 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;0.86 – 0.9 mmol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;Increase&amp;nbsp;&lt;/strong&gt;by 30 mL/hour.&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;In 4 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;0.91 – 0.99 mmol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;Increase&amp;nbsp;&lt;/strong&gt;by 15 mL/hour.&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;In 6 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;1.0-1.25 mmol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;No Change&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;In 6 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;Greater than 1.25 mmol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;strong&gt;Decrease&amp;nbsp;&lt;/strong&gt;by 15 mL/hour.&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;In 4 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Monitoring&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Consider monitoring calcium post filter if using plasma replacement due to risk of developing hypocalcemia from citrate in plasma&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;When using Citrate:&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Total and ionized patient calcium levels must be monitored. A Total Ca/ionized Ca ratio &amp;gt;2.5 indicates citrate toxicity.&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Patients receiving Plasma as a replacement solution are at increased risk of citrate toxicity due to the citrate in Plasma&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;4% citrate is a hypertonic solution (Na 408 mmol/L) therefore there is a risk of a rapid rise in Serum [Na], particularly in hyponatremic patients.&amp;nbsp; Exercise caution using citrate in these patients&lt;/em&gt;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Adjust citrate infusion rate as per nomogram - View the TPE Citrate Nomogram report on the Sidebar Summary activity.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Monitoring:&amp;nbsp;&lt;/strong&gt;The infusion of Citrate is titrated to the post-filter ionized calcium target goal selected. The post-filter ionized calcium must be drawn from theblue injection port on the hemofilter set.&lt;br&gt;Total and ionized patient calcium levels must be monitored. An increasing gap between patient’s total calcium and ionized calcium levels or elevated totalcalcium level indicates citrate toxicity.&lt;br&gt;Monitor patient and circuit post-filter ionized calcium levels Q1h.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Citrate Titration">    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Clinical Decision Support">      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />    </Item>    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ddd49576-2a7e-42d7-b258-bcb0b9b1a420" concept_name="PC Nursing Communication Order" max="1" path="/items" name="Nursing Communication - Citrate Titration" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.section_supporting_context.v1" concept_name="Section supporting context" path="/items" />  </definition>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.adhoc.v1">    <digest id="MD5-CAM-1.0.1">8AE70853975A75254E6D39A8DE3652AF</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1">    <digest id="MD5-CAM-1.0.1">4B2F0F6CD593E442DA9C30277A3EA9CF</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1">    <digest id="MD5-CAM-1.0.1">AD8DFB192E67715F0D9E11E95E674AD0</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1">    <digest id="MD5-CAM-1.0.1">230A8D964017E1FD7AC82EBDEE0202A6</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-INSTRUCTION.section_supporting_context.v1">    <digest id="MD5-CAM-1.0.1">A7EC4E4C870D8C3431B29F6514353416</digest>  </integrity_checks></template>