<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>bbbad6e1-1d27-4d0f-b266-1d8d2cbb7914</id>   <name>C Difficile Pediatric Inpatient Orders</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;strong&gt;Order Set Components&lt;/strong&gt;&lt;/p&gt;</value>       </item>     <item><key>Technical.ﻩ Technical Traceability</key><value>{~AHSID~bbbad6e1-1d27-4d0f-b266-1d8d2cbb7914~NAME~C Difficile Pediatric Inpatient Orders.oet}</value></item></items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;strong&gt;Order Set Restrictions:&lt;/strong&gt; For pediatric patients (&amp;lt;18 years old) who have tested positive for &lt;em&gt;Clostridioides difficile &lt;/em&gt;toxin by EIA and/or PCR test.&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Order Set Keywords:&lt;/strong&gt; Cdiff, C diff, CDI, diarrhea&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Order Set Requirements:&lt;/strong&gt; Weight&lt;/p&gt;&lt;p&gt;&lt;em&gt;&amp;nbsp;&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Order Set Layout:&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; General &lt;em&gt;C. difficile&lt;/em&gt; infection (CDI) orders&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;strong&gt;Smart Group: &lt;/strong&gt;Mild Illness&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;strong&gt;Smart Group: &lt;/strong&gt;Moderate Infection&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;strong&gt;Smart Group: &lt;/strong&gt;Severe Infection&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;span style=&quot;color:null&quot;&gt;&lt;strong&gt;General &lt;em&gt;Clostridioides&lt;/em&gt; (previously &lt;em&gt;Clostridium&lt;/em&gt;) &lt;em&gt;difficile&lt;/em&gt; infection (CDI) orders&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #2&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&lt;em&gt;Note: C. difficile testing is not indicated for patients with solid/formed stool.&lt;/em&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&lt;em&gt;In patients less than 2 years old, C. difficile testing is generally not indicated and consultation with microbiologist is needed before test request.&lt;/em&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&lt;em&gt;&amp;nbsp;&lt;/em&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&lt;em&gt;Do not test for C. difficile as test of cure; repeat test only if diarrhea recurs.&lt;/em&gt;&lt;/span&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #3&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;strong&gt;General&lt;/strong&gt;&lt;strong&gt;&amp;nbsp; &lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #4&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;If the patient does not have diarrhea:&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #5&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;For patients with diarrhea: &lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #6&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;strong&gt;Patient Care&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Clostridioides (previously Clostridium) difficile Infection (CDI) Pediatric Order Set">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="- #1"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #1"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="- #2"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #2"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="- #3"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #3"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Isolation"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #4">       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ddd49576-2a7e-42d7-b258-bcb0b9b1a420" concept_name="PC Nursing Communication Order" max="1" path="/items" name="Nursing Communication; No isolation or treatment or change in management."/>     </Item>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="- #5"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #5">       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="25bb4da1-7d1b-40ea-94c9-ad9b92cd1e40" concept_name="SURG ISOLATION Initiate contact isolation Order" max="1" path="/items" name="Initiate contact isolation; Contact Precautions (AHS sporicidal contact precaution sign for C. difficile) "/>       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ddd49576-2a7e-42d7-b258-bcb0b9b1a420" concept_name="PC Nursing Communication Order" max="1" path="/items" name="Nursing Communication; Contact Infection Prevention &amp; Control prior to discontinuation of Contact Precautions after formed stool for a minimum of 48 hours, or an alternate diagnosis is made."/>     </Item>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="- #6"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #6"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Patient care interventions">       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="07af740c-f7eb-4017-9aa2-c688f984867b" concept_name="PC Patient Education (Specify) Order" max="1" path="/items" name="Patient Education (Specify); Hand Hygiene. To prevent the spread of C. difficile, practice good hand hygiene. Remind family care providers to clean hands and help the child keep his or her hands clean by washing them well and often with soap and clean, runnin"/>     </Item>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="- #7"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" template_id="bfba8991-9a04-42c0-a70b-1ea320347f2c" concept_name="C Diff Mild Illness Smart Group" max="1" path="/items" name="Mild Illness; watery diarrhea without systemic toxicity, typically less than 4 abnormal stools/day"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="-"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" template_id="aab045f1-2fb1-410c-b42c-9a433f113ebe" concept_name="C Diff Moderate Infection" max="1" path="/items" name="Moderate Infection; 4 or more abnormal stools/day but no evidence of ileus, toxic megacolon or colonic perforation"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="- #4"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" template_id="4fdc54f9-a2f4-4656-be22-7492f45f52f9" concept_name="C Diff Severe Infection Smart Group" max="1" path="/items" name="Severe Infection; ileus, suspected toxic megacolon or colonic perforation "/>   </definition>   <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.adhoc.v1">     <digest id="MD5-CAM-1.0.1">8AE70853975A75254E6D39A8DE3652AF</digest>   </integrity_checks>   <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1">     <digest id="MD5-CAM-1.0.1">4B2F0F6CD593E442DA9C30277A3EA9CF</digest>   </integrity_checks>   <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-E