   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Neurological Vital Signs; Acute Post Thrombolysis and EVT; every 15 min for 2 hours&apos;]/activities[at0001]">
