<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>d77594a3-6e47-4a31-a462-9921efc2fcdc</id>   <name>Common Pediatric IV Fluids Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose />       <use />       <misuse />     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value />       </item>       <item>         <key>Business Process Level</key>         <value />       </item>       <item>         <key>Care setting</key>         <value />       </item>       <item>         <key>Client group</key>         <value />       </item>       <item>         <key>Clinical Record Element</key>         <value />       </item>       <item>         <key>Copyright</key>         <value />       </item>       <item>         <key>Issues</key>         <value />       </item>       <item>         <key>Owner</key>         <value />       </item>       <item>         <key>Sign off</key>         <value />       </item>       <item>         <key>Speciality</key>         <value />       </item>       <item>         <key>User roles</key>         <value />       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.ï»© Technical Traceability</key>         <value>&lt;p&gt;{~AHSID~d77594a3-6e47-4a31-a462-9921efc2fcdc~NAME~Common Pediatric IV Fluids Smart Group.oet}&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;strong&gt;&lt;em&gt;0.9% Saline Solutions&lt;/em&gt;&lt;/strong&gt;&lt;em&gt; are the preferred solution for most hospitalized patients. Plasmalyte or Ringers Lactate can be considered in clinical situations where chloride load needs to be reduced.&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;The following solutions to include: calculated rate (calculate maintenance, 75% maintenance, 50% maintenance, 1.5X maintenance, 2X maintenance) as well in addition to the calculated rates include IV rate options for TKVO, 5 mL/hour, 10 mL/hour, 20 mL/hour&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #1']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Non-dextrose Solutions:&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #2']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Solutions with D10:&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #3']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Hypotonic Solutions with 0.45% sodium chloride should not be used routinely for hospitalized patients (as they can contribute to hyponatremia) and should be reserved for clinical situations that require a more hypotonic solution&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #4']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Hypotonic Solutions with Sodium Chloride 0.2% should not be used routinely in pediatric patients; may be considered in the newborn and in specific clinical situations. Common newborn solution choices are provided here as options&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Common Pediatric IV Fluids Smart Group">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="24fc5d56-95fe-4de8-8ce0-e439da2e7acc" concept_name="PHARM dextrose 5%-sodium chloride 0.9% Infusion Order" max="1" path="/items" name="dextrose 5%-sodium chloride 0.9% Infusion" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="fd4babdb-0d6f-4e0e-bf77-327cc6b9bb47" concept_name="PHARM potassium chloride 20 mmol in dextrose 5%-sodium chloride 0.9% 1000 mL Infusion Order" max="1" path="/items" name="potassium chloride 20 mmol in dextrose 5%-sodium chloride 0.9% 1000 mL Infusion" />     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #1" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="a9ef28ba-22e1-4bac-aa9a-e6b3a4172eff" concept_name="PHARM sodium chloride 0.9% Infusion Order" max="1" path="/items" name="sodium chloride 0.9% Infusion" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="5f5cec5c-826e-4e02-b318-850480b60756" concept_name="PHARM potassium chloride 20 mmol in sodium chloride 0.9% 1000 mL Bag Order" max="1" path="/items" name="potassium chloride 20 mmol in sodium chloride 0.9% 1000 mL Bag"