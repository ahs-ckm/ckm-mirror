<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>936380c3-88e1-45b7-a6e7-792f2f28e40b</id>   <name>End of Life Adult Critical Care Order Set</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose />       <use />       <misuse />     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value />       </item>       <item>         <key>Business Process Level</key>         <value />       </item>       <item>         <key>Care setting</key>         <value />       </item>       <item>         <key>Client group</key>         <value />       </item>       <item>         <key>Clinical Record Element</key>         <value />       </item>       <item>         <key>Copyright</key>         <value />       </item>       <item>         <key>Issues</key>         <value />       </item>       <item>         <key>Owner</key>         <value />       </item>       <item>         <key>Sign off</key>         <value />       </item>       <item>         <key>Speciality</key>         <value />       </item>       <item>         <key>User roles</key>         <value />       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>CKT.Synonym(s)</key>         <value>&lt;p&gt;End of Life, Intensive Care, ICU, Critical Care, Tissue Donation, Organ Donation&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>       <item>         <key>Technical.ﻩ Technical Traceability</key>         <value>&lt;p&gt;{~AHSID~4bdf549f-4b28-4e6a-874a-980ca44755c7~NAME~Order Set Template.oet}&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Respiratory Assessments']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Weaning Parameters;  Mechanical ventilation weaning algorithm as per protocol']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&amp;nbsp;Mechanical ventilation weaning algorithm as per protocol&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;1.&amp;nbsp;Reduce FiO2 to 0.21 and PEEP to zero.&lt;/p&gt;&lt;p&gt;2.&amp;nbsp;If patient appears uncomfortable, promptly use bolus medication or increase infusions until comfortable.&lt;/p&gt;&lt;p&gt;3. This should not take more than 15 minutes.&lt;/p&gt;&lt;p&gt;4. Place patient on Pressure Support (PS 6-16 cm H2O). If patient uncomfortable, use bolus medication&lt;/p&gt;&lt;p&gt;5. or increase infusions until patient appears comfortable (15-minute goal).&lt;/p&gt;&lt;p&gt;6. If tolerating PS, reduce incrementally to 6 cm H2O. If patient uncomfortable, use bolus medication or&lt;/p&gt;&lt;p&gt;increase infusions until patient appears comfortable (15-minute goal).&lt;/p&gt;&lt;p&gt;7. Once patient is breathing regularly or if stable on PS 6, the patient is either extubated, or in cases of expected airway obstruction, is to remain intubated on room air.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Fluids/Electrolytes']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='IV Fluid Infusions']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Ringer's infusion']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;50 mL/hr&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Fluids/Electrolytes']/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='IV Fluid Infusions']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='sodium chloride 0.9% infusion']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;50 mL/hr&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" min="1" name="End of Life Adult Critical Care">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="General">       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Case Request" />       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Admission" />       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Transfer" />       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Anticipated Date of Discharge" />       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Goals of Care">         <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" template_id="ee59d121-2fbf-49fc-9bd6-0fa798162b4d" concept_name="Goals of Care Designation Order Panel" max="1" path="/items" name="Goals of Care Designation Order Panel" />       </Item>       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Isolation" />       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items