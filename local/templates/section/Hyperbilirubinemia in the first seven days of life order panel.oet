<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>67329b34-e968-4b08-989f-01401567d0c5</id>  <name>Hyperbilirubinemia in the first seven days of life order panel</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose/>      <use/>      <misuse/>    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value/>      </item>      <item>        <key>Business Process Level</key>        <value/>      </item>      <item>        <key>Care setting</key>        <value/>      </item>      <item>        <key>Client group</key>        <value/>      </item>      <item>        <key>Clinical Record Element</key>        <value/>      </item>      <item>        <key>Copyright</key>        <value/>      </item>      <item>        <key>Issues</key>        <value/>      </item>      <item>        <key>Owner</key>        <value/>      </item>      <item>        <key>Sign off</key>        <value/>      </item>      <item>        <key>Speciality</key>        <value/>      </item>      <item>        <key>User roles</key>        <value/>      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs&apos;]/activities[at0156]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Every 4 hours, Measure Temperature, Heart Rate, and Respiratory Rate only. Maintain thermoneutral environment&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Weigh Patient&apos;]/activities[at0156]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Daily, while recieiving phototherapy&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication 1&apos;]/activities[at0156]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Opaque covering for the eyes while in the vicinity of phototherapy light&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication 2&apos;]/activities[at0156]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Minimize interruptions to phototherapy as required for feeding, vital signs and newborn care.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication 3&apos;]/activities[at0156]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Document irradiance levels on initiation of phototherapy and whenever equipment repositioned.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Laboratory Investigations - Stat&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Total serum bilirubin (TSB) level should be drawn six hours following the initiation of phototherapy or four hours following initiation of phototherapy if previous TSB levels demonstrated a pattern of a rapidly rising bilirubin level, or with a critical TSB greater than 400 umol/L&lt;br&gt;&amp;nbsp;&lt;br&gt;Do not use transcutaneous bilirubinometry (TcB) if phototherapy is initiated&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Laboratory Investigations - Stat&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose-6-Phosphate Dehydrogenase (G6PD) Panel; Stat&apos;]/activities[at0156]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;1 application, BOTH eyes, 4 times per day, as needed, &amp;nbsp;for 48 hours, For use during phototherapy&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Inpatient Consults&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;If no lactation consultant available, consult clinician with breastfeeding experience&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ï»© Technical Traceability</key>        <value>{~AHSID~67329b34-e968-4b08-989f-01401567d0c5~NAME~Hyperbilirubinemia in the first seven days of life order panel}</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Hyperbilirubinemia in the first seven days of life order panel">    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Gui