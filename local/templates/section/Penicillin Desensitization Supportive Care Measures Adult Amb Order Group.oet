  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP)&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Stop administration of penicillin and notify MRHP for ANY suspected hypersensitivity reaction during Penicillin Desensitization Protocol&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
