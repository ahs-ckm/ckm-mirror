  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs; Per protocol&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Monitor vital signs prior to initiation of protocol, at least every 15 minutes for duration of protocol, and at 30 minutes and 60 minutes after first treatment dose&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs; Per protocol&apos;]/activities[at0001]/description[at0009]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Monitor vital signs prior to infusion, at least every ***&amp;nbsp;minutes with increment increase in rate during the infusion, and then every ***&amp;nbsp;minutes when maximum tolerated rate achieved, and then ***&amp;nbsp;minutes post infusion&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
