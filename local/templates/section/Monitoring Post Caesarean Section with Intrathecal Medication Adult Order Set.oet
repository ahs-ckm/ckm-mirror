  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; Postpartum&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;indomethacin Capsule&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;em&gt;Recommended standard dosing 25 to 50 mg. Suggested maximum 200 mg/day.&lt;/em&gt;&lt;/p&gt;</value>
      </item>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;50 mg, oral, 3 times per day, with meals, Suggested maximum 200 mg/day.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; Postpartum&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;indomethacin Suppository 100 mg&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;em&gt;Recommended standard dosing 50 to 100 mg. Suggested maximum 200 mg/day. Administer twice daily if dose is greater than 100 mg. &lt;/em&gt;&lt;/p&gt;</value>
      </item>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;100 mg, rectal, 2 times per day, Suggested maximum 200 mg/day.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; Postpartum&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;celecoxib Capsule&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;em&gt;Recommended only if proven history of ulcers, complicated perforation, obstruction or major bleeding. &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Recommended standard dosing 100 to 200 mg twice/day&lt;/em&gt;&lt;/p&gt;</value>
      </item>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;200 mg, every 12 hours, With meals&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Opioid Analgesics; Postpartum&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;morphine, short acting Tablet&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;em&gt;Recommended standard dosing 5 to 10 mg.&lt;/em&gt;&lt;/p&gt;</value>
      </item>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;5-10 mg, oral,&amp;nbsp;every 2 hours as needed,&amp;nbsp;for breakthrough pain.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Opioid Analgesics; Postpartum&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;HYDROmorphone, short acting Tablet&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;em&gt;Recommended standard dosing 1 to 2 mg.&lt;/em&gt;&lt;/p&gt;</value>
      </item>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;1-&amp;nbsp;2 mg, oral,&amp;nbsp;every 2 hours as needed,&amp;nbsp;for breakthrough pain.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Opioid Analgesics; Postpartum&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;oxyCODONE, short acting Tablet&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;em&gt;Recommended standard dosing 5 to 10 mg. May have higher rate of transfer into breast milk.&lt;/em&gt;&lt;/p&gt;</value>
      </item>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;5-10 mg, oral, every 2 hours, as needed, breakthrough pain&lt;/p&gt;&lt;p&gt;Admin Inst.: May have higher rate of transfer into breast milk.&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
