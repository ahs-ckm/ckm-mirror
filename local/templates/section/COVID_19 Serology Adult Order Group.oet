  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;table&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&amp;nbsp;&lt;/td&gt;&lt;td&gt;&lt;p&gt;If a COVID-19 NAT positive patient is unvaccinated or immunocompromised and considered for treatment with casirivimab/imdevimab, then COVID-19 serology (reason = &quot;for monoclonal antibody therapy&quot;) is indicated.&amp;nbsp;&lt;br&gt;&amp;nbsp;&lt;br&gt;&lt;strong&gt;- Manual: Casirivimab-Imdevimab Orders&amp;nbsp;&lt;/strong&gt;&lt;br&gt;&lt;em&gt;https://ahsweb.ca/HEE/COVID-19-Casirivimab-Imdevimab&lt;/em&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;COVID-19 Rapid Serology Test&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;table&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;This test&amp;nbsp;is&amp;nbsp;restricted based on site. If testing will be conducted in regional hospital-based APL labs in High Level, Grande Prairie, Fort McMurray, Red Deer, Lethbridge or Medicine Hat, CHOOSE:&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;COVID-19 Rapid Serology Test&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Is this serology test to evaluate eligibility for casirivimab/imdevimab?&amp;nbsp;&lt;strong&gt;Yes&lt;/strong&gt;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;COVID-19 Rapid Serology Test&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">
