       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine Gel 4%; For ages 1 month of age and up&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Clinical Guidance</key>
             <value>&lt;p&gt;Apply 0.75 grams&amp;nbsp;directly over vein&amp;nbsp;and cover with&lt;br&gt;occlusive dressing for 30 to&amp;nbsp;45 minutes preprocedure&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine-prilocaine Cream 2.5%-2.5% (EMLA); For ages 3 months of age and up&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Clinical Guidance</key>
             <value>&lt;p&gt;Apply 1-2.5&amp;nbsp;grams directly&amp;nbsp;over vein and cover with&amp;nbsp;occlusive&amp;nbsp;dressing for 45 to&amp;nbsp;60 minutes preprocedure&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For infants 1 month to 3 years of age&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Clinical Guidance</key>
             <value>&lt;p&gt;Apply 1 gram (2&amp;nbsp;cm length) to two sites directly over&amp;nbsp;veins and covered with an&amp;nbsp;occlusive&amp;nbsp;dressing for 30&amp;nbsp;minutes prior to procedure.&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For children age 4 years and up&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Clinical Guidance</key>
             <value>&lt;p&gt;Apply 2.5 grams&amp;nbsp;directly over vein&amp;nbsp;and cover with an&amp;nbsp;occlusive&amp;nbsp;dressing for 30&amp;nbsp;minutes prior to&amp;nbsp;procedure.&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sucrose ORAL liquid 24%; For ages 0 to four months&apos;]/activities[at0001]">
