<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>1c44040e-1fe2-4a3a-b9a5-a8220615e7d4</id>   <name>Atrial Fibrillation, Adult Inpatient Order Set</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&lt;strong&gt;Panels included:&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-size:12px&quot;&gt;Magnesium protocol panel&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size:12px&quot;&gt;Cardiac Potassium protocol panel&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-size:12px&quot;&gt;Amiodarone Panel&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&lt;strong&gt;Order Set Keywords: &lt;/strong&gt;Atrial fibrillation, Afib, Atrial flutter, Aflutter, Arrhythmia, Cardiac, Cardiology, Flutter, Fib&lt;/span&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Monitors&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Consider Rapid Response Team call for IV medications and Cardiac Monitoring for those units that do not have cardiac monitoring which is required to give many of the IV medications&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Notify Physician&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP); IV Diltiazem - if response indadequate 15 minutes after bolus dosing&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;ensure RN aware of respose expectations&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;GR Chest 1 Projection; Portable&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;anterior-posterior&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;GR Chest 2 Projections&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;posterior-anterior &amp;amp; lateral&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Other Tests&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;If not done in the last 6 months&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Anticoagulants&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;use caution if &amp;gt;130 kg&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiarrhythmics&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;** Consider only if patient is: hemodynamically unstable; NVAF and known duration is less than 12h without recent stroke/TIA; NVAF and known duration is &amp;lt;48h and low risk (CHADS2&amp;lt;2); adequate anticoagulation for at least 3 weeks; TEE confirms no left atrial thrombus present&lt;/p&gt;&lt;p&gt;** Anticoagulation is required for at least 4 weeks following cardioversion.&lt;/p&gt;&lt;p&gt;*** NVAF=Nonvalvular atrial fibrillation (absence of mechanical heart valve; any rheumatic mitral stenosis; moderate-severe nonrheumatic mitral stenosis)&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiarrhythmics&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;PLACEHOLDER - Vernakalant; ___ IV; infuse over 10 minutes&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;3 mg/kg (maximum dose: 339 mg)&lt;/p&gt;&lt;p&gt;If conversion to sinus rhythm does not occur within 15 minutes after the end of the initial infusion, a second infusion of 2 mg/kg (maximum dose: 226 mg) IV over 10 minutes may be administered.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiarrhythmics&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Procainamide IV&lt;/p&gt;&lt;p&gt;If pre-excited atrial fibrillation, procainamide should be used.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Loading dose&lt;/p&gt;&lt;p&gt;**Cardiac monitoring required**&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiarrhythmics&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;procainamide Injection; 100mg; direct IV; every 5 minutes until arrhythmia is controlled, adverse effects occur, or a total dose of 500 mg is administered&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;(Although it is unusual to require doses greater than 600 mg to initially control an arrhythmia, the MAX recommended total dose given is 1g.)&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiarrhythmics&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #2&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Maintenance dose&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiarrhythmics&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;procainamide infusion; ___ mg/minute; continuous IV&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;range: 1 to 4 mg/minute&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Beta Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Safety Parameters&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;PLACEHOLDER - Goal heart range: ___ to ___&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;suggest 60 to 110 at rest&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Beta Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;IV MetoPROLOL&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;** cardiac monitoring required**&lt;/p&gt;&lt;p&gt;Do not use concomitantly with IV Calcium Channel Blockers&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Beta Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;IV MetoPROLOL&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;metoPROLOL Injection; 2.5 mg; IV; every 5 minutes&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Maximum 15 mg over 10 to 15 minute period&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Beta Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;IV MetoPROLOL&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;metoPROLOL Injection; 5 mg; IV; every 5 minutes&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;maximum 15 mg over 10 to 15 minute period&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Beta Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;PO MetoPROLOL&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Use another medication if 200 mg of oral Metoprolol in 24 hours (maximum recommended) has not met goal heart rate&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Beta Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;PO bisoPROLOL&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Use another medication if 20 mg of oral BisoPROLOL in 24 hours (maximum recommended) has not met goal heart rate&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Calcium-Channel Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;IV DilTIAZem&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;** cardiac monitoring required**&lt;/p&gt;&lt;p&gt;Do not use if LV dysfunction present&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Do not use concomitantly with IV Beta-Blockers&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Calcium-Channel Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;IV DilTIAZem&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;dilTIAZem Injection; ___ mg; direct IV; once&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;0.25 mg/kg/dose;&lt;/p&gt;&lt;p&gt;Usual 15 to 20 mg&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;bolus dose given over 2 minutes&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Calcium-Channel Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;IV DilTIAZem&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;If response to first initial dose is inadequate after 15 minutes:&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Calcium-Channel Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;IV DilTIAZem&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;dilTIAZem Injection; ___ mg; direct IV; once #1&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;0.35 mg/kg/dose;&lt;/p&gt;&lt;p&gt;Usual 20 to 25 mg&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;bolus dose given over 2 minutes&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Calcium-Channel Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Continuous IV infusion DilTIAZem&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;usual 5 to 15 mg&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Start infusion following bolus dose(s) of dilTIAZem. Titration goal - HR less than110 but greater than 70&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Calcium-Channel Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Digoxin&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Monitor renal function if daily dosing&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Calcium-Channel Blockers&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Digoxin&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;digoxin Injection; 0.25 mg; direct IV; once&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;initial loading bolus&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adho