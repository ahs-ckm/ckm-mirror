<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>1db9d46c-d5ee-4070-b97a-e78fa2116856</id>  <name>Nicotine Replacement Therapy for Withdrawal Pediatric Inpatient</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose/>      <use/>      <misuse/>    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value/>      </item>      <item>        <key>Business Process Level</key>        <value/>      </item>      <item>        <key>Care setting</key>        <value/>      </item>      <item>        <key>Client group</key>        <value/>      </item>      <item>        <key>Clinical Record Element</key>        <value/>      </item>      <item>        <key>Copyright</key>        <value/>      </item>      <item>        <key>Issues</key>        <value/>      </item>      <item>        <key>Owner</key>        <value/>      </item>      <item>        <key>Sign off</key>        <value/>      </item>      <item>        <key>Speciality</key>        <value/>      </item>      <item>        <key>User roles</key>        <value/>      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ï»© Technical Traceability</key>        <value>{~AHSID~1db9d46c-d5ee-4070-b97a-e78fa2116856~NAME~Nicotine Replacement Therapy for Withdrawal Pediatric Inpatient}</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;&lt;strong&gt;&lt;span style=&quot;background-color:#f1c40f&quot;&gt;Criteria: (sugget add and clarify below)&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;Patient reports that he has never smoked.&lt;/p&gt;&lt;p&gt;He has been exposed to tobacco smoke.&lt;/p&gt;&lt;p&gt;He does not have any smokeless tobacco history on file.&lt;br&gt;&amp;nbsp;&lt;br&gt;Admission Tobacco use screen: Patient reports to use of tobacco/tobacco like products in the past 30 days.&lt;br&gt;&lt;span style=&quot;background-color:#f39c12&quot;&gt;to pharmacotherapy to reduce tobacco use during his admission. (suggest remove)&lt;/span&gt;&lt;br&gt;&amp;nbsp;&lt;br&gt;Assessing for Dependence&lt;br&gt;Interventions for nicotine withdrawal may be initiated during hospitalization to provide comfort to teens demonstrating nicotine dependence. Assess for dependence using the Hooked on Nicotine Checklist (HONC)&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;https://ahsweb.ca/HEE/Hooked on NIcotine Checklist (HONC)&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Monitoring for Toxicty&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Monitoring for Toxicty&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Provider to advise patient and/or parent/guardian to monitor for headache, increased heart rate, dizziness, confusion, agitation, restlessness, lethargy. Notify the physician of any signs of nicotine toxicity&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;&lt;span style=&quot;background-color:#f1c40f&quot;&gt;NEEDS TO BE CLARIFIED&lt;/span&gt;&lt;/p&gt;&lt;p&gt;Smoking tobacco can alter the metabolism of a number of medications, including caffeine. This is primarily due to substances in tobacco smoke. &amp;nbsp;&lt;br&gt;Vaping (Tobacco-like) Products (e.g. E-cigarettes or JUUL) :&lt;br&gt;Assessment of nicotine dependence is vital to ensure NRT is required for comfort.&lt;br&gt;Given varying concentrations of nicotine in products and no current evidence for recommendations on NRT, approach conservatively beginning with short-acting treatments and titrate up accordingly.&lt;br&gt;Withdrawal from high nicotine concentration products like JUUL may require consideration of long-acting treatment options.&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Nicotine Replacement Therapy&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Nicotine Replacement Therapy&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Patient may administer.&lt;br&gt;&amp;nbsp;&lt;br&gt;For occasional tobacco use (non-daily): Nicotine Replacement Therapy (NRT) not recommended&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Nicotine Replacement Therapy&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;For abstinence up to 16 hours OR light tobacco use; 10 or less cigarettes per day (not effective for smokeless tobacco users). Choose one of the following:&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Nicotine Replacement Therapy&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;For abstinence more than 16 hours AND moderate to heavy tobacco use; more than 10 cigarettes per day (or first cigarette within 30 minutes of waking). Choose one of the following:&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Consults/Referrals&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id=