  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine Gel 4%; For ages 1 month of age and up&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;Topical, Once, Apply 0.75 grams ½ tube directly over vein and cover with occlusive dressing for 30 to 45 minutes pre-procedure&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine-prilocaine Cream 2.5%-2.5% (Emla); For ages 3 months of age and up&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Topical, Once,&amp;nbsp;Apply 1- 2.5 grams directly over vein and cover with occlusive dressing for 45 to 60 minutes pre-procedure&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For infants 1 months to 3 years of age&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp; topical, once, Apply 1 gram (2 cm length) to two sites directly over veins and covered with an occlusive dressing for 30 minutes prior to procedure.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For children age 4 years and up&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp; topical, once, Apply 2.5 grams directly over vein and cover with an occlusive dressing for 30 minutes prior to procedure&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sucrose Oral liquid 24%; For ages 0 to four months&apos;]/activities[at0001]">
