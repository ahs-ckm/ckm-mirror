  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antibiotics&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;cefTRIXone AND AZIthomycin OR doxycycline&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;doxycycline capsule&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;doxycycline Capsule; 100 mg PO 2 times per day for 9 doses&apos;]/activities[at0001]">
