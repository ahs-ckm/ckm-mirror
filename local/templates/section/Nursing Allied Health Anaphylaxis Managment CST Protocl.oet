<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>091d3c11-90a9-4099-b6d7-e5d3a6d1e43d</id>  <name>Nursing Allied Health Anaphylaxis Managment CST Protocl</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose />      <use />      <misuse />    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value />      </item>      <item>        <key>Business Process Level</key>        <value />      </item>      <item>        <key>Care setting</key>        <value />      </item>      <item>        <key>Client group</key>        <value />      </item>      <item>        <key>Clinical Record Element</key>        <value />      </item>      <item>        <key>Copyright</key>        <value />      </item>      <item>        <key>Issues</key>        <value />      </item>      <item>        <key>Owner</key>        <value />      </item>      <item>        <key>Sign off</key>        <value />      </item>      <item>        <key>Speciality</key>        <value />      </item>      <item>        <key>User roles</key>        <value />      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Transform.Technical Notes</key>        <value>&lt;p&gt;Synonyms:&lt;/p&gt;&lt;p&gt;Nurse&lt;/p&gt;&lt;p&gt;Nursing&lt;/p&gt;&lt;p&gt;Allergic Reaction&lt;/p&gt;&lt;p&gt;Epi&lt;/p&gt;&lt;p&gt;Reaction&lt;/p&gt;&lt;p&gt;Nurse Ordering&lt;/p&gt;&lt;p&gt;Severe Reaction&lt;/p&gt;&lt;p&gt;Allergy&lt;/p&gt;&lt;p&gt;Epinephrine&lt;/p&gt;&lt;p&gt;CST&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>      <item>        <key>Technical.ï»© Technical Traceability</key>        <value>{~AHSID~091d3c11-90a9-4099-b6d7-e5d3a6d1e43d~NAME~Nursing Allied Health Anaphylaxis Managment CST Protocl}</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Anaphylaxis is&amp;nbsp;a serious life-threatening allergic reaction. &amp;nbsp;Alberta Health Services (AHS) supports the practice of a Registered Nurse (RN) to prescribe and in turn&amp;nbsp;administer intramuscular (IM)&amp;nbsp;epinephrine, from a one (1) mg/mL concentration ampoule to patients experiencing suspected anaphylaxis in practice situations where epinephrine&amp;nbsp;is used and accessible.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;The Anaphlaxis Management: RN Prescribing and Administering Intramuscular Epinephrine Clinical Support Tool (CST) Protocol is not intended to provide direction for health care professionals who administer epinephrine for suspected anaphylaxis with a patient-specific order or when an authorized presciber (e.g. Physician or Nurse Practitioner) is immediately available to provide an order.&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;For all non-pharmacologic nursing interventions, refer to the CST Protocol&lt;/p&gt;&lt;p&gt;For additional resources refer to the Anaphylaxis Managment Insite Page&lt;/p&gt;&lt;p&gt;Anaphylaxis Management Policy Suite&lt;/p&gt;&lt;p&gt;(http://ahsweb.ca/HEE/AHS_Anaphylaxis_Management_Provincial_Policy_Suite)&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;For additional resources refer to:&lt;/p&gt;&lt;p&gt;Anaphylaxis Managment Insite Page&lt;/p&gt;&lt;p&gt;(http://ahsweb.ca/HEE/Anaphylaxis_Management)&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Patient Care']/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;vital signs with oxygen saturation (if available). At least every 5 minutes, until the patient is showing no new or continued signs and symptoms of anaphylaxis, then as per practice setting standards, or as ordered by the MRHP.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Medications']/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance']">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Epinephrine -&amp;nbsp;&lt;strong&gt;LESS than 30 kg&lt;/strong&gt;&amp;nbsp;OR less than 8 years old (if weight not confirmed&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Medications']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='EPINEPHrine, anaphylaxis kit Kit 1 mg/mL;']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;0.15 mg, IM once.&lt;/p&gt;&lt;p&gt;Re-order/administer every 5 minutes, to a maximum of 3 doses, if the patient's condition does not improve.&lt;/p&gt;&lt;p&gt;STAT&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Medications']/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #1']">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Epinephrine -&amp;nbsp;&lt;strong&gt;30 kg or more&lt;/strong&gt;&amp;nbsp;OR 8 years and older (or if weight not confirmed)&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Medications']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='EPINEPHrine, anaphylaxis kit Kit 1 mg/mL #1;']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;0.3 mg, IM once.&lt;/p&gt;&lt;p&gt;Re-order/administer every 5 minutes, to a maximum of 3 doses, if the patient's condition does not improve&lt;/p&gt;&lt;p&gt;STAT&lt;/p&gt;</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Nursing/Allied Health Anaphylaxis Management CST Protocol">    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Patient Care">      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="3075bff7-28dd-4e96-816d-4cfddd0ee1d5" concept_name="PC Vital Signs Order" max="1" path="/items" name="Vital Signs" />    </Item>    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Medications">      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ff481662-45cc-4e90-9564-e3a03644012b" concept_name="PHARM EPINEPHrine, anaphylaxis kit Kit 1 mg/mL Order" max="1" path="/items" name="EPINEPHrine, anaphylaxis kit Kit 1 mg/mL;" />      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #1" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ff481662-45cc-4e90-9564-e3a03644012b" concept_name="PHARM EPINEPHrine, anaphylaxis 