  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Keep hyperkalemia therapies at bedside:&lt;br&gt;Anesthetic bag with oxygen flow to provide hyperventilation - Calcium chloride 10% (0.2 ml/kg) IV bolus - Sodium Bicarbonate 8.4% 1mmol/kg (1mL/kg) IV bolus - Albuterol continuous nebulizer 0.5%,(0.5mg/kg) to a maximum dose of 10mg nebulized&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
