  </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diuretics&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Furosemide greater than 25 kg&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Infuse furosemide using syringe pump over 5-30 minutes to avoid permanent ototoxicity and deafness.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diuretics&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Furosemide greater than 25 kg&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;furosemide ORAL liquid 10 mg/mL; 0.5 mg/kg/dose daily&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Maximum ORAL dose: 6 mg/Kg/dose, adult is 200mg/dose. Children maximum 6 mg/kg/dose&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diuretics&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Furosemide greater than 25 kg&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;furosemide infusion; 0.1 to 0.5 mg/kg/hour IV continuous&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;-&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Default concentration: 10 mg/mL&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Respiratory Tract Agents&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Nebulizers are restricted to patients known to be unresponsive to inhaler/MDIs or who are unable to use.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Salbutamol MDI: 2 to 6 inhalations every 20 minutes for 2 to 3 doses, then every 2 to 4 hours for 24 to 28 hours.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Respiratory Tract Agents&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Salbutamol MDI 100 mcg/actuation _____ puffs inhaled every ______,administration via spacer&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;o&amp;nbsp;&amp;nbsp; This is a restricted drug. What is the indication for this order?: Life-threatening respiratory disease (e.g. respiratory arrest, hypoventilation, continuous nebulization, end stage COPD, CF)&lt;/p&gt;&lt;p&gt;o&amp;nbsp;&amp;nbsp; Administration Instructions:&amp;nbsp; Utilize Airborne Precautions for this Aerosol-Generating Medical Procedure&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Respiratory Tract Agents&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Epinephrine 1 mg/mL injection for inhalation 5 mL every once&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;o&amp;nbsp;&amp;nbsp; Administration Instructions:&amp;nbsp; Utilize Airborne Precautions for this Aerosol-Generating Medical Procedure&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antivirals&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Not routinely recommended for COVID-19. Expert consultation suggested. Evidence from SARS suggests that if antivirals are to be used early initiation (within first 48 hours of admission) is more effective than delayed initiation.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antimalarials&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Hydroxychloroquine is the only option available option in Alberta.&lt;/p&gt;&lt;p&gt;Not routinely recommended for COVID-19. Expert consultation suggested.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Glucocorticoids&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Not routinely recommended for COVID-19 unless concomitant AECOPD, asthma, or other indication&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" 