                    <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -1&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                      <items>
                        <item>
                          <key>CKT.Clinical Guidance</key>
                          <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Prophylaxis for Thiotepa&lt;/p&gt;</value>
                        </item>
                        <item>
                          <key>CKT.Content Details</key>
                          <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Taper/Ramp&lt;/p&gt;&lt;p&gt;Day -1 to Day +1 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;Set dose to&amp;nbsp;0.5 mg/kg/dose&amp;nbsp;every 12 hours x 6 doses.&lt;/p&gt;&lt;p&gt;Day +2 to Day +3 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Set dose to&amp;nbsp;0.25 mg/kg/dose every 12 hours x 4 doses.&lt;/p&gt;&lt;p&gt;Day +4 to Day +5 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Set dose to&amp;nbsp;0.125 mg/kg/dose&amp;nbsp;every 12 hours x 4 doses.&lt;/p&gt;&lt;p&gt;Day +6 to Day +7 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Set dose to&amp;nbsp;0.125 mg/kg/dose&amp;nbsp;every 24 hours x 2 doses.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
                        </item>
                      </items>
                    </annotations>
                    <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day 0&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Orders&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                      <items>
                        <item>
                          <key>CKT.Content Details</key>
                          <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Infuse stem cells at least 48 hours after the last dose of thiotepa.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
                        </item>
                      </items>
                    </annotations>
                    <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day 0&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Orders&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]">
