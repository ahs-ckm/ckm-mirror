  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diazepam&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
