  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose Meter POCT; Diabetic Patients&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&lt;span style=&quot;font-family:Helvetica Neue&quot;&gt;&lt;span style=&quot;font-size:12px&quot;&gt;As needed; &lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-family:Helvetica Neue&quot;&gt;&lt;span style=&quot;font-size:12px&quot;&gt;For diabetic patients recieving Propanolol;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;span style=&quot;font-family:Helvetica Neue&quot;&gt;&lt;span style=&quot;font-size:12px&quot;&gt;Glucometer reading before propranolol&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-family:Helvetica Neue&quot;&gt;&lt;span style=&quot;font-size:12px&quot;&gt;Glucometer reading 30 minutes after propanolol&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-family:Helvetica Neue&quot;&gt;&lt;span style=&quot;font-size:12px&quot;&gt;Glucometer reading before FDG injection&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-family:Helvetica Neue&quot;&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&amp;nbsp;If &amp;gt;2 mmol/L change between glucometer readings, repeat glucometer readings q30 min x4&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span style=&quot;font-family:Helvetica Neue&quot;&gt;&lt;span style=&quot;font-size:12px&quot;&gt;&amp;nbsp;If &amp;lt;2 mmol/L change between readings, repeat glucometer reading once after imaging&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose Meter POCT; Non-diabetic Patients&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Once&lt;/p&gt;&lt;p&gt;Routine Glucose Meter POCT reading is obtained for standard FDG PET protocol&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
