  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;nce blood glucose is above 4.0 mmol/L follow Treatment of Hypoglycemia - ADULT procedure element 5.8&amp;nbsp;&lt;/p&gt;&lt;p&gt;- For patients where their clinical condition changes to conscious and able to swallow follow element 5.8 a) or b) &amp;nbsp;&lt;br&gt;- For patients receiving PN refer to element 5.8 c)&amp;nbsp;&lt;br&gt;- For patients that are NPO or unable to swallow refer to element 5.8 d)&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
