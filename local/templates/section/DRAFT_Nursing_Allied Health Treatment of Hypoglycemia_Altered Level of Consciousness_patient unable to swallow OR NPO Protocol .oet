  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose Meter POCT; as needed&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Repeat POCT blood glucose&amp;nbsp;15 minutes after each treatment&amp;nbsp; to maximum of two treatments.&lt;/p&gt;</value>
