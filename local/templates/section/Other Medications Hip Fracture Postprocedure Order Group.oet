       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Bone Resorption Inhibitors (for patients with osteoporosis)&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">
         <items>
           <item>
             <key>CKT.Clinical Guidance</key>
             <value>&lt;p&gt;&lt;em&gt;Contraindications: esophageal stricture or impaired swallowing, Creatinine Clearance less than 35 mL/Min. If patient has previously received zoledronic acid in the past year, or denosumab in the past 6 months&lt;/em&gt;&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Bone Resorption Inhibitors (for patients with osteoporosis)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;alendronate Tablet&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Content Details</key>
             <value>&lt;p&gt;70 mg Oral, weekly at least 30 minutes before breakfast. Start on POD 7. Patient to remain upright for at least 30 minutes after medication given&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Bone Resorption Inhibitors (for patients with osteoporosis)&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">
