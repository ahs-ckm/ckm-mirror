<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>656b50fb-d000-41c1-9e47-43b5ae90a667</id>   <name>Pediatric Insert and Maintain IV Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sodium chloride, lock solution&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Dose: 1-3 mL&lt;/p&gt;&lt;p&gt;Route: IV lock&lt;/p&gt;&lt;p&gt;Frequency: Every 8 hours and PRN following intermittent administration of fluid, medication or blood products&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Pediatric Topical Anesthetic Agents&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; border=&quot;1&quot; style=&quot;width:500px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;strong&gt;Agent&lt;/strong&gt;&lt;/td&gt;&lt;td&gt;&lt;strong&gt;Indications for Use&lt;/strong&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;tetracaine 4% gel&lt;/td&gt;&lt;td&gt;&lt;p&gt;Wait time 30 - 45 minutes&lt;/p&gt;&lt;p&gt;Contraindications:&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Need urgent access&lt;/li&gt;&lt;li&gt;Allergy to tetracaine&lt;/li&gt;&lt;/ul&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;lidocaine-prilocaine cream 2.5%-2.5% (EMLA)&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Wait time 45-60 minutes&lt;/p&gt;&lt;p&gt;Contraindications:&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Need urgent access&lt;/li&gt;&lt;li&gt;Prone to methemoglobinemia (i.e. G6PD deficiency)&lt;/li&gt;&lt;li&gt;Allergy to lidocaine&lt;/li&gt;&lt;/ul&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;lidocaine, liposomal 4% cream (MAXILENE)&lt;/td&gt;&lt;td&gt;&lt;p&gt;Wait time 30 minutes&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Safe for infants delivered at 37 weeks or greater or infants two weeks of age&lt;/li&gt;&lt;/ul&gt;&lt;p&gt;Contraindications:&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Need urgent access&lt;/li&gt;&lt;li&gt;Allergy to lidocaine&lt;/li&gt;&lt;/ul&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine 4% Gel&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;apply 1/2 tube directly over vein and cover with occlusive dressing for 30 to 45 minutes pre-procedure&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine-prilocaine 2.5%-2.5% Cream&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;apply directly over vein and cover with occlusive dressing for 45 to 60 minutes pre-procedure&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal 4% Cream&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;May be applied to two sites and covered with an occlusive dressing for 30 minutes prior to procedure&lt;/p&gt;&lt;p&gt;Dose: For neonates to 3 years old, use 1/5th tube or 2cm length of cream per site (equals 1 gram per site)&lt;/p&gt;&lt;p&gt;For 4 years old and OLDER, use 1/2 tube of cream per site (equals 2.5 grams per site)&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sucrose 24% ORAL liquid&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;2 mL, PO, once PRN for mild pain&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]"><items><item><key>Technical.ï»© Technical Traceability</key><value>{~AHSID~656b50fb-d000-41c1-9e47-43b5ae90a667~NAME~Pediatric Insert and Maintain IV Smart Group.oet}</value></item></items></annotations><definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Pediatric Insert and Maintain IV">     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="97c832cb-cfa5-421b-a