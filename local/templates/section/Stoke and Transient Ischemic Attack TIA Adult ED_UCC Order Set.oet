                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;&lt;strong&gt;For acute stroke patients: &lt;/strong&gt;drawing blood work and awaiting ancillary test results should not delay definitive imaging or decisions regarding treatment with acute stroke therapies.&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diet and Nutrition&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;Caution: Patients with acute disabling stroke should remain NPO, and oral medications held, until swallow screen has been completed and recommendations for appropriate food textures are available.&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diet and Nutrition&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]">
                     <items>
                       <item>
                         <key>Transform.Preselected</key>
                         <value>&lt;p&gt;Y&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Intake and Output&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;Intake and Output will be done once per shift. If a patient specific order is required, please specify below.&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;POCT Glucose&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;&lt;strong&gt;All patients with suspected acute ischemic stroke should have a rapid determination of blood glucose levels; hypoglycemia should be treated immediately.&lt;/strong&gt;&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;General Radiology&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;Consider Chest X-Ray if evidence of acute heart failure or pulmonary disease. Chest X-ray must NOT delay decision making or treatment with thrombolysis or endovascular therapy.&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;General Radiology&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;GR Chest 2 Projections; Urgent, Once&apos;]/activities[at0001]">
                     <items>
                       <item>
                         <key>CKT.Content Details</key>
                         <value>&lt;p&gt;&amp;nbsp;Is a mobile exam required?&amp;nbsp;No&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;General Radiology&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;GR Chest 1 Projection; STAT, Once&apos;]/activities[at0001]">
                     <items>
                       <item>
                         <key>CKT.Content Details</key>
                         <value>&lt;p&gt;&amp;nbsp;Is a mobile exam required? Yes&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Computed Tomography&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;CT Head and Neck Angiogram; STAT, Once&apos;]/activities[at0001]">
                     <items>
                       <item>
                         <key>CKT.Content Details</key>
                         <value>&lt;p&gt;&amp;nbsp;Acute Stroke Protocol (within 12 hours of onset of symptoms for Stroke/TIA)&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Computed Tomography&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;CT Head and Neck Angiogram; Urgen, Once&apos;]/activities[at0001]">
                     <items>
                       <item>
                         <key>CKT.Content Details</key>
                         <value>&lt;p&gt;&amp;nbsp;Non Acute Stroke Protocol (greater than 12 hours of onset of symptoms for Stroke/TIA)&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Ultrasound&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;Carotid Doppler ultrasound may be considered for carotid imaging in TIA / non-disabling ischemic stroke depending on availability and patient characteristics.&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Ultrasound&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                     <items>
                       <item>
                         <key>CKT.Content Details</key>
                         <value>&lt;p&gt;&amp;nbsp;Is a mobile exam required? No&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Magnetic Resonance&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;MR imaging may be considered in suspected acute ischemic stroke but must NOT delay decision making or treatment with thrombolysis or endovascular therapy&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Other Tests&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                     <items>
                       <item>
                         <key>CKT.Clinical Guidance</key>
                         <value>&lt;p&gt;&amp;nbsp;ECG should be performed in all patients with suspected acute ischemic stroke or TIA. Unless the patient is hemodynamically unstable, ECG must NOT delay decision making or treatment with thrombolysis or endovascular therapy.&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
                   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Other Tests&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                     <items>
                       <item>
                         <key>CKT.Content Details</key>
                         <value>&lt;p&gt;&amp;nbsp;Reason for Exam: Suspected acute ischemic stroke or TIA&lt;/p&gt;</value>
                       </item>
                     </items>
                   </annotations>
