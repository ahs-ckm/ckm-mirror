<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>994f58cd-7566-4d76-b78d-9b3ed4a5c4e9</id>   <name>COVID-19 ICU Anti-Infective Agents Order Panel</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose />       <use />       <misuse />     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value />       </item>       <item>         <key>Business Process Level</key>         <value />       </item>       <item>         <key>Care setting</key>         <value />       </item>       <item>         <key>Client group</key>         <value />       </item>       <item>         <key>Clinical Record Element</key>         <value />       </item>       <item>         <key>Copyright</key>         <value />       </item>       <item>         <key>Issues</key>         <value />       </item>       <item>         <key>Owner</key>         <value />       </item>       <item>         <key>Sign off</key>         <value />       </item>       <item>         <key>Speciality</key>         <value />       </item>       <item>         <key>User roles</key>         <value />       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.ï»© Technical Traceability</key>         <value>{~AHSID~2d989060-8f0c-4e30-9549-a6b7b98d1111~NAME~ICU Anti-Infective Agents Order Panel.oet}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #1']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Empiric Antimicrobial Therapy of Hospitalized Suspect COVID-19 Patients&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;REASSESS WITH VIRAL AND BACTERIAL LAB RESULTS&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Pending confirmation COVID-19 positive AND bacterial culture negative. &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;For pregnant patients - choose Azithromycin. Doxycycline NOT recommended&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='cefTRIAXone injection; 1000 mg, Intravenous, daily, for 3 doses. Reassess after 48-72 hours. On unit']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;IF patient greater than 100kg, give 2g every 24 hours&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='vancomycin IV']/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Recommended loading dose 25 to 30 mg/kg/dose, maximum 3000 mg/dose.&lt;/p&gt;&lt;p&gt;Recommended maintenance dose 15 mg/kg/dose&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='vancomycin IV']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='vancomycin injection; 15 mg/kg/dose, Intravenous, Every 12 hours for 3 days. Starting 12 hours after initial 25 mg/kg/dose dose. Target trough. Reassess in 48-72 hours. ']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory cultures are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid']/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;All patients with functional GI tract should use oral formulation, use IV formulation only if non-functional GI tract.&lt;/p&gt;&lt;p&gt;Alternate if renal dysfunction or known prior MRSA pneumonia&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid injection; 600 mg, Intravenous, every 12 hours, for 6 doses.']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 48-72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory cultures are negative for MRSA. Use IV formulation only if non-functional GI tract&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid Tablet; 600 mg, Oral, every 12 hours, for 6 doses']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 48-72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory cultures are negative for MRSA.&amp;nbsp;Encouraged for all patients with functional GI tract&lt;/em&gt;.&amp;nbsp;&lt;em&gt;Encouraged for all patients with functional GI tract.&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;span style="color:inherit"&gt;On Unit&lt;/span&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #3']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Management of Possible Secondary Bacterial Infection/Ventilator Associated Pneumonia in Adult COVID-19 patients&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Worsening pneumonia may also be due to inflammation so prolonged antibiotic therapy beyond 5- 7 days in the absence of positive cultures is not currently recommended. &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Culture directed therapy is preferred.&amp;nbsp; Empiric therapy pending sputum/bronch culture results:&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #4']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;If history of MRSA carriage or high suspicion MRSA, Add&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Vancomycin IV']/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Recommended loading dose 25-30 mg/kg/dose, maximum 3000 mg/dose. Recommended maintenance dose 15 mg/kg/dose&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Vancomycin IV']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='vancomycin injection; 15 mg/kg/dose, Intravenous, Every 12 hours for 3 days. Starting 12 hours after initial 25 mg/kg/dose dose. Target trough. Reassess in 748-2 hours. ']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory cultures are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid #1']/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;All patients with functional GI Tract should use Oral formulation, use IV formulation only if non-functional GI tract&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Alternate if renal dysfunction or known prior MRSA pneumonia&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid #1']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid injection; 600 mg, Intravenous, every 12 hours, for 6 doses']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 48-72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory cultures are negative for MRSA. Use IV formulation only if non-functional GI tract&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid #1']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid Tablet; 600 mg, Oral, every 12 hours, for 6 doses.']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 48-72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory cultures are negative for MRSA. Encouraged for all patients with functional GI tract&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="COVID-19 ICU Anti-Infective Agents Order Panel">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #1" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="46facdb5-2f39-40f1-9c04-7f66e4c25df5" concept_name="PHARM cefTRIAXone injection Order" max="1" path="/items" name="cefTRIAXone injection; 1000 mg, Intravenous, daily, for 3 doses. Reassess after 48-72 hours. On unit" />     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="AND" />     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderable_section.v1" concept_name="Orderable Section" max="1" path="/items" name="Doxycycline">       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="6c343f9d-ab54-4fd7-8b40-6c42c6356b51" concept_name="PHARM doxycycline Capsule Order" max="1" path="/items" name="doxycycline Capsule; 200 mg, oral, once, On unit" />       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance