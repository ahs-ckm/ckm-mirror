    <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Laboratory Investigations Routine&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Urine&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
      <items>
        <item>
          <key>CKT.Content Details</key>
          <value>&lt;p&gt;Frequency: Once&lt;/p&gt;&lt;p&gt;Specimen Type: Urine&lt;/p&gt;&lt;p&gt;Specimen Source: Urine, Catheter&amp;nbsp;&lt;span style=&quot;color:#e74c3c&quot;&gt;&lt;strong&gt;&lt;em&gt;Question for author: Is would this specimen be taken from a bag or a catheter?&lt;/em&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;</value>
        </item>
      </items>
    </annotations>
    <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Laboratory Investigations Routine&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Microbiology&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">
