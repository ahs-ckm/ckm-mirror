  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;lactulose&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Dose 1 to 2 g/kg/day (equivalent to 1.5 to 3 mL/kg/day)&lt;/p&gt;&lt;p&gt;Administered OD or divided into BID dosing.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;lactulose&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">
