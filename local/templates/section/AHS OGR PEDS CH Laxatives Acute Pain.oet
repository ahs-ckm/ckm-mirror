  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;sennosides&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Age based dosing&lt;/p&gt;&lt;p&gt;2-6 years: 4.4 – 6.6 mg (2.5-3.75 mL)&lt;/p&gt;&lt;p&gt;6-12 years: 8.8-13.2 mg (5-7.5 mL)&lt;/p&gt;&lt;p&gt;Greater than 12 years: 17.6-26.4 mg (10-15 mL)&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
