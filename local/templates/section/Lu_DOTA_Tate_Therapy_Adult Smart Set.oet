  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Lu-DOTA-Tate Therapy&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Lu-177 dotatate&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;***MBq&lt;/p&gt;&lt;p&gt;Administer over 1-2 hours, Start 1 to 3 hours after START of AAIS product and concurrently with AAIS product infusion&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Lu-DOTA-Tate Therapy&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Non Standard Medication&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;1,000 mL, intravenous, once&lt;/p&gt;&lt;p&gt;&amp;nbsp;Administer over 4 hours. Infusion rates: 150 mL/hr to 375 mL/hr. Adjust rate according to tolerance., Routine&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Per protocol Until Specified,&lt;/p&gt;&lt;p&gt;Temperature, Blood Pressure, Respiratory Rate, Pule, and Saturations -Prior to Amino Acid Infusion Solution infusion&lt;/p&gt;&lt;p&gt;-Prior to Lu-DOTA-TATE infusion&lt;/p&gt;&lt;p&gt;-15 minutes into Lu-DOTA-TATE infusion&lt;/p&gt;&lt;p&gt;-After Amino Acid Infusion Solution infusion&lt;/p&gt;&lt;p&gt;Inpatient Treatment - Signed/Held&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiemetics&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;dimenhyDRINATE PO/IV&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #2&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Avoid dimenhyDRINATE in patients 65 years of age or older due to increased risk of side effects including delirium. Suggest 25 mg for mild/moderate nausea, 50 mg for moderate/severe nausea&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiemetics&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;dimenhyDRINATE PO/IV&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;dimenhyDRINATE Tablet&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;25-50 mg, oral, every 4 hours, as needed, nausea, vomiting, Starting today at 1113, Inpatient Treatment - Signed/Held&lt;br&gt;Routine, Sign and Hold&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antiemetics&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;dimenhyDRINATE PO/IV&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">
