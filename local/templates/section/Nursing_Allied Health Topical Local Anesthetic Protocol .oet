  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;(Link display name: APPLICATION OF TOPICAL LOCAL ANESTHETIC FOR POTENTIALLY PAINFUL SKIN-BREAKING PROCEDURES protocol HCS-265-01&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;(This is the web address: https://extranet.ahsnet.ca/teams/policydocuments/1/clp-ahs-topical-anesthetic-hcs-265-01.pdf&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;!--StartFragment --&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For infants 1 month to 3 years of age&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Apply 1 gram (2&amp;nbsp;cm length) to two sites directly over&amp;nbsp;veins and covered with an&amp;nbsp;occlusive&amp;nbsp;dressing for 30&amp;nbsp;minutes prior to procedure.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For infants 1 month to 3 years of age&apos;]/activities[at0156]">
    <items>
      <item>
        <key>Transform.Clinical Notes</key>
        <value>&lt;p&gt;Duration of Action 1-2 hours&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For children age 4 years and up&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Apply 2.5 grams&amp;nbsp;directly over vein&amp;nbsp;and cover with an&amp;nbsp;occlusive&amp;nbsp;dressing for 30&amp;nbsp;minutes prior to&amp;nbsp;procedure.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; For children age 4 years and up&apos;]/activities[at0156]">
