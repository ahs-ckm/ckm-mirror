  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine Gel 4%; For ages 1 month of age and up&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Apply 0.5 - 1 grams&amp;nbsp;directly over vein&amp;nbsp;and cover with&lt;br&gt;occlusive dressing for 30 to&amp;nbsp;45 minutes prior to procedure&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine Gel 4%; For ages 1 month of age and up&apos;]/activities[at0156]">
