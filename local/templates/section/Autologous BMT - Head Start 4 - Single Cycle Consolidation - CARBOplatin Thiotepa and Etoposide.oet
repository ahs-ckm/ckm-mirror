                <item>
                  <key>CKT.Clinical Guidance</key>
                  <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Initiate program specific guidelines for thiotepa skin care.&lt;/p&gt;</value>
                </item>
                <item>
                  <key>CKT.Content Details</key>
                  <value>&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; border=&quot;0&quot; style=&quot;width:514px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;width:275px&quot;&gt;Conditions:&lt;/td&gt;&lt;td style=&quot;width:238px&quot;&gt;Modifications:&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:275px&quot;&gt;Age less than 6 years&lt;/td&gt;&lt;td style=&quot;width:238px&quot;&gt;Set dose to&amp;nbsp;10 mg/kg/dose&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:275px&quot;&gt;Age greater than or equal to 6 years&lt;/td&gt;&lt;td style=&quot;width:238px&quot;&gt;Set dose to&amp;nbsp;300 mg/m2/dose&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;Offset: 0&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
