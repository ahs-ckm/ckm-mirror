  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;PICU/ED ONLY Repeated Dosing Salbutamol Nebulizer&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;5 mg, Inhalation, every 15 mins for 3 doses.&lt;/p&gt;&lt;p&gt;Once a total of 15 mg administered, then reassess.&lt;/p&gt;</value>
