<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>02b0ba00-0a43-4c69-9eae-12a841c8b1a9</id>   <name>Oxygen Therapy Order Panel</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Oxygen Therapy: All presentations EXCEPT Acute Coronary Syndrome, known CO2 retainers and Carbon Monoxide Poisoning.&amp;nbsp;All other presentations (including pregnancy and acute stroke) should adhere to the following SPO2 goals. NOTE: For acute stroke, don&apos;t apply supplemental oxygen unless SPO2 is under 90%&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Oxygen Therapy&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Titrate oyxgen to maintain saturation range of SpO2 92% to 96%&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Routine&lt;/p&gt;&lt;p&gt;Titrate to Maintain Target Saturation Range: 92 to 96%&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Oxygen Therapy: Known CO2 retainer&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Oxygen Therapy #1&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Titrate Oxygen to maintain saturation range SpO2 88% to 92%&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Routine&lt;/p&gt;&lt;p&gt;Titrate to Maintain Target Saturation Range: 88 to 92%&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #2&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Oxygen Therapy: All Acute Coronary Syndromes (ACS)&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Oxygen Therapy #2&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;When SpO2 is under 90%, titrate Oxygen to maintain saturation range SpO2 90% to 92%&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Routine&lt;/p&gt;&lt;p&gt;Titrate to Maintain Target Saturation Range: 90 to 92%&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #3&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Oxygen Therapy: Most Responsible Healthcare Provider (MRHP) Notifications&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP)&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Notify MRHP if Oxygen flow increase by greater than 2L/min from previous to maintain the same level of oxygentation, or if there is a progressive increase in the work of breathing&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP) #1&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Notify MRHP if a new change to Oxygen flow of 8L/min or higher to maintain same level of oxygenation&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]"><items><item><key>Technical.ï»© Technical Traceability</key><value>{~AHSID~02b0ba00-0a43-4c69-9eae-12a841c8b1a9~NAME~Oxygen Therapy Order Panel.oet}</value></item></items></annotations><definition x