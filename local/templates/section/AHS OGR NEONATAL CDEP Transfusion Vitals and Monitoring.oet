  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs; Transfusion, per protocol&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Check Temperature, Blood Pressure, Heart Rate, Respiratory Rate and Oxygen Saturation: pre-transfusion, every 15 minutes for the first hour, every 30 minutes for the second and third hours, and then every hour until transfusion complete. Revert to previous routine after transfusion completed.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs; Transfusion, per protocol&apos;]/activities[at0156]">
