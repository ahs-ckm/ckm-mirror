  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Modified Bromage Scale Assessment&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Frequency: q1h&lt;/p&gt;&lt;p&gt;For: 3 occurences&lt;/p&gt;&lt;p&gt;Comments: Report any alterations in motor power to Most Responsible Anesthesia Provider.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
