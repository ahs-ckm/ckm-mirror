<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>6da43792-d033-4955-873d-c6b42ceae6bb</id>   <name>ALL SCTped 2012 FORUM Conditioning 1 - MSD and MD - TBI, Etoposide, ATG - ORDER SET</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.ﻩ Technical Traceability</key>         <value>{~AHSID~6da43792-d033-4955-873d-c6b42ceae6bb~NAME~Allogeneic BMT (MA) for ALL – ALL SCTped 2012 FORUM Study Conditioning 1.oet}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Patients aged 48 months or older; Matched Sibling Donor or Matched Donor/Matched Unrelated Donor&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;TBI, Etoposide + ATG&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;*See section 9.0 regarding calculation and use of ideal body weight (IBW) or adjusted ideal body weight (AIBW) for dosing of cytotoxic drugs for obese patients.&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -7&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>&lt;p&gt;To start&amp;nbsp;the evening of admission and continue until Etoposide infusion starts on Day - 3.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;strong&gt;- For Matched Donor/Matched Unrelated Donor (MD) Anti-thymocyte Globulin is to be given on Day -3 to Day -1.&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;- For Matched Sibling Donor (MSD) Anti-thymocyte Globulin is NOT given.&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -4* to Day -1&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;methylPREDNISolone acetate injection&apos;]/activities[at0001]">     <items>       <item>         <key>Catalogue Pattern.Configuration Notes</key>         <value>&lt;p&gt;Every&amp;nbsp;12 hours for 7 doses.&lt;/p&gt;&lt;p&gt;&lt;em&gt;*To be started 12 hours prior to the first Thymoglobulin dose. Patient MUST receive 2 doses of MethylPREDNISolone before the first Thymoglobulin dose.&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3 to Day -1&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Hypersensitivity precautions with Thymoglobulin&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;If patient experiences fever, chills or rigors extend the infusion to 6 -12 hours. Consider use of acetaminophen for fevers. Consult attending physician. Do not infuse with lipids or cyclosporine&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Premedications to be given 30 minutes prior to Etoposide:&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;etoposide injection&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Hypersensitivity precautions with Etoposide&lt;/p&gt;&lt;p&gt;&lt;em&gt;Pharmacy to prepare Etoposide as a 10 mg/mL solution&lt;/em&gt;&lt;em&gt;.&lt;/em&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;On day of Etoposide infusion monitor urine output every 4 hours. If urine output is LESS than 80% of fluid intake measured every 4 hours notify MD/NP for diuretic order. &amp;nbsp;MD/NP may consider Furosemide.&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;dextrose 5%-sodium chloride 0.9% Infusion&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;To run concurrently with Etoposide through a separate lumen and for 20 hours following completion of Etoposide.&lt;/p&gt;&lt;p&gt;&lt;em&gt;*Total IV/PO intake should equal ~125 mL/m²/hr&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -2 to Day 0&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;To start with&amp;nbsp;Etoposide post-hydration is complete and to continue until Day 0. Reassess fluids on Day 0.&lt;/p&gt;&lt;p&gt;&lt;em&gt;*Total IV/PO intake should equal ~62.5 mL/m²/hr&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Allogeneic BMT (MA) for ALL – ALL SCTped 2012 FORUM Study Conditioning 1">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="fa80ca89-b938-4053-b4ab-6eaf57409b6f" concept_name="PLACEHOLDER Order Item" max="1" path="/items" name="PLACEHOLDER - ClinDoc: Select weight calculation used, if required (obese patients only):  IBW  AIBW"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.