    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;u&gt;&lt;strong&gt;Acute GVHD&lt;/strong&gt;&lt;/u&gt;&lt;/p&gt;&lt;table border=&quot;1&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;Stage&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Skin&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Liver&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Gut&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;0&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;No rash&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Total bilirubin &amp;lt; 34 umol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;No diarrhea&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;1&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Maculopapular rash &amp;lt;25% body surface area&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Total bilirubin 34 to 50 umol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Diarrhea 500 – 1000 mL/day or nausea with positive gastric biopsy&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;2&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Maculopapular rash 25-50% body surface area&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Total bilirubin 51 to 100 umol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Diarrhea 1000 – 1500 mL/day&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;3&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Maculopapular rash &amp;gt;50% body surface area&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Total bilirubin 101 to 250 umol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Diarrhea 1500 – 2000 mL/day&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;4&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Generalized exfoliative, ulcerative or bullous dermatitis&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Total bilirubin &amp;gt; 250 umol/L&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Diarrhea &amp;gt;2000 mL/day or severe abdominal pain or ileus&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;table border=&quot;1&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;td colspan=&quot;5&quot;&gt;&lt;p&gt;Stage&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;Grade&lt;/p&gt;&lt;/td&gt;&lt;td colspan=&quot;2&quot;&gt;&lt;p&gt;Skin&lt;/p&gt;&lt;/td&gt;&lt;td colspan=&quot;2&quot;&gt;&lt;p&gt;Liver&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Gut&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;0&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;0&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;And&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;0&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;And&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;0&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;I&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;1-2&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;And&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;0&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;And&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;0&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;II&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;3&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Or&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;1&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Or&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;1&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;III&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;2-3&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Or&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;2-4&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;p&gt;IV&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;4&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;Or&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;4&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;td&gt;&lt;p&gt;4&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
