      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;em&gt;Intravenous: 2 to 4 mg/kg/day, max 300 mg/DAY&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Oral: 2 to 4 mg/kg/day, max 300 mg/DAY&lt;/em&gt;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
