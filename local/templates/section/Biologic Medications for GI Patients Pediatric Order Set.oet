  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Discharge Instructions&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
