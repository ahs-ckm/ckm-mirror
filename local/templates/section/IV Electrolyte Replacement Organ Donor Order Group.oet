   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;potassium&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;potassium chloride 10 mmol in sterile water for injection 100 mL; 10 mmol, intravenous, every 6 hours, as needed&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;for serum potassium less than 3.5 mmol/L, Recommend to infuse each dose over 60 Minutes. &amp;nbsp;Refer to Parenteral Monograph&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;potassium&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;potassium chloride 20 mmol in sterile water for injection 100 mL; 20 mmol, intravenous, every 6 hours, as needed&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;for serum potassium less than 3.5 mmol/L, For central line infusion. Recommend to infuse each dose over 60 Minutes. &amp;nbsp;Refer to Parenteral Monograph&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;potassium&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;potassium phosphates IV (Adult); 15 mmol, intravenous, every 2 hours, as needed&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;for serum phosphate less than 0.8 mmol/L AND serum potassium less than 3.5 mmol/L, Recommend to infuse each dose over 2 hours. &amp;nbsp;Refer to Parenteral Monograph&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;calcium chloride&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;calcium chloride IV; 1 g, intravenous, Administer over 30 Minutes, every 6 hours, as needed&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;For serum ionized calcium less than 1.0 mmol/L, Recommend to Infuse each dose over 30 Minutes. &amp;nbsp;Refer to Parenteral Monograph&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;calcium chloride&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;calcium gluconate IV; 3 g, intravenous, Administer over 30 Minutes, every 6 hours, as needed&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;For serum ionized calcium less than 1.0 mmol/L, Recommend to Infuse each dose over 30 Minutes. &amp;nbsp;Refer to Parenteral Monograph&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;magnesium sulfate infusion; 4 g, intravenous, Administer over 2 Hours, every 6 hours, as needed&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;For Magnesium less that 0.7 mmol/L, Recommend to Infuse each dose over 2 hours. Refer to Parenteral Monograph&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sodium phosphates Injection (adult); 15 mmol, intravenous, every 6 hours, as needed&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;For phosphate less than 0.8 mmol/L, If sodium is greater than 150 mmol/L consider using potassium phosphate 15 mEq instead of sodium phosphate.&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
