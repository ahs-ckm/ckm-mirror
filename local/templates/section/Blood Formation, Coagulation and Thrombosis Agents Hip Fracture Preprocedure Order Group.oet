     <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;If Patient receiving Direct Oral Anticoagulant (DOAC)&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
       <items>
         <item>
           <key>CKT.Clinical Guidance</key>
           <value>&lt;p&gt;&amp;nbsp;&lt;em&gt;Heparin or LMWH not required for 48 hours while direct oral anticoagulant stopped&lt;/em&gt;&lt;/p&gt;</value>
         </item>
       </items>
     </annotations>
     <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;If Patient receiving Direct Oral Anticoagulant (DOAC)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
       <items>
         <item>
           <key>CKT.Content Details</key>
           <value>&lt;p&gt;do not give DOACs (i.e. dabigatran, rivaroxaban, apixaban, and others). Document date and time of last dose of dabigatran, rivaroxaban, apixaban or others&amp;nbsp;taken. &amp;nbsp;Heparin or LMWH not required for 48 hours while direct oral anticoagulant stopped&lt;/p&gt;</value>
         </item>
       </items>
     </annotations>
     <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Management of Anti-Platelet Therapy&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
       <items>
         <item>
           <key>CKT.Clinical Guidance</key>
           <value>&lt;p&gt;Continue Anti-Platelet Therapy for high risk vascular patients (within 12 months of drug eluting stent, or 6 weeks of a bare metal stent).&lt;/p&gt;</value>
         </item>
       </items>
     </annotations>
     <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Management of Anti-Platelet Therapy&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Continue Anti-Platelet Therapy&apos;]/activities[at0001]">
       <items>
         <item>
           <key>CKT.Content Details</key>
           <value>&lt;p&gt;&amp;nbsp;Continue anti-platelet therapy for high risk vascular patients (within 12 months of drug eluting stent, or 6 weeks of a bare metal stent).&lt;/p&gt;</value>
         </item>
       </items>
     </annotations>
     <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Management of Anti-Platelet Therapy&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Do NOT give Anti-Platelet Therapy&apos;]/activities[at0001]">
