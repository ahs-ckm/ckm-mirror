     <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;If Patient Receiving Warfarin&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication&apos;]/activities[at0001]">
       <items>
         <item>
           <key>CKT.Content Details</key>
           <value>&lt;p&gt;Do NOT give warfarin pre-op for hip fracture repair. Document date and time of last dose taken.&lt;/p&gt;</value>
         </item>
       </items>
     </annotations>
