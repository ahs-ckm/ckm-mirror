<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>f6436d4a-b126-4167-8ef7-ab190bff8d1a</id>   <name>Surgical Antibiotic Prophylaxis Pediatric Order Panel</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;Refer to AHS Surgical Prophylaxis Recommendations (https://www.albertahealthservices.ca/assets/info/hp/as/if-hp-as-surgical-prophylaxis.pdf) for specific antibiotic recommendations based on surgery type, patient weight and allergies.&amp;nbsp; Antibiotics should be given within 60 minutes prior to incision.&amp;nbsp; For exceptions to this, see Table 1 of AHS Surgical Prophylaxis Recommendations (https://www.albertahealthservices.ca/assets/info/hp/as/if-hp-as-surgical-prophylaxis.pdf).&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;ceFAZolin Injection; _____ mg (30 mg/kg) IV, once, For 1 Dose, to be given within 60 minutes prior to incision&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Pre-procedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;If patient has ceFAZolin allergy or severe non-IgE mediated reaction to any β-lactam (see &amp;nbsp;AHS Algorithm for β-lactam Allergy and Surgical Prophylaxis [https://www.albertahealthservices.ca/assets/info/hp/as/if-hp-as-blactam-allergy-surgical-prophylaxis-algorithm.pdf]):&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;clindamycin Injection; _____mg (10 mg/kg) IV, once, For 1 dose, to be given within 60 minutes prior to incision&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Pre-procedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;gentamicin Injection; _____mg (1.5 mg/kg) IV, once, For 1 dose, to be given within 60 minutes prior to incision&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Pre-procedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]"><items><item><key>Technical.ﻩ Technical Traceability</key><value>{~AHSID~f6436d4a-b126-4167-8ef7-ab190bff8d1a~NAME~Surgical Antibiotic Prophylaxis Pediatric Order Panel.oet}</value></item></items></annotations><definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Surgical Antibiotic Prophylaxis Pediatric">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="d8ff3407-9c0a-4e65-9cc2-b91041b4451a" concept_name="PHARM ceFAZolin Injection Order" max="1" path="/items" name="ceFAZolin Injection; _____ mg (30 mg/kg) IV, once, For 1 Dose, to be given within 60 minutes prior to incision"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="Clinical Guidance #1"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="eae3d03c-57f0-4341-8e81-0577dfab869f" concept_name="PHARM clindamycin Injection Order" max="1" path="/items" name="clindamycin Injection; _____mg (10 mg/kg) IV, once, For 1 dose, to be given within 60 minutes prior to incision"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" max="1" path="/items" name="AND/OR"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="4c64035f-72ac-454d-bf7b-60d73c421c66" concept_name="PHARM gentamicin Injection Order" max="1" path="/items" name="gentamicin Injection; _____mg (1.5 mg/kg) IV, once, For 1 dose, to be given within 60 minutes prior to incision"/>   