        <key>CKT.Clinical Guidance</key>
        <value>&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; border=&quot;1&quot; style=&quot;width:564px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;width:313px&quot;&gt;&lt;p&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&lt;strong&gt;&amp;nbsp; Flat Dose Recommendation&lt;/strong&gt;:&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;width:250px&quot;&gt;&amp;nbsp;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:313px&quot;&gt;&lt;p&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; &amp;nbsp;All eligible infants&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;width:250px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;0.2mL&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:313px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&lt;strong&gt;&amp;nbsp;OR Weight-based Dose Recommendation:&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td style=&quot;width:250px&quot;&gt;&amp;nbsp;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:313px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; Less than 750g &amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td style=&quot;width:250px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;0.05 mL&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:313px&quot;&gt;&lt;p&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; 751 – 1250g&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;width:250px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;0.1 mL&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:313px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; 1251 – 1500g&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td style=&quot;width:250px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;0.15 mL&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:313px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; Greater than 1500g&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td style=&quot;width:250px&quot;&gt;&lt;span style=&quot;font-size:11px&quot;&gt;&lt;span style=&quot;font-family:Helvetica&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;0.2 mL&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;</value>
