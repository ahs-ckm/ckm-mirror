  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Antifungals&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;nystatin Oint 100 000 unit/g&apos;]/activities[at0001]">
    <items>
