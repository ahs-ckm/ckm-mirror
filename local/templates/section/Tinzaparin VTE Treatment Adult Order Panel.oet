  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Where once daily dosing is appropriate, use tinzaparin subcutaneously once daily for general VTE treatment, cancer associated thrombosis, pregnancy, major trauma, or obesity.&lt;/p&gt;</value>
      </item>
    </items>
