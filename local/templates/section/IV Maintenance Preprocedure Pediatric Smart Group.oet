<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>502aaa32-e20f-4bd1-8e2f-cd0c9aa7a9a9</id>   <name>IV Maintenance Preprocedure Pediatric Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.ﻩ Technical Traceability</key>         <value>{~AHSID~502aaa32-e20f-4bd1-8e2f-cd0c9aa7a9a9~NAME~IV Maintenance Preprocedure Pediatric Smart Group}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Pediatric Topical Anesthetic Agents&lt;/p&gt;&lt;table border=&quot;1&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot; style=&quot;width:0px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Agent&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Indications for Use&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;em&gt;tetracaine 4% gel (Ametop)&lt;/em&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;em&gt;Wait time 30-45 minutes&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;preferred choice unless need urgent access or neonate&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Contraindications:&lt;/em&gt;&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;em&gt;Need urgent access&lt;/em&gt;&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;em&gt;Allergy to tetracaine&lt;/em&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;em&gt;lidocaine-prilocaine cream 2.5%-2.5% (EMLA)&lt;/em&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;em&gt;Wait time 45-60 minutes&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Preferred agent in neonates&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Contraindications:&lt;/em&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;&lt;em&gt;Need urgent access&lt;/em&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;em&gt;Prone to methemoglobinemia (i.e. G6PD deficiency)&lt;/em&gt;&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;em&gt;Allergy to lidocaine&lt;/em&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;em&gt;lidocaine, liposomal 4% cream (MAXILENE)&lt;/em&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;em&gt;Wait time 30 minutes&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Preferred agent for urgent access. &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Can be used in infants as young as 2 weeks&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;&amp;nbsp;Contraindications:&lt;/em&gt;&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;em&gt;Need urgent access&lt;/em&gt;&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;em&gt;Allergy to lidocaine&lt;/em&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Insert Peripheral IV; Once&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Maintain IV Access&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Convert to Saline Lock IV&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sodium chloride 0.9% infusion&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;1 to 3 mL, Intravenous, Every 24 hours and PRN following intermittent administration of fluid, medication or blood products&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine Gel 4%; topical, Once as needed&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;apply ½ tube, up to 1 g, directly over vein, may be applied to two sites and covered with an occlusive dressing for 30 to 45 minutes prior to procedure&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine-prilocaine Cream 2.5%-2.5% (EMLA); Topical, Once as needed&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;If patient less than 5 kg apply less than 1 g; if patient 5 kg and greater apply 1 to 2 g directly over vein, may be applied to two sites and covered with an occlusive dressing for 45 to 60 minutes prior to procedure&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%; Topical, Once as needed&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;May be applied to two sites and covered with an occlusive dressing for 30 minutes prior to procedure.&lt;/p&gt;&lt;p&gt;DOSE: For neonates to 3 years old, use 1/5th tube or 2 cm length of cream per site (equals 1 gram per site).&lt;/p&gt;&lt;p&gt;For 4 years old and OLDER, use ½ tube of cream per site (equals 2.5 grams per site)&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sucrose ORAL liquid 24%; 2 mL, Oral, Once as needed&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;For ages 0 to four months. 2 mL, Oral, As needed, mild pain. Oral sucrose shall not exceed five (5) doses in 24 hours. For ages 0 to four months.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="IV Maintenance Preprocedure Pediatric Smart Group">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="97c832cb-cfa5-421b-a871-473a0f5fcae6" concept_name="PC Insert Peripheral IV Order" max="1" path="/items" name="Insert Peripheral IV; Once"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="edc6d443-87da-40da-9326-33d7653b0129" concept_name="PC Maintain IV Access Order" max="1" path="/items" name="Maintain IV Access"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" templat