<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>2d989060-8f0c-4e30-9549-a6b7b98d1111</id>   <name>ICU Anti-Infective Agents Order Panel</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose />       <use />       <misuse />     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value />       </item>       <item>         <key>Business Process Level</key>         <value />       </item>       <item>         <key>Care setting</key>         <value />       </item>       <item>         <key>Client group</key>         <value />       </item>       <item>         <key>Clinical Record Element</key>         <value />       </item>       <item>         <key>Copyright</key>         <value />       </item>       <item>         <key>Issues</key>         <value />       </item>       <item>         <key>Owner</key>         <value />       </item>       <item>         <key>Sign off</key>         <value />       </item>       <item>         <key>Speciality</key>         <value />       </item>       <item>         <key>User roles</key>         <value />       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.ï»© Technical Traceability</key>         <value>{~AHSID~2d989060-8f0c-4e30-9549-a6b7b98d1111~NAME~ICU Anti-Infective Agents Order Panel.oet}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;Patients who are at highest risk of COVID-19 related-adverse outcomes should be considered for treatment with off-label antiviral drugs (non-exclusive):&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Laboratory confirmed COVID-19 infection as determined by PCR (or suspected COVID-19 if possible delay in testing greater than 24 hours) requiring hospitalization for management of pneumonia&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;AND&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp;&lt;em&gt;Age greater than 60 years&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Or&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;em&gt;Age greater than 50 years PLUS one of&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; - Hypertension&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; -&amp;nbsp;&lt;em&gt;Diabetes&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; -&amp;nbsp;&lt;em&gt;Structural lung disease (ie COPD)&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; -&amp;nbsp;&lt;em&gt;Immunocompromised&lt;/em&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Or&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp;&lt;em&gt;Other adult patients with severe pneumonia (ARDS, respiratory failure, multi-organ failure)&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #1']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Empiric Antimicrobial Therapy of Hospitalized Suspect COVID-19 Patients&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;REASSESS WITH VIRAL AND BACTERIAL LAB RESULTS&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Pending confirmation COVID-19 positive AND bacterial culture negative&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='vancomycin injection; 15 mg/kg/dose, Intravenous, Every 12 hours for 3 days. Starting 12 hours after initial 25 mg/kg/dose dose. Target trough. Reassess in 72 hours. ']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid injection; 600 mg, Intravenous, every 12 hours, for 6 doses.']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid Tablet; 600 mg, Oral, every 12 hours, for 6 doses']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #3']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Management of Possible Secondary Bacterial Infection/Ventilator Associated Pneumonia in Adult COVID-19 patients&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Worsening pneumonia may also be due to inflammation so prolonged antibiotic therapy beyond 5- 7 days in the absence of positive cultures is not currently recommended. &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Culture directed therapy is preferred.&amp;nbsp; Empiric therapy pending sputum/bronch culture results:&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #4']">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;Plus, if not documented MRSA negative, ADD&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #4']/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='vancomycin injection; 25 mg/kg/dose, (max 2000 mg) Intravenous, Once.']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value='Clinical Guidance #4']/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='vancomycin injection; 15 mg/kg/dose, Intravenous, Every 12 hours for 3 days. Starting 12 hours after initial 25 mg/kg/dose dose. Target trough.']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid #1']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid injection; 600 mg, Intravenous, every 12 hours, for 6 doses']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value='Linezolid #1']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='linezolid Tablet; 600 mg, Oral, every 12 hours, for 6 doses.']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&lt;em&gt;Reassess in 72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EH