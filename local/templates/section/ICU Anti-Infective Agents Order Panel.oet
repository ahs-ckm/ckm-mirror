  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Empiric Antimicrobial Therapy of Hospitalized Suspect COVID-19 Patients&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;REASSESS WITH VIRAL AND BACTERIAL LAB RESULTS&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Pending confirmation COVID-19 positive AND bacterial culture negative. For pregnant patients - choose Azithromycin. Doxycycline NOT recommended&lt;/em&gt;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;cefTRIAXone injection; 1000 mg, Intravenous, daily, for 3 doses. Reassess after 48-72 hours. On unit&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;IF patient greater than 100kg, give 2g every 24 hours&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Recommended loading dose 25 to 30 mg/kg/dose, maximum 3000 mg/dose.&lt;/p&gt;&lt;p&gt;Recommended maintenance dose 15 mg/kg/dose&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Linezolid&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;All patients with functional GI tract should use oral formulation, use IV formulation only if non-functional GI tract. Alternate if renal dysfunction or known prior MRSA pneumonia&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Linezolid&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;linezolid IV; 600 mg, Intravenous, every 12 hours, for 6 doses.&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&lt;em&gt;Reassess in 72 hours. Discontinue vancomycin or linezolid if MRSA carriage swab and bacterial respiratory culture are negative for MRSA&lt;/em&gt;&lt;/p&gt;&lt;p&gt;On Unit&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Linezolid&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;linezolid tablet; 600 mg, Oral, every 12 hours, for 6 doses&apos;]/activities[at0001]">
