ities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;- &amp;nbsp;Heart rate greater than ______&amp;nbsp;beats/minute&lt;/p&gt;&lt;p&gt;- MAP less than ______&amp;nbsp; mmHg or SBP less than ______ &amp;nbsp;mmHg&lt;/p&gt;&lt;p&gt;- Wedge pressure greater than ______&amp;nbsp;mmHg or CVP/right atrial pressure greater than &amp;nbsp; &amp;nbsp; ______&amp;nbsp;mmHg&lt;/p&gt;&lt;p&gt;- Cardiac&amp;nbsp;Index less than ______&amp;nbsp;L/min/m2&lt;/p&gt;&lt;p&gt;- Other: &amp;nbsp;______&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Impella Weaning Protocol&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication :&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Decrease Power (P) level by *** (suggest 1-2) and reassess in *** hours(suggest 1-2hours)&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Impella Weaning Protocol&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&amp;nbsp;Every 15 minutes x 4 then hourly after every Power (P) level change&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Impella Weaning Protocol&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Measure Cardiac output&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&amp;nbsp;Every 30minutes after Power (P) level change if pulmonary artery catheter present&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Impella Weaning Protocol&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Pulmonary Capillary Wedge Pressure&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&amp;nbsp;Every 30minutes after Power (P) level change if pulmonary artery catheter present&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Impella Weaning Protocol&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication:&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;If parameters met, continue to decrease P level as per above guidelines until P level 2; do not wean lower than P level 2&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Impella Weaning Protocol&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication: #1&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&amp;nbsp;If parameters not met, go back to the previous P level and notify CICU physician&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Impella Weaning Protocol&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication: #2&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&amp;nbsp;