th="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Parameters']/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;- &amp;nbsp;Heart rate greater than ______&amp;nbsp;beats/minute&lt;/p&gt;&lt;p&gt;- MAP less than ______&amp;nbsp; mmHg or SBP less than ______ &amp;nbsp;mmHg&lt;/p&gt;&lt;p&gt;- Wedge pressure greater than ______&amp;nbsp;mmHg or CVP/right atrial pressure greater than &amp;nbsp; &amp;nbsp; ______&amp;nbsp;mmHg&lt;/p&gt;&lt;p&gt;- Cardiac&amp;nbsp;Index less than ______&amp;nbsp;L/min/m2&lt;/p&gt;&lt;p&gt;- Other: &amp;nbsp;______&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication :']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Decrease Power (P) level by *** (suggest 1-2) and reassess in *** hours(suggest 1-2hours)&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Vital Signs']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;Every 15 minutes x 4 then hourly after every Power (P) level change&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Measure Cardiac Output']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;Every 30minutes after Power (P) level change if pulmonary artery catheter present&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Pulmonary Capillary Wedge Pressure']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;Every 30minutes after Power (P) level change if pulmonary artery catheter present&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication:']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;If parameters met, continue to decrease P level as per above guidelines until P level 2; do not wean lower than P level 2&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication: #1']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;If parameters not met, go back to the previous P level and notify CICU physician&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication: #2']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;When P level 2 is reached measure &amp;nbsp;*** &amp;nbsp;STAT&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value='Impella Weaning Protocol']/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Choose ONE for when P level 2 is reached&lt;/p&gt;</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Impella Weaning Panel">    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Impella Weaning Parameters">      <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="fa80ca89-b938-4053-b4ab-6eaf57409b6f" concept_name="PLACEHOLDER Order Item" max="1" path="/items" name="PLACEHOLDER: Weaning Parameters" />    </Item>    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Impella Weaning Protocol">      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ddd49576-2a7e-42d7-b258-bcb0b9b1a420" concept_name="PC Nursing Communication Order" max="1" path="/items" name="Nursing Communication :" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="3075bff7-28dd-4e96-816d-4cfddd0ee1d5" concept_name="PC Vital Signs Order" max="1" path="/items" name="Vital Signs" />      <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="8747a1ff-e672-4a4b-9849-27b085834b3f" concept_name="PC Measure Cardiac output Order" max="1" path="/items" name="Measu