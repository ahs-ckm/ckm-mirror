  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Oxygen Therapy&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">
