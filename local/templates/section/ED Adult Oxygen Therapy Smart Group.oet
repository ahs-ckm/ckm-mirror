<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>713d9666-2fed-4279-8e60-38a1667d2dc1</id>   <name>ED Adult Oxygen Therapy Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.EPIC ID</key>         <value>&lt;p&gt;174349&lt;/p&gt;</value>       </item>     <item><key>Technical.ﻩ Technical Traceability</key><value>{~AHSID~713d9666-2fed-4279-8e60-38a1667d2dc1~NAME~ED Adult Oxygen Therapy Smart Group.oet}</value></item></items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Oxygen Therapy - All presentations EXCEPT Acute Coronary Syndrome, known CO2 retainers, or if need for High Flow, High Percentage O2 Therapy (e.g. CO Poisoning)&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Use for all presentations EXCEPT Acute Coronary Syndrome, known CO2 retainers, or when High Flow, High Percentage Oxygen Therapy is required (such as Carbon Monoxide poisoning)&lt;br&gt;&amp;nbsp;&lt;br&gt;All other presentations (including pregnancy and acute stroke) should adhere to the following Sp02 goals (92% - 96%). &amp;nbsp;NOTE: For acute stroke, don’t apply supplemental oxygen unless SpO2 is under 90%.&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Oxygen Therapy - All presentations EXCEPT Acute Coronary Syndrome, known CO2 retainers, or if need for High Flow, High Percentage O2 Therapy (e.g. CO Poisoning)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Priority: Routine&lt;/p&gt;&lt;p&gt;Titrate to Maintain Target Saturation Range: 92 to 96%&amp;nbsp;&lt;br&gt;Comments: Oxygen Therapy: All presentations EXCEPT Acute Coronary Syndrome, pCO2 retainers or when High Flow, High Percentage O2 Therapy is required (e.g. Carbon Monoxide Poisoning).&amp;nbsp;&amp;nbsp;All other presentations (including pregnancy and acute stroke) should adhere to the above Sp02 goals. NOTE: For acute stroke, don&apos;t apply supplemental oxygen unless SpO2 is under 90%&amp;nbsp;&amp;nbsp; &amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Oxygen Therapy - Known CO2 retainer&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Priority: Routine&amp;nbsp;&lt;br&gt;Titrate to Maintain Target Saturation Range: 88 to 92% (Known CO2 retainer)&amp;nbsp;&lt;br&gt;Comments: NOTE: For acute stroke and acute coronary syndrome, don&apos;t apply supplemental oxygen unless SpO2 is under 90%&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Oxygen Therapy - All Acute Coronary Syndromes (ACS)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Priority: Routine&amp;nbsp;&lt;br&gt;Titrate to Maintain Target Saturation Range: Other range&amp;nbsp;&lt;br&gt;Target Saturation Min: 90&amp;nbsp;&lt;br&gt;Target Saturation Max: 92&amp;nbsp;&lt;br&gt;Comments: When SpO2 is under 90%, titrate Oxygen to maintain saturation range SpO2 90-92%&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Oxygen Therapy - High Flow, High Percentage O2 Therapy Required (e.g. CO Poisoning)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Priority: Routine&amp;nbsp;&lt;br&gt;Titrate to Maintain Target Saturation Range: High flow, high percent oxygen (CO poisoning)&amp;nbsp;&lt;br&gt;Oxygen Therapy by FiO2. As needed based on patient Oxygen requirements (Select all that apply): Other (Specify)&amp;nbsp;&lt;br&gt;Other (Specify): Partial non-rebreather&amp;nbsp;&lt;br&gt;FiO2 in percent: 100&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP); if Oxygen flow increase by greater than 2 L/min from&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Frequency: Until discontinued&lt;/p&gt;&lt;p&gt;Starting: Today&lt;br&gt;Comments: if Oxygen flow increase by greater than 2 L/min from previous to maintain the same level of oxygenation, or if there is a progressive increase in the work of breathing&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP); if a new change to Oxygen flow of 8 L/min&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Frequency: Until discontinued&lt;/p&gt;&lt;p&gt;Starting: Today&lt;br&gt;Comments: if a new change to Oxygen flow of 8 L/min or higher to maintain same level of oxygenation&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="ED Adult Oxygen Therapy Smart Group">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderable_section.v1" concept_name="Orderable Section" max="1" path="/items" name="Oxygen Therapy - All presentations EXCEPT Acute Coronary Syndrome, known CO2 retainers, or if need for High Flow, High Percentage O2 Therapy (e.g. CO Poisoning)">       <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="be4b8a6b-a79a-4e0b-8735-fb7e017b9225" concept_name="RESP ADULT Oxygen Therapy Order" max="1" path="/items" name="Oxygen Therapy"/>     </Item>     <Item xsi:type="SECTION" archetype_id="open