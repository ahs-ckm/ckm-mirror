  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose Meter POCT; Conditional PRN&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Recheck the blood glucose 30 min after the intervention (glucose gel and feeding) or a max of 60 min after the initial blood glucose measurement, in order to avoid a very long feeding attempt delaying subsequent checks.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
