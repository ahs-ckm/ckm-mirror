otations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diet and Nutrition&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;OR&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diet and Nutrition&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #5&apos;]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;OR&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Point of Care Testing Glucose&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Diabetes Canada Guidelines&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Point of Care Testing Glucose&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose Meter POCT&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;4 times daily before meals and at bedtime &amp;nbsp;for 4 days, Assess 15 to 30 minutes before scheduled meals and at bedtime, AND PRN for suspected hypoglycemia&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Point of Care Testing Glucose&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Glucose Meter POCT #1&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;One occurrance&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Point of Care Testing Glucose&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP); Blood Glucose Targets&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;If blood glucose is less than 4.0 mmol/L initiate AHS Hypoglycemia Policy and contact authorized Prescriber. If blood glucose is greater than 18.0 mmol/L OR if patient on insulin pump SGLT2 inhibitors and blood glucose is greater than 14.0 mmol/L initiate hyperglycemia Procedure, and notify Authorized prescriber and collect ketones.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Assessments&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Skin Assessment&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Nursing to complete skin assessment&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Assessments&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Behaviour Mapping&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Nursing to complete Behavior Mapping daily for 2 weeks&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Respiratory Interventions&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Titrate Oxygen to maintain saturation range at or above 89%&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Anticoagulants&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Reason for Consult?&lt;/p&gt;&lt;p&gt;Free Text: Please manage warfarin therapy&lt;/p&gt;&lt;p&gt;Did you contact the consultant? No&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Anaphylaxis&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Initiate emergency assistance immediately as per your site practice setting&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Until discontinued, Initiate emergency assistance immediately as per your site practice setting&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Anaphylaxis&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;EPINEPHrine IM anaphylaxis; anaphylaxis IM&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;0.3 mg, intramuscular, every 5 minutes, as needed, anaphylaxis, &amp;nbsp;for 3 doses, Administer deep intramuscular to mid-anterior lateral thigh (vastus lateralis). Repeat epinephrine dose every 5 minutes, to a maximum of 3 doses if the resident&apos;s condition does not improve. &amp;nbsp;If other doses of epinephrine are required administer in the resident&apos;s thigh. Assess for and treat anaphylaxis and monitor response to epinephrine as per AHS Anaphylaxis Management Administration of Intramuscular Epinephrine Policy and notify physician.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Cardiovascular Agents&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Chest Pain and Goals of Care of R or M&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Cardiovascular Agents&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Chest Pain and Goals of Care of R or M&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Cardiovascular Agents&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Chest Pain and Goals of Care of R or M&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;nitroglycerin tablet sublingual&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;0.3 mg, sublingual, every 5 minutes, as needed, chest pain, Administer up to 3 doses if patient has chest pain. Give only if blood pressure is greater than or equal to 90/60. If 3 doses given and chest pain resolved, notify attending physician. After 3rd dose, if chest pain unresolved after 5 minutes seek emergency care (call EMS or co-located ER and attending physician).&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Cardiovascular Agents&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Chest Pain and Goals of Care of R or M&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;OR&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Cardiovascular Agents&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Chest Pain and Goals of Care of R or M&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;nitroglycerin 0.4 mg/spray translingual spray&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;1 spray, sublingual, every 5 minutes, as needed, chest pain, Administer up to 3 doses if patient has chest pain. Give only if blood pressure is greater than or equal to 90/60. If 3 doses given and chest pain resolved, notify attending physician. After 3rd dose, if chest pain unresolved after 5 minutes seek emergency care (call EMS or co-located ER and attending physician).&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Cardiovascular Agents&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Chest Pain and Goals of Care of R or M&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;160 mg, oral, daily as needed, chest pain, Starting today at 10:04&lt;br&gt;Administer WITH 3rd dose of nitroglycerin and if chest pain remains unresolved.&lt;br&gt;Chew if possible. Give if not contraindicated by a) ASA allergy; b) recent gastro-intestinal bleed&lt;br&gt;Routine&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Cardiovascular Agents&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Chest Pain and Goals of Care of R or M&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1