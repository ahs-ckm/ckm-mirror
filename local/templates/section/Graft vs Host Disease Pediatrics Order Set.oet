  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;General&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Consensus Organ staging and overall clinical grading for GVHD&lt;/p&gt;&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; border=&quot;1&quot; style=&quot;width:672px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;width:48px&quot;&gt;Stage&lt;/td&gt;&lt;td style=&quot;width:202px&quot;&gt;Skin&lt;/td&gt;&lt;td style=&quot;width:192px&quot;&gt;Liver&lt;/td&gt;&lt;td style=&quot;width:225px&quot;&gt;Gut&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:48px&quot;&gt;0&lt;/td&gt;&lt;td style=&quot;width:202px&quot;&gt;No rash&lt;/td&gt;&lt;td style=&quot;width:192px&quot;&gt;Total bilirubin less than 34 umol/L&lt;/td&gt;&lt;td style=&quot;width:225px&quot;&gt;No diarrhea&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:48px&quot;&gt;1&lt;/td&gt;&lt;td style=&quot;width:202px&quot;&gt;Maculopapular rash less than 5% body surface area&lt;/td&gt;&lt;td style=&quot;width:192px&quot;&gt;Total bilirubin 34 to 50&lt;/td&gt;&lt;td style=&quot;width:225px&quot;&gt;Diarrhea 500 – 1000 mL/day or nausea with positive gastric biopsy&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:48px&quot;&gt;2&lt;/td&gt;&lt;td style=&quot;width:202px&quot;&gt;Maculopapular rash 25 – 50% body surface area&lt;/td&gt;&lt;td style=&quot;width:192px&quot;&gt;Total bilirubin 51 to 100&lt;/td&gt;&lt;td style=&quot;width:225px&quot;&gt;Diarrhea 1000 – 1500 mL/day&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:48px&quot;&gt;3&lt;/td&gt;&lt;td style=&quot;width:202px&quot;&gt;Maculopapular rash greater than 50% body surface area&lt;/td&gt;&lt;td style=&quot;width:192px&quot;&gt;Total bilirubin 101 to 250&lt;/td&gt;&lt;td style=&quot;width:225px&quot;&gt;Diarrhea 1500 – 2000 mL/day&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:48px&quot;&gt;4&lt;/td&gt;&lt;td style=&quot;width:202px&quot;&gt;Generalized exfoliative, ulcerative or bullous dermatitis&lt;/td&gt;&lt;td style=&quot;width:192px&quot;&gt;Total bilirubin greater than 250&lt;/td&gt;&lt;td style=&quot;width:225px&quot;&gt;Diarrhea greater than 2000 mL/day or severe abdominal pain or ileus&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
