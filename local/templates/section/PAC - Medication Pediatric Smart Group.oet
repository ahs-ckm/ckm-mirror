<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>fcea1682-2ab9-4739-ab14-18e20cab83f4</id>   <name>PAC - Medication Pediatric Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;table border=&quot;0&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot; style=&quot;width:0px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Agent&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;&lt;strong&gt;&lt;em&gt;Indications for Use&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;tetracaine 4% gel (Ametop)&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;Wait time 30-45 minutes&lt;/p&gt;&lt;p&gt;preferred choice unless need urgent access or neonate&lt;/p&gt;&lt;p&gt;Contraindications:&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Need urgent access&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Allergy to tetracaine&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;lidocaine-prilocaine cream 2.5%-2.5% (EMLA)&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;Wait time 45-60 minutes&lt;/p&gt;&lt;p&gt;Preferred agent in neonates&lt;/p&gt;&lt;p&gt;Contraindications:&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Need urgent access&lt;/li&gt;&lt;/ul&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Prone to methemoglobinemia (i.e. G6PD deficiency)&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Allergy to lidocaine&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;lidocaine, liposomal 4% cream (MAXILENE)&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;Wait time 30 minutes&lt;/p&gt;&lt;p&gt;Preferred agent for urgent access.&lt;/p&gt;&lt;p&gt;Can be used in infants as young as 2 weeks&lt;/p&gt;&lt;p&gt;Contraindications:&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Need urgent access&lt;/p&gt;&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Allergy to lidocaine&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine Gel 4%&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;topical, once, as needed&amp;nbsp;&lt;/p&gt;&lt;p&gt;apply ½ tube, up to 1 g, directly over vein, may be applied to two sites and covered with an occlusive dressing for 30 to 45 minutes prior to procedure&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Pre-Admission Clinic&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine-prilocaine Cream 2.5%-2.5% (EMLA)&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;topical, once, as needed&amp;nbsp;&lt;/p&gt;&lt;p&gt;if patient less than 5 kg apply less than 1 g; if patient 5 kg and greater apply 1 to 2 g directly over vein,&amp;nbsp;may be applied to two sites and covered with an occlusive dressing for 45 to 60 minutes prior to procedure&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Pre-Admission Clinic&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lidocaine, liposomal Cream 4%&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Topical, once, as needed&amp;nbsp;&lt;/p&gt;&lt;p&gt;May be applied to two sites and covered with an occlusive dressing for 30 minutes prior to procedure.&lt;/p&gt;&lt;p&gt;DOSE: For neonates to 3 years old, use 1/5th tube or 2 cm length of cream per site (equals 1 gram per site).&lt;/p&gt;&lt;p&gt;For 4 years old and OLDER, use ½ tube of cream per site (equals 2.5 grams per site).&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;·&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Pre-Admission Clinic&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sucrose ORAL liquid 24%&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;2 mL, Oral, Once, as needed, procedural pain&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Pre-Admission Clinic&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]"><items><item><key>Technical.ﻩ Technical Traceability</key><value>{~AHSID~fcea1682-2ab9-4739-ab14-18e20cab83f4~NAME~PAC - Medication Pediatric Smart Group.oet}</value></item></items></annotations><definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="PAC - Medication Pediatric Smart Group">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="0e29f686-4c87-405d-9d45-3d98c85c6f47" concept_name="PHARM tetracaine Gel 4% Order" max="1" path="/items" name="tetracaine Gel 4%"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="de239ed9-bf2c-4b0d-8e94-3c8f8419b5ce" concept_name="PHARM lidocaine-prilocaine Cream 2.5%-2.5% (EMLA) Order" max="1" path="/items" name="lidocaine-prilocaine Cream 2.5%-2.5% (EMLA)"/>     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="7ccf14a3-fddd-4ca6-b3ee-ccaff2c335d2" concept_n