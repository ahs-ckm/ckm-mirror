        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Every 15 minutes for a minimum of 30 minutes and patient achieves Aldrete score greater than or equal to 8 or baseline&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Postprocedure Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs; with Pulse Oximetry; Recovery and On Unit&apos;]/activities[at0001]">
