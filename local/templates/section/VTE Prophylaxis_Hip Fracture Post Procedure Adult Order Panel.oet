<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>5a7581aa-d85e-4b50-b205-91d17b530ab0</id>  <name>VTE Prophylaxis_Hip Fracture Post Procedure Adult Order Panel</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose/>      <use/>      <misuse/>    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value/>      </item>      <item>        <key>Business Process Level</key>        <value/>      </item>      <item>        <key>Care setting</key>        <value/>      </item>      <item>        <key>Client group</key>        <value/>      </item>      <item>        <key>Clinical Record Element</key>        <value/>      </item>      <item>        <key>Copyright</key>        <value/>      </item>      <item>        <key>Issues</key>        <value/>      </item>      <item>        <key>Owner</key>        <value/>      </item>      <item>        <key>Sign off</key>        <value/>      </item>      <item>        <key>Speciality</key>        <value/>      </item>      <item>        <key>User roles</key>        <value/>      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Transform.Technical Notes</key>        <value>&lt;p&gt;&lt;em&gt;&amp;nbsp;VTE Prophylaxis_Surgery Adult Order Panel&lt;/em&gt;&lt;/p&gt;</value>      </item>      <item>        <key>Technical.ﻩ Technical Traceability</key>        <value>{~AHSID~5a7581aa-d85e-4b50-b205-91d17b530ab0~NAME~VTE Prophylaxis_Hip Fracture Post Procedure Adult Order Panel}</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Clinical Decision Support&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;table border=&quot;0&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot; style=&quot;width:0px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;strong&gt;Risk Assessment / Scoring Tools / Screening:&amp;nbsp;&lt;/strong&gt;&lt;br&gt;&amp;nbsp;&lt;br&gt;&lt;em&gt;Note: All patients with hip fracture are at higher risk of VTE and should be considered for early initiation and extended prophylaxis&amp;nbsp;&lt;/em&gt;&lt;br&gt;&amp;nbsp;&lt;br&gt;&lt;u&gt;&lt;strong&gt;Recommendation:&lt;/strong&gt;&lt;/u&gt; Patients undergoing major surgery should receive prophylaxis at least during hospital stay and longer for those with additional risk factors.&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&amp;nbsp;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;table border=&quot;1&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;Low Risk&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;Non-major surgery in patients less than 40 years old with no risk factors&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;Moderate Risk&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;1.&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Non-major surgery with additional risk factors, or:&lt;/p&gt;&lt;p&gt;2.&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Age 40-60 years old&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;High Risk&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;1.&amp;nbsp;&amp;nbsp;&amp;nbsp; Age greater than 60 years old, or:&lt;/p&gt;&lt;p&gt;2.&amp;nbsp;&amp;nbsp;&amp;nbsp; 40-60 years old with additional risk factors&lt;/p&gt;&lt;p&gt;3.&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Major surgery in patients aged less than 40 with multiple risk factors&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;Very High&amp;nbsp;&lt;/p&gt;&lt;p&gt;Risk&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;1.&amp;nbsp;&amp;nbsp;&amp;nbsp; Major surgery in patients with multiple risk factors or:&lt;/p&gt;&lt;p&gt;2.&amp;nbsp;&amp;nbsp;&amp;nbsp; Any patient having hip or knee arthroplasty, or:&lt;/p&gt;&lt;p&gt;3.&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; Major trauma or spinal cord injury.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&lt;br&gt;&lt;u&gt;&lt;strong&gt;Risk Factors:&lt;/strong&gt;&lt;/u&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;u&gt;Major Risk Factors:&amp;nbsp;&lt;/u&gt;Major surgery, trauma, ischemic stroke or paralysis, spinal cord injury, previous VTE, congenital and/or acquired thrombophilia states, or active cancer and its treatment, active COVID-19 infection&lt;/p&gt;&lt;p&gt;&lt;u&gt;Minor Risk Factors:&amp;nbsp;&lt;/u&gt;Age greater than 40 years, estrogen therapy, nephrotic syndrome, pregnancy/postpartum, prolonged immobility greater than 24 hours, general anesthesia longer than 1 hour, severe respiratory disease, collagen vascular disease, inflammatory bowel disease, congestive heart failure, obesity, sepsis, varicose veins&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;u&gt;&lt;strong&gt;Contraindications:&lt;/strong&gt;&lt;/u&gt;&lt;/p&gt;&lt;p&gt;Uncontrolled hypertension, Bleeding disorders (platelets less than 50 x 109/L), Recent intracranial or intraocular hemorrhage, Lumbar puncture within 24 hours, Active bleeding, Heparin-induced thrombocytopenia (HIT)&lt;/p&gt;&lt;p&gt;For patients on heparins, certain drugs can enhance bleeding tendency: acetylsalicylic acid (ASA) and other antiplatelet agents (e.g. clopidogrel, ticagrelor), and nonsteroidal anti-inflammatory drugs (NSAIDs) (particularly ketorolac). This is particularly relevant for patients with intrathecal catheters for analgesia where low molecular weight heparin (LMWH) or unfractionated heparin(UFH ) must NOT be used in conjunction with antiplatelet agents.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;u&gt;&lt;strong&gt;Caution –&amp;nbsp;Management of heparins with spinal punctures:&lt;/strong&gt;&lt;/u&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; border=&quot;1&quot; style=&quot;width:720px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;width:113px&quot;&gt;&amp;nbsp;&lt;/td&gt;&lt;td style=&quot;width:133px&quot;&gt;&lt;table border=&quot;0&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot; style=&quot;width:0px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;text-align:left; vertical-align:top&quot;&gt;&amp;nbsp;&lt;/td&gt;&lt;td style=&quot;text-align:left; vertical-align:top&quot;&gt;&lt;p&gt;Time Before Puncture/Catheter Manipulation or Removal&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/td&gt;&lt;td style=&quot;width:172px&quot;&gt;Use of anticoagulant in Patients with Indwelling Neuraxial Catheter&lt;/td&gt;&lt;td style=&quot;width:111px&quot;&gt;&lt;table border=&quot;0&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot; style=&quot;width:143px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top; width:143px&quot;&gt;&lt;p&gt;Time After Puncture/Catheter Manipulation or Removal&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/td&gt;&lt;td style=&quot;width:186px&quot;&gt;Laboratory Tests&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top; width:113px&quot;&gt;Unfractionated Heparin (UFH) - for prophylaxis, less than 15,000 units /day&lt;/td&gt;&lt;td style=&quot;width:133px&quot;&gt;4 to 6 hours&lt;/td&gt;&lt;td style=&quot;width:172px&quot;&gt;Acceptable&lt;/td&gt;&lt;td style=&quot;width:111px&quot;&gt;&lt;table border=&quot;0&quot; cellspacing=&quot;0&quot; cellpadding=&quot;0&quot; style=&quot;width:0px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top&quot;&gt;&lt;p&gt;1 hour&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/td&gt;&lt;td style=&quot;width:186px&quot;&gt;Platelets during treatment for greater than 3 days&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;vertical-align:top; width:113px&quot;&gt;Low Molecular Weight Heparin (LMWH) - for prophylaxis&lt;/td&gt;&lt;td style=&quot;width:133px&quot;&gt;12 hours&lt;/td&gt;&lt;td style=&quot;width:172px&quot;&gt;&lt;p&gt;For single daily dosing –&amp;nbsp;catheter acceptable&lt;/p&gt;&lt;p&gt;(remove catheter 12 hours prior to next dose of LMWH, and subsequent dose should be greater than 4 hours after removal)&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;For Twice daily dosing –&amp;nbsp;Do not maintain catheter&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;width:111px&quot;&gt;&lt;p&gt;First postoperative dose at least 12 hours after neuraxial puncture and at least 4 hours after removal&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Subsequent dose at least 24 hours after the first dose&lt;/p&gt;&lt;/td&gt;&lt;td style=&quot;width:186px&quot;&gt;Platelets during treatment for greater than 3 days&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Dose Adjustment: &lt;/strong&gt;Consider adjusting dosage for extremes of weight and renal dysfunction. Adjusted Dosing orders are available in this order set.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;In order to avoid the higher risk of heparin-induced thrombocytopenia, use of unfractionated heparin should be minimized.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Post-Op Prophylaxis to be continued up to 28 - 42 days. &amp;nbsp;Select &lt;strong&gt;ALL&lt;/strong&gt; orders from &lt;strong&gt;ONE&lt;/strong&gt; of the 2 following medication groups (either fondaparinux or low molecular weight heparin).&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;fondaparinux injection (linked orders) - For patients with history of HIT&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;fondaparinux injection (linked orders) - For patients with history of HIT&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;fondaparinux injection; 2.5 mg, subcutaneous, once, Give 6 hours Post-Op&apos;]/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;fondaparinux injection (linked orders) - For patients with history of HIT&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;fondaparinux injection; 2.5 mg, subcutaneous, every 24 hours, Start Post Op Day 1. Do not start every 24 hour dose LESS than 12 hours after previous fondaparinux dose.&apos;]/activities[at0001]">    <items>      <item>        <key>Transform.Technical Notes</key>        <value>&lt;p&gt;Starting H+24 hours&lt;/p&gt;</value>      </item>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;&amp;nbsp;Start Post Op Day 1. Do not start every 24 hour dose LESS than 12 hours after previous fondaparinux dose.&amp;nbsp;&lt;br&gt;Extended therapy for 28 to 35 days.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;fondaparinux injection (linked orders) - For patients with history of HIT&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;fondaparinux injection; 2.5 mg, subcutaneous, every 24 hours, Start Post Op Day 1. Do not start every 24 hour dose LESS than 12 hours after previous fondaparinux dose.&apos;]/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;tinzaparin injection&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;tinzaparin injection&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Select both tinzaparin orders.&lt;br&gt;·&amp;nbsp;&amp;nbsp; &amp;nbsp;Do not start q24h dose LESS than 12 hours after previous tinzaparin dose. Extended therapy for 28 days minimum.&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment Options&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Very High Risk - Extended Prophylaxis&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;tinzaparin injection&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 