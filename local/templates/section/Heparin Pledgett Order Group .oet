    <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; heparin pledgett&apos;]/activities[at0001]">
      <items>
        <item>
          <key>CKT.Content Details</key>
          <value>&lt;p&gt;Expose nail bed of affected digit, scratch surface every 1 to 2 hours and apply heparin soaked pledgett (10 000 units heparin in 100 mLs NS) to encourage bleeding&lt;/p&gt;</value>
