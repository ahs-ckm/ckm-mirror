        <value>&lt;p&gt;Frequency: Per protocol&lt;/p&gt;&lt;p&gt;Comments: Every 5 minutes x4, then every 15 minutes x2, then as per&amp;nbsp;protocol.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs; ; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Monitor Sensory Level(Dermatomes)&apos;]/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs; ; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Orthostatic blood pressure; once&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Postural Blood Pressure once, prior to first ambulation post neuraxial catheter insertion.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Interventions; ; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Bladder function&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Monitor bladder function post neuraxial catheter insertion: every shift and PRN&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Interventions; ; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;In and Out Catheter&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Frequency: PRN for urinary retention&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Interventions; ; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP)&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Notify Most Responsible Health Practitioner if urinary retention problems arise.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Wound Care; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Dressing Instructions; Implanted neuraxial catheter pocket site&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Location: Implanted neuraxial catheter pocket site&lt;/p&gt;&lt;p&gt;Frequency: Daily&lt;/p&gt;&lt;p&gt;Change dressing 24 hours post-procedure. Use sterile Normal Saline to cleanse incision site. Change daily until dry.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Wound Care; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Dressing Instructions; Neuraxial catheter access site&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Location: Neuraxial catheter access site&lt;/p&gt;&lt;p&gt;As per policy and procedure.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Wound Care; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Remove Sutures&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Starting: T+7 days at 0800&lt;/p&gt;&lt;p&gt;Comments:Remove sutures 7 days post neuraxial catheter insertion.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Notify Physician; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP); Acute Pain Service&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Notify Acute Pain Service with any concerns post neuraxial catheter insertion.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Notify Physician; Recovery &amp; On Unit&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP); Anesthesia