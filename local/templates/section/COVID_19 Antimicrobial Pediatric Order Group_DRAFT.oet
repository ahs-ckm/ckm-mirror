      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&amp;nbsp;50-75 mg/Kg/dose every 24 hours or Divided every 12 hours&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
