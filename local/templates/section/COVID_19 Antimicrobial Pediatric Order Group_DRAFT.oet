  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;strong&gt;Antibiotics&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Recommended if patient critically ill, at risk of early deterioration, suspicion of secondary bacterial pneumonia, or sepsis.&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Continuation of Antibiotics after initial empiric therapy is not recommended for confirmed COVID-19 patients who do not have proven (or strongly suspected) bacterial or fungal co-infection/superinfection.&lt;/em&gt;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Moderate Illness&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;amoxicillin or ampicillin as tolerated&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;amoxicillin recommended dosage: 45-90 mg/kg/day divided every 8 hours&lt;/p&gt;&lt;p&gt;ampicillin recommended dosage: 200-400 mg/kg/day divided every 6 hours.&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Severe Illness&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;cefTRIAXone&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
