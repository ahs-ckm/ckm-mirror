  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Monitor Sensory Level(Dermatomes)&apos;]/activities[at0001]">
