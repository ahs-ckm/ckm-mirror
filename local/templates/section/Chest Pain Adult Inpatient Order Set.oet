gt; &lt;em&gt;include any requirements for order set only if required for ordering below. I.e. Height and weight for medication calculations&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;span style=&quot;font-size:18px&quot;&gt;Order Set Keywords: __________&lt;/span&gt;&lt;/strong&gt; &lt;em&gt;search terms / synonyms for the order set&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Objectives:&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Target population: adults in the acute care setting with undifferentiated chest pain&lt;/li&gt;&lt;li&gt;Diagnose threat-threatening causes of chest pain e.g.:&lt;ul&gt;&lt;li&gt;Acute coronary syndrome&lt;/li&gt;&lt;li&gt;Acute aortic dissection&lt;/li&gt;&lt;li&gt;Pulmonary embolism&lt;/li&gt;&lt;li&gt;Tension pneumothorax&lt;/li&gt;&lt;li&gt;Pericardial tamponade&lt;/li&gt;&lt;li&gt;Empyema&lt;/li&gt;&lt;li&gt;Mediastinitis (e.g. esophagel rupture)&lt;/li&gt;&lt;/ul&gt;&lt;/li&gt;&lt;li&gt;Non-&apos;chest&apos; conditions that can present as chest pain&lt;ul&gt;&lt;li&gt;Referred pain of cholecystitis, thrombotic thrombocytopenia purpura, vertebral wedge fracture, pancreatitis&lt;/li&gt;&lt;/ul&gt;&lt;/li&gt;&lt;li&gt;Initial care is supportive and not specific to etiology&lt;/li&gt;&lt;/ul&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs&apos;]">     <items>       <item>         <key>Technical.ﻩ Technical Traceability</key>         <value>{~AHSID~d49ddec9-c657-431d-b456-a68643935794~NAME~Vital Signs Order Panel.oet}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs; Every 4 hours&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;if pain resolved&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Measure Blood Pressure; Every 1 hour; For 3 occurences&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;measure both arms&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Notify Most Responsible Health practioner(MRHP)&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;&amp;nbsp;if there is a greater than 20 mm Hg difference in systolic blood pressure at any reading&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Computed Tomography&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;CT Chest Enhanced&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Follow PE Protocol&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Diagnostic Imaging&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Interventional Radiology&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;(to assess for aortic dissection)&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Other Tests&apos;]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Electrocardiogram 12 Lead; PRN&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;For chest pain&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Nitrates&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;For Chest Pain;&lt;/p&gt;&lt;p&gt;Call MRHP if chest pain persists&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics&apos;]">     <items>       <item>         <key>Technical.ﻩ Technical Traceability</key>         <value>{~AHSID~c7048890-e469-4d70-b195-a200d3fe518a~NAME~Analgesics and Antipyretics Order Panel.oet}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v