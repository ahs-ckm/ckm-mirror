<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>592df834-521c-4362-855b-ef531225a698</id>   <name>Isolation COVID-19 Pediatric Admission Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose />       <use />       <misuse />     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value />       </item>       <item>         <key>Business Process Level</key>         <value />       </item>       <item>         <key>Care setting</key>         <value />       </item>       <item>         <key>Client group</key>         <value />       </item>       <item>         <key>Clinical Record Element</key>         <value />       </item>       <item>         <key>Copyright</key>         <value />       </item>       <item>         <key>Issues</key>         <value />       </item>       <item>         <key>Owner</key>         <value />       </item>       <item>         <key>Sign off</key>         <value />       </item>       <item>         <key>Speciality</key>         <value />       </item>       <item>         <key>User roles</key>         <value />       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]">     <items>       <item>         <key>Technical.ﻩ Technical Traceability</key>         <value>{~AHSID~4da59846-0ff1-481f-9514-7be15ed54fe8~NAME~Isolation COVID-19 Adult ICU Admission Order Panel.oet}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;strong&gt;How is COVID-19 Transmitted? &lt;/strong&gt;Droplet, indirect and direct contact.&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&amp;nbsp;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;Precautions Needed:&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;Contact and Droplet Precautions&lt;/p&gt;&lt;p&gt;OR&lt;/p&gt;&lt;p&gt;Move to Airborne Precautions (isolation room with negative pressure ventilation) if &amp;nbsp;Aerosol-Generating Medical Procedures required. If unable to move patient to airborne isolation room, ensure door is closed prior to AGMP.&lt;/p&gt;&lt;p&gt;N95 respirator required ONLY when performing AGMPs.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;List of Aerosol generating medical procedures (AGMPs):&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;· Humidified high flow oxygen systems (e.g. ARVO, Optiflow)&lt;/p&gt;&lt;p&gt;· Intubation and related procedures (e.g. manual ventilation, open endotracheal suctioning)&lt;/p&gt;&lt;p&gt;·&amp;nbsp;Cardiopulmonary resuscitation (CPR)&lt;/p&gt;&lt;p&gt;·&amp;nbsp;Bi-level Positive Airway Pressure (e.g. BiPAP)&lt;/p&gt;&lt;p&gt;·&amp;nbsp;Tracheostomy care&lt;/p&gt;&lt;p&gt;·&amp;nbsp;Bronchoscopy&lt;/p&gt;&lt;p&gt;·&amp;nbsp;Sputum induction&lt;/p&gt;&lt;p&gt;·&amp;nbsp;Nebulized therapy/aerosolized medication administration&lt;/p&gt;&lt;p&gt;·&amp;nbsp;Open respiratory/airway suctioning&lt;/p&gt;&lt;p&gt;· High frequency oscillatory ventilation&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Safety and Precautions – Refer to Infection and Prevention and Control (IPC) guidelines (link:&amp;nbsp;https://www.albertahealthservices.ca/assets/healthinfo/ipc/hi-ipc-resource-manual-main-document.pdf)&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Initiate airborne isolation']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Confirmed or suspected reason for airborne isolation? &lt;strong&gt;!&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Initiate airborne and contact isolation']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Confirmed or suspected reason for airborne &amp;amp; contact isolation? &lt;strong&gt;!&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Initiate contact isolation']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Confirmed or suspected reason for contact&amp;nbsp;isolation? &lt;strong&gt;!&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Initiate contact and droplet isolation']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Confirmed or suspected reason for contact &amp;amp; droplet isolation?&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; Other: COVID-19&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Initiate contact and droplet isolation']/activities[at0156]">     <items>       <item>         <key>Transform.Preselected</key>         <value>&lt;p&gt;Y&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Initiate droplet isolation']/activities[at0001]">     <items>       <item>         <key>CKT.Content Details</key>         <value>&lt;p&gt;Confirmed or suspected reason for droplet&amp;nbsp;isolation? &lt;strong&gt;!&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.orderable_section.v1" concept_name="Orderable Section" max="1" name="Isolation COVID-19 Pediatric Admission Smart Group">     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="11651acf-3b92-4684-8535-5a54a7a53ce9" concept_name="SURG ISOLATION Initiate airborne isolation Order" max="1" path="/items" name="Initiate airborne isolation" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="f70f05b5-8bd8-4b2e-b421-a6e24e596a7e" concept_name="SURG ISOLATION Initiate airborne and contact isolation Order" max="1" path="/items" name="Initiate airborne and contact isolation" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="25bb4da1-7d1b-40ea-94c9-ad9b92cd1e40" concept_name="SURG ISOLATION Initiate contact isolation Order" max="1" path="/items" name="Initiate contact isolation" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="11afb2d3-2d4c-4ae8-8bef-9aa4d567abdc" concept_name="SURG ISOLATION Initiate contact and droplet isolation Order" max="1" path="/items" name="Initiate contact and droplet isolation" />     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="e005e3c0-01ab-4b8d-81e4-c3723fb68cc0" concept_name="SURG ISOLATION Initi