  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Ongoing management: serum glucose Less than 14 mmol/L (D5-NaCl 0.45%)&apos;]">
