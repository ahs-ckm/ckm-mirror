                 <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                   <items>
                     <item>
                       <key>CKT.Clinical Guidance</key>
                       <value>&lt;p&gt;1 cycles, 3 days (5 days if given riTUXimab), Cyclophosphamide, Etoposide, CISplatin, riTUXimab, stem cell mobilization&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
                     </item>
                   </items>
                 </annotations>
                 <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day 0 &apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Orders&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Provider Communication&apos;]/activities[at0001]">
                   <items>
                     <item>
                       <key>CKT.Content Details</key>
                       <value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; This protocol has optional riTUXimab. If riTUXimab is not required, please delete all orders related to riTUXimab on Day 0 and Day 4.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
                     </item>
                   </items>
                 </annotations>
                 <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day 0 &apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Patient Care Orders&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Provider Communication&apos;]/activities[at0156]">
