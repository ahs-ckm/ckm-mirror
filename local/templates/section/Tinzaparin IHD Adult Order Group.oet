<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>0acd487f-e9ad-4452-8571-200f268e5d76</id>  <name>Tinzaparin IHD Adult Order Group</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose/>      <use/>      <misuse/>    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value/>      </item>      <item>        <key>Business Process Level</key>        <value/>      </item>      <item>        <key>Care setting</key>        <value/>      </item>      <item>        <key>Client group</key>        <value/>      </item>      <item>        <key>Clinical Record Element</key>        <value/>      </item>      <item>        <key>Copyright</key>        <value/>      </item>      <item>        <key>Issues</key>        <value/>      </item>      <item>        <key>Owner</key>        <value/>      </item>      <item>        <key>Sign off</key>        <value/>      </item>      <item>        <key>Speciality</key>        <value/>      </item>      <item>        <key>User roles</key>        <value/>      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Dosing Approach&lt;br&gt;&amp;nbsp;&lt;br&gt;1. The following pre-filled syringes will be in stock for dialysis use: 2500 IU, 3500 IU, and 4500 IU (8000 IU based on needs).&amp;nbsp;&lt;br&gt;2. Standard dose shall be 2500 IU and titrated based on needs.&amp;nbsp;&lt;br&gt;&amp;nbsp;&lt;/p&gt;&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;1&quot; border=&quot;1&quot; style=&quot;width:677px&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style=&quot;width:162px&quot;&gt;Signs of Clotting&lt;/td&gt;&lt;td style=&quot;width:514px&quot;&gt;Tinzaparin Adjustment Guidelines (after 2 consecutive runs)&lt;br&gt;Note: minimum effective dose of Tinzaparin should be used with consideration for anticoagulation-free dialysis whenever possible.&amp;nbsp;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:162px&quot;&gt;0 – 1&lt;/td&gt;&lt;td style=&quot;width:514px&quot;&gt;Decrease by 1 increment to 2500 IU. If there is risk of bleeding, attempt anticoagulation-free dialysis.&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:162px&quot;&gt;1&lt;/td&gt;&lt;td style=&quot;width:514px&quot;&gt;Normal, no Tinzaparin adjustment required (2500 IU). If consistently 1, attempt to decrease Tinzaparin by one increment to 2500 IU.&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style=&quot;width:162px&quot;&gt;Greater than 2&lt;/td&gt;&lt;td style=&quot;width:514px&quot;&gt;Increase by 1 increment. If dose greater than 4500 IU, obtain order.&amp;nbsp;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tinzaparin subcutaneous; 2500 unit/0.25 mL injection syringe&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;2,500 units, other - see admin instructions, once, as needed, in dialysis, into IHD circuit at beginning of run&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tinzaparin subcutaneous; 3500 unit/0.35 mL injection syringe&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;3,500 units, other - see admin instructions, once, as needed, in dialysis, into IHD circuit at beginning of run&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tinzaparin subcutaneous; 4500 unit/0.45 mL injection syringe&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;4,500 units, other - see admin instructions, once, as needed, in dialysis, into IHD circuit at beginning of run&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tinzaparin subcutaneous; 8000 unit/0.4 mL injection syringe&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;8,000 units, other - see admin instructions, once, as needed, in dialysis, into IHD circuit at beginning of run&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; IHD - Record and Monitor Filter Score&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Record and monitor Filter Clotting Score.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ﻩ Technical Traceability</key>        <value>{~AHSID~0acd487f-e9ad-4452-8571-200f268e5d76~NAME~Tinzaparin IHD Adult Order Group}</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1