<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>ec879fc3-df81-4b6c-84cd-c09b7e89c496</id>   <name>Laxatives Preprocedure Pediatric Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">     <items>       <item>         <key>Technical.ﻩ Technical Traceability</key>         <value>{~AHSID~ec879fc3-df81-4b6c-84cd-c09b7e89c496~NAME~Laxatives Preprocedure Pediatric Smart Group}</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;Polyethylene Glycol 3350 (PEG 3350)&lt;/em&gt; &lt;em&gt;should be ordered regularly for all patients on opioids on enteral feeds unless contraindicated. Dose 0.5-1g/kg OD max suggested dose is 17 g/DAY and increase to BID if no bowel movement after 72 hours. &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;If PEG 3350 not effective, start by combining with laxative agent from different class.&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Consider discussion with pharmacy regarding methylnaltrexone and or naloxegol for end of life&amp;nbsp;intractable constipation scenarios.&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;bisaCODyl&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;Age based dosing.&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Not recommended for children under 6 years.&amp;nbsp; &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;6 -12 years: 5 mg&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Over 12 years: 10 mg&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;bisaCODyl&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;bisaCODyl tablet enteric-coated; 5 mg, oral, daily, at bedtime&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;&lt;em&gt;Do not crush, chew, or split.&lt;/em&gt;&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;bisaCODyl&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;bisaCODyl tablet enteric-coated; 5 mg, oral, daily, at bedtime as needed, constipation&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;&lt;em&gt;Do not crush, chew, or split.&lt;/em&gt;&lt;/p&gt;</value>       </item>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;bisaCODyl&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;bisaCODyl Suppository 5 mg; 5 mg, rectal, daily&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;bisaCODyl&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;bisaCODyl Suppository 5 mg; 5 mg, rectal, daily as needed, constipation&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Lactulose&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;Dose 1 to 2 g/kg/day (equivalent to 1.5 to 3mL/kg/day) &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Administered daily or divided into BID dosing.&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Lactulose&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lactulose ORAL liquid 667 mg/mL; 1.5 mL/kg/day, oral, 2 times per day&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Lactulose&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;lactulose ORAL liquid 667 mg/mL; 1.5 mL/kg/dose, oral, 2 times per day, as needed, constipation&apos;]/activities[at0001]">     <items>       <item>         <key>CKT.Phase of Care</key>         <value>&lt;p&gt;Preprocedure/Day Surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;sennosides&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>CKT.Clinical Guidance</key>         <value>&lt;p&gt;&lt;em&gt;Age based dosing&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;2-6 years: 4.4 – 6.6mg (2.5-3.75mL) &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;6-12 years: 8.8-13.2mg (5-7.5mL) &lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;Greater than 12 years: 17.6-26.4mg (10-15mL)&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;sennosides&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sennosides ORAL liqui