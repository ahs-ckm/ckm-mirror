   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Opioid Naive Healthy Adult Oral and IV&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Oral Opioids&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;oxyCODONE-acetaminophen Tablet 5 mg-325 mg (Percocet); 1 tab PO once&apos;]/activities[at0001]">
     <items>
       <item>
