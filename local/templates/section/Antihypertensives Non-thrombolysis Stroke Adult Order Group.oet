       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Antihypertensives ACUTE Treatment&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
         <items>
           <item>
             <key>CKT.Clinical Guidance</key>
             <value>&lt;p&gt;&amp;nbsp;&lt;strong&gt;Antihypertensives – Acute Treatment&lt;/strong&gt;&lt;br&gt;Treat acute BP elevation greater than 220/120 for ischemic stroke. Avoid short-acting calcium channel blockers because of the risk of hypertension (Notify MD if BP 220/120 mmHg on two consecutive readings 10 to 15 min apart). &amp;nbsp;Use intravenous antihypertensives for the first 24 hours only. &amp;nbsp;Switch to oral/per NG medication as soon as reasonably possible after 24 hours&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Antihypertensives ACUTE Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;labetalol injection&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Content Details</key>
             <value>&lt;p&gt;&amp;nbsp;10 mg, intravenous, every 10 minutes, as needed, hypertension, Maximum 40 mg/dose. Maximum daily dose of 300 mg. &amp;nbsp;Administer only on monitored units. Caution for HR less than 60 bpm&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Antihypertensives ACUTE Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;metoPROLOL injection&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Content Details</key>
             <value>&lt;p&gt;5 mg, intravenous, every 10 minutes, as needed, hypertension, Titrate to effect. Max dose 15 mg in 3 hours. Administer only on monitored units. &amp;nbsp;Caution for HR less than 60 bpm&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Antihypertensives ACUTE Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;hydrALAZINE injection&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Content Details</key>
             <value>&lt;p&gt;&amp;nbsp;10 mg, every 10 minutes, as needed, hypertension, Titrate to effect. Maximum 40mg/dose&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Antihypertensives ACUTE Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;enalaprilat injection&apos;]/activities[at0001]">
         <items>
           <item>
             <key>CKT.Content Details</key>
             <value>&lt;p&gt;&amp;nbsp;1.25 mg, intravenous, every 6 hours, as needed, hypertension, Titrate to effect.&lt;/p&gt;</value>
           </item>
         </items>
       </annotations>
       <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Antihypertensives ACUTE Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;capTOPRIL Tablet&apos;]/activities[at0001]">
