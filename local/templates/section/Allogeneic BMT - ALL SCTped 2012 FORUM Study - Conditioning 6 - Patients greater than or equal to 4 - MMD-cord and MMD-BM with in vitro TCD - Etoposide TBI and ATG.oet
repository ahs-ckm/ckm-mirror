tems[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -11&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Systemic Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]"><items><item><key>Transform.Preselected</key><value>&lt;p&gt;Y&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -10 to Day -9&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]"><items><item><key>CKT.Clinical Guidance</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Premedication&amp;nbsp;to be given 30 minutes prior to ATG.&lt;/p&gt;</value></item><item><key>CKT.Content Details</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Dose:&amp;nbsp;1 mg/kg/dose; maximum dose 50 mg&lt;/p&gt;&lt;p&gt;&amp;nbsp; Offset: 0&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -10 to Day -9&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]"><items><item><key>Transform.Preselected</key><value>&lt;p&gt;Y&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -10 to Day -9&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Systemic Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]"><items><item><key>CKT.Clinical Guidance</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Hypersensitivity precautions with ATG.&lt;br&gt;&amp;nbsp;&amp;nbsp;If patient experiences fever, chills or rigors extend the infusion to 6 -12 hours.&amp;nbsp;&lt;br&gt;&amp;nbsp; Consider use of acetaminophen for fevers. Consult attending physician.&amp;nbsp;&lt;br&gt;&amp;nbsp; Do not infuse with lipids or cyclosporine.&lt;/p&gt;</value></item><item><key>CKT.Content Details</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Dose:&amp;nbsp;2.5 mg/kg/dose&lt;/p&gt;&lt;p&gt;&amp;nbsp; Offset: 30 minutes&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -10 to Day -9&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Systemic Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]"><items><item><key>Transform.Preselected</key><value>&lt;p&gt;Y&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day - 6 to Day -4&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Systemic Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]"><items><item><key>CKT.Content Details</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Dose: 2 Gy twice daily&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day - 6 to Day -4&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Systemic Treatment&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]"><items><item><key>Transform.Preselected</key><value>&lt;p&gt;Y&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Nursing Orders&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]"><items><item><key>CKT.Content Details</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; On day of etoposide infusion monitor urine output every 4 hours. If urine output is LESS than 80% of fluid intake measured every 4 hours notify MD/NP for diuretic order. &amp;nbsp;MD/NP may consider furosemide.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Nursing Orders&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]"><items><item><key>Transform.Preselected</key><value>&lt;p&gt;Y&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-Medications&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;dexAMETHasone Injection; IV, once&apos;]/activities[at0001]"><items><item><key>CKT.Clinical Guidance</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&amp;nbsp;Premedications to be given 30 minutes prior to etoposide.&lt;/p&gt;</value></item><item><key>CKT.Content Details</key><value>&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp; Dose: 5 mg/m2/dose; maximum dose 8 mg&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value></item></items></annotations><annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day -3&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-Medications&apos;]/ite