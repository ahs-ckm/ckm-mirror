  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Vital Signs&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;Frequency: Per protocol&lt;/p&gt;&lt;p&gt;Comments:&amp;nbsp;Pulse, Blood Pressure, Respirations, Oxygen Saturation POST procedure every&amp;nbsp;5 minutes x 4, then every 15 minutes &amp;nbsp;x 2, then every&amp;nbsp;30 minutes x 2.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
