  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Patients who are at higher risk of GI complications (high dose or use of scheduled NSAIDs for greater than 5 days, history of peptic ulceration or GI bleeding) may benefit from agents that could suppress gastric acidity.&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;famotidine&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;Suggested dose famotidine injection:&lt;br&gt;neonates: 0.25 - 0.5 mg/kg/dose, every 24 hours&lt;br&gt;Infants/Children: 0.5 - 1 mg/kg/dose, every 12 hours, MAX dose 20 mg/dose&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;famotidine&apos;]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">
