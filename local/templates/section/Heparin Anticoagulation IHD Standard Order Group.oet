<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>3a411276-4915-45fd-8bdb-1cb40eb4f2f6</id>  <name>Heparin Anticoagulation IHD Standard Order Group</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose />      <use />      <misuse />    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value />      </item>      <item>        <key>Business Process Level</key>        <value />      </item>      <item>        <key>Care setting</key>        <value />      </item>      <item>        <key>Client group</key>        <value />      </item>      <item>        <key>Clinical Record Element</key>        <value />      </item>      <item>        <key>Copyright</key>        <value />      </item>      <item>        <key>Issues</key>        <value />      </item>      <item>        <key>Owner</key>        <value />      </item>      <item>        <key>Sign off</key>        <value />      </item>      <item>        <key>Speciality</key>        <value />      </item>      <item>        <key>User roles</key>        <value />      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='heparin, preservative free injection; 500 units/mL in NaCl 0.9% syringe; *** units/hr, other - see admin instructions, continuous as needed, into IHD circuit']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Titrate as per nomogram, for IHD circuit.&lt;/p&gt;&lt;table cellpadding="1" cellspacing="1" border="1" style="width:742px"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="width:148px"&gt;&lt;p&gt;PTT&lt;/p&gt;(results in seconds)&lt;/td&gt;&lt;td style="width:591px"&gt;Change in Heparin Infusion Rate&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="width:148px"&gt;&lt;p&gt;Less than or equal to&lt;/p&gt;50&lt;/td&gt;&lt;td style="width:591px"&gt;Re-bolus with 1000 units and increase rate by 500 units/h&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="width:148px"&gt;51 to 64&lt;/td&gt;&lt;td style="width:591px"&gt;Increase rate by 500 units/h&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="width:148px"&gt;65 - 90&lt;/td&gt;&lt;td style="width:591px"&gt;No change to rate&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="width:148px"&gt;91 –&amp;nbsp;125&lt;/td&gt;&lt;td style="width:591px"&gt;Decrease by 500 units/h. If infusion rate less than 250 units/h, leave at current rate and contact physician&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="width:148px"&gt;Greater than 125&lt;/td&gt;&lt;td style="width:591px"&gt;Stop heparin infusion for 60 minutes, then restart at 500 units/h less than previous rate&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; IHD Clotting Score']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Until discontinued&lt;/p&gt;&lt;p&gt;For a clotting score of 2 to 3, contact physician for possible increase to heparin rate or bolus.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; IHD Clotting Score']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; IHD BPA']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Until discontinued&lt;/p&gt;&lt;p&gt;BPA - Flush filter with 150 mL substituate fluid every one (1) hour as needed.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Nursing Communication; IHD BPA']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.section_supporting_context.v1]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;N&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ﻩ Technical Traceability</key>        <value>{~AHSID~3a411276-4915-45fd-8bdb-1cb40eb4f2f6~NAME~Heparin Anticoagulation IHD Standard Order Group}</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Heparin Anticoagulation">    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="f6df7262-0810-4ee2-bb10-c315c36e987c" concept_name="PHARM heparin, preservative free injection Order" max="1" path="/items" name="heparin, preservative free injection; 1000 unit/mL injection bolus; *** units, other - see admin instructions, once, Initial bolus dose into the IHD circuit" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="f6df7262-0810-4ee2-bb10-c315c36e987c" concept_name="PHARM heparin, preservative free injection Order" max="1" path="/items" name="heparin, preservative free injection; 500 units/mL in NaCl 0.9% syringe; *** units/hr, other - see adm