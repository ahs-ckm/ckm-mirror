  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;strong&gt;When NPO, if Blood Glucose is Greater than 10.0 mmol/L – Use Correction Insulin for Hyperglycemia.&lt;br&gt;&amp;nbsp;&lt;br&gt;Patients who are not eating and drinking must still receive basal and correction insulin.&lt;/strong&gt; &amp;nbsp;For patients with known home regimen, order patient’s usual correction insulin brand and dosing. &amp;nbsp;For patients with unknown home insulin regimen, calculate the patient&apos;s total daily dose (TDD) of home insulin estimating 0.3 - 0.5 units/kg/day and choose correction dose below.&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;&lt;strong&gt;When NPO, if Blood Glucose is Greater than 10.0 mmol/L – Use Correction Insulin for Hyperglycemia.&lt;br&gt;&amp;nbsp;&lt;br&gt;Patients who are not eating and drinking must still receive basal and correction insulin. &amp;nbsp;&lt;/strong&gt;For patients with known home regimen, order patient’s usual correction insulin brand and dosing. &amp;nbsp;For patients with unknown home insulin regimen, calculate the patient&apos;s total daily dose (TDD) of home insulin estimating 0.3 - 0.5 units/kg/day and choose correction dose below.&amp;nbsp;&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0156]">
    <items>
      <item>
        <key>Transform.Preselected</key>
        <value>&lt;p&gt;Y&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro injection; for TDD 15 - 30&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;0-2 units, subcutaneous, Q4H as needed&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 1 unit if Blood Glucose 10.1 - 14.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 14.1 - 18.0 mmol/L&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro injection; for TDD 31-50&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;0-3&amp;nbsp;units, subcutaneous, Q4H as needed&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 1 unit if Blood Glucose 10.1 - 12.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 12.1 - 15.0 mmol/L&lt;br&gt;Give 3 units if Blood Glucose 15.1 - 18.0 mmol/L&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro injection; for TDD 51 - 80&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;0-5&amp;nbsp;units, subcutaneous, Q4H as needed&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 10.1 - 12.0 mmol/L&lt;br&gt;Give 3 units if Blood Glucose 12.1 - 14.0 mmol/L&lt;br&gt;Give 4 units if Blood Glucose 14.1 - 16.0 mmol/L&lt;br&gt;Give 5 units if Blood Glucose 16.1 - 18.0 mmol/L&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro injection; for TDD 81 or more&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;&amp;nbsp;0-10&amp;nbsp;units, subcutaneous, Q4H as needed&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 10.1 - 11.0 mmol/L&lt;br&gt;Give 4 units if Blood Glucose 11.1 - 13.0 mmol/L&lt;br&gt;Give 6 units if Blood Glucose 13.1 - 15.0 mmol/L&lt;br&gt;Give 8 units if Blood Glucose 15.1 - 17.0 mmol/L&lt;br&gt;Give 10 units if Blood Glucose 17.1 - 18.0 mmol/L&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro injection; Custom Correction Dose lispro (Known Home Regimen, Post-pancreatectomy, Extreme Insulin Sensitivity or Resistance)&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;*** units, subcutaneous, Q4H as needed&lt;/p&gt;&lt;p&gt;BG *** to *** mmol/L = insulin dose *** units&lt;br&gt;BG *** to *** mmol/L = insulin dose *** units&lt;br&gt;BG *** to *** mmol/L = insulin dose *** units&lt;br&gt;BG *** to *** mmol/L = insulin dose *** units&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
