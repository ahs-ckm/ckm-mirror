  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro (HumaLOG) injection; for TDD 15 - 30&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;0-2 units, subcutaneous, 3 times per day, with meals&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 1 unit if Blood Glucose 10.1 - 14.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 14.1 - 18.0 mmol/L&lt;br&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro (HumaLOG) injection; for TDD 31-50&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;&amp;nbsp;0-3&amp;nbsp;units, subcutaneous, 3 times per day, with meals&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 1 unit if Blood Glucose 10.1 - 12.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 12.1 - 15.0 mmol/L&lt;br&gt;Give 3 units if Blood Glucose 15.1 - 18.0 mmol/L&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro (HumaLOG) injection; for TDD 51 - 80&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;0-5&amp;nbsp;units, subcutaneous, 3 times per day, with meals&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 10.1 - 12.0 mmol/L&lt;br&gt;Give 3 units if Blood Glucose 12.1 - 14.0 mmol/L&lt;br&gt;Give 4 units if Blood Glucose 14.1 - 16.0 mmol/L&lt;br&gt;Give 5 units if Blood Glucose 16.1 - 18.0 mmol/L&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Day of Procedure (Inpatient)&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Treatment for Above Target Blood Glucose Pre-procedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Correction Dose lispro (single select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;insulin lispro (HumaLOG) injection; for TDD 81 or more&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Content Details</key>
        <value>&lt;p&gt;&amp;nbsp;&amp;nbsp;0-10&amp;nbsp;units, subcutaneous, 3 times per day, with meals&lt;/p&gt;&lt;p&gt;Give 0 unit if Blood Glucose 4.1 - 10.0 mmol/L&lt;br&gt;Give 2 units if Blood Glucose 10.1 - 11.0 mmol/L&lt;br&gt;Give 4 units if Blood Glucose 11.1 - 13.0 mmol/L&lt;br&gt;Give 6 units if Blood Glucose 13.1 - 15.0 mmol/L&lt;br&gt;Give 8 units if Blood Glucose 15.1 - 17.0 mmol/L&lt;br&gt;Give 10 units if Blood Glucose 17.1 - 18.0 mmol/L&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>
      </item>
    </items>
  </annotations>
