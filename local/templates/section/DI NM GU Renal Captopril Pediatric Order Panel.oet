  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Vital Signs&apos;]/activities[at0001]">
    <items>
      <item>
        <key>CKT.Clinical Guidance</key>
        <value>&lt;p&gt;VS x1 before ACE inhibitor administration; then VS q10-15 minutes for 1 hour; then, VS x 1 after Scan&lt;/p&gt;</value>
      </item>
