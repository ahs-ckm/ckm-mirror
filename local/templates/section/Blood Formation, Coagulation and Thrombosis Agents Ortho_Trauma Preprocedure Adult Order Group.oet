     <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Management of Anit-Platelet Therapy&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
