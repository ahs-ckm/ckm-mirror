<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>429f0df2-eca3-4f2b-9798-d27ebfc23ad7</id>  <name>DRAFT_IV Insulin_NPO Patients Requiring Regular Fluid Volumes Order Group</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose />      <use />      <misuse />    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value />      </item>      <item>        <key>Business Process Level</key>        <value />      </item>      <item>        <key>Care setting</key>        <value />      </item>      <item>        <key>Client group</key>        <value />      </item>      <item>        <key>Clinical Record Element</key>        <value />      </item>      <item>        <key>Copyright</key>        <value />      </item>      <item>        <key>Issues</key>        <value />      </item>      <item>        <key>Owner</key>        <value />      </item>      <item>        <key>Sign off</key>        <value />      </item>      <item>        <key>Speciality</key>        <value />      </item>      <item>        <key>User roles</key>        <value />      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;Exclude patients with fluid restrictions, end stage renal failure, and/or diabetic ketoacidosis&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Please do POCT Glucose every one hour for sugars less than 5 and greater than 15. For sugars 5.1-15 do POCT glucose every 2 hours.&amp;nbsp;Only AHS POCT blood glucose device to be used to initiate and monitor IV insulin infusion.&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Glucose Meter POCT']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='insulin regular (HumuLIN R) 100 units in NaCl 0.9% 100 mL (1 unit/mL) infusion']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Intravenous, continuous&lt;/p&gt;&lt;p&gt;CAUTION: High Alert Medication&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Table 1:&amp;nbsp; Initiation of Insulin for POCT Glucose Reading of 5.1mmol/L and Greater for NPO patients on Regular Fluids&lt;/p&gt;&lt;table border="1" cellspacing="0" cellpadding="0"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;POCT Glucose Reading (mmol/L)&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Insulin Rate (Unit/hour = ml/hour)&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Less than or equal to 4.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;DO NOT start insulin.&lt;/p&gt;&lt;p&gt;Hold and follow HYPOglycemia procedure&lt;/p&gt;&lt;p&gt;START D10W at 50mL/hour[TM1]&amp;nbsp;.&lt;/p&gt;&lt;p&gt;&amp;nbsp;Notify MRHP.&lt;/p&gt;&lt;p&gt;&amp;nbsp;Repeat POCT BG in 1 hour.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;4.1 – 5.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;DO NOT start insulin.&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour.&lt;/p&gt;&lt;p&gt;Initiate insulin once POCT glucose is greater than 5.0 mmol/L[TM2]&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;5.1- 8.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;START insulin at 0.7 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;8.1 – 12.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;START insulin at 1 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;12.1 – 15.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;START insulin at 1.2 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;15.1 – 18.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;START insulin at 1.5 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Greater than 18.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Follow HYPERglycemia procedure and contact MRHP.&lt;/p&gt;&lt;p&gt;Send for STAT ketone. If ketone result is positive, contact MRHP[TM3]&amp;nbsp;.&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour[TM4]&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;Table 2: Insulin Rate Adjustment&lt;/p&gt;&lt;p&gt;(USE TABLE 3 if POCT blood glucose drops by more than 3 mmol/L)&lt;/p&gt;&lt;table border="1" cellspacing="0" cellpadding="0"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;POCT Glucose Reading (mmol/L)&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Adjust Insulin Rate (Unit/Hour = mL/hour)&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Less than or equal to 5.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;*DECREASE by 0.5 units/hour&lt;/p&gt;&lt;p&gt;Notify MRHP&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;5.1 - 8.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;*DECREASE by 0.3 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 2 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;8.1 – 12.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;No Change to Rate&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 4 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;12.1 – 15.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;INCREASE by 0.5 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in&amp;nbsp; 4 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;15.1 – 18.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;INCREASE by 1 unit/hour&lt;/p&gt;&lt;p&gt;Repeat&amp;nbsp; POCT BG 2 hours&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Greater than 18.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Follow HYPERglycemia procedure and contact MRHP.&lt;/p&gt;&lt;p&gt;Send for STAT ketone. If ketone result is positive, contact MRHP.&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour[TM5]&amp;nbsp;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&lt;strong&gt;*If insulin infusion is stopped, restart at 0.5 units/hour once POCT blood glucose rises to greater than 12 mmol/L. Use Table 2 for subsequent rate adjustments.&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&amp;nbsp;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;Table 3: Rate when POCT blood glucose drops by 3 mmol/L or more from previous reading&lt;/p&gt;&lt;table border="1" cellspacing="0" cellpadding="0"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;POCT Glucose Reading (mmol/L)&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Insulin Rate (Unit/hour = mL/hour)&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Less than or equal to 8.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;INFUSE at 0.5 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;8.1 – 12.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;INFUSE at 1 units/ hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;Greater than 12.0&lt;/p&gt;&lt;/td&gt;&lt;td style="vertical-align:top"&gt;&lt;p&gt;INFUSE at 1.5 units/hour&lt;/p&gt;&lt;p&gt;Repeat POCT BG in 1 hour&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p&gt;&lt;strong&gt;&amp;nbsp;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;[TM1]Karmon/Leta: Did you want this here?&lt;/p&gt;&lt;p&gt;&amp;nbsp;[TM2]Karmon/Leta: Added as per our last meeting in November. Please validate this change&lt;/p&gt;&lt;p&gt;&amp;nbsp;[TM3]Karmon/Leta: Did you want this here?&lt;/p&gt;&lt;p&gt;&amp;nbsp;[TM4]Stopped panels at 18 mmol/L&lt;/p&gt;&lt;p&gt;&amp;nbsp;[TM5]Stopped panels at 18 mmol/L&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='insulin regular (HumuLIN R) 100 units in NaCl 0.9% 100 mL (1 unit/mL) infusion']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='dextrose 10% in water infusion']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;80 mL/hr, intravenous, continuous.&lt;/p&gt;&lt;p&gt;Infuse at 80 mL/hr regardless of all subsequent capillary glucose readings&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='dextrose 10% in water infusion']/activities[at0156]">    <items>      <item>        <key>Transform.Preselected</key>        <value>&lt;p&gt;Y&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Beta-Hydroxybutyrate (BOH)']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;As needed, Conditional Order: Available for nurse to activate if patient on insulin and blood glucose is greater than 18.0 mmol/L&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Urinalysis; For urine ketones']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;As needed, Conditional Order: Available for nurse to activate if patient on insulin and blood glucose is greater than 18.0 mmol/L&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value='Urinalysis, POCT; For urine ketones']/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;As needed, Conditional Order: Available for nurse to activate if patient on insulin and blood glucose is greater than 18.0 mmol/L&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ﻩ Technical Traceability</key>        <value>{~AHSID~429f0df2-eca3-4f2b-9798-d27ebfc23ad7~NAME~DRAFT_IV Insulin_NPO Patients Requiring Regular Fluid Volumes Order Group}</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="IV Insulin - NPO Patients Requiring Regular Fluid Volumes">    <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="e8da34ee-6e55-438b-ba29-8a6c15655d40" concept_name="LAB Glucose Meter POCT Order" max="1" path="/items" name="Glucose Meter POCT" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="28fe8b51-7253-4365-86fd-5b8512de277e" concept_name="PHARM insulin regular (HumuLIN R) 100 units in NaCl 0.9% 100 mL (1 unit/mL) infusion Order" max="1" path="/items" name="insulin regular (HumuLIN R) 100 units in NaCl 0.9% 100 mL (1 unit/mL) infusion" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="38385979-e2ab-4d77-aed7-a1b51519a42d" concept_name="PHARM dextrose 10% in water infusion Order" max="1" path="/items" name="dextrose 10% in water infusion" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="991ba455-614e-4c92-b890-e8daffecdabb" concept_name="LAB Beta-Hydroxybutyrate (BOH) Order" max="1" path="/items" name="Beta-Hydroxybutyrate (BOH)" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="384a2bd0-a6cd-4355-a421-a87822e31f2a" concept_name="LAB Urinalysis Order" max="1" path="/items" name="Urinalysis; For urine ketones" />    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="a5014e76-dbf6-4c5b-869d-84217c4769bc" concept_name="LAB Urinalysis POCT Order" max="1" path="/items" name="Urinalysis, POCT; For urine ketones" />  </definition>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.adhoc.v1">    <digest id="MD5-CAM-1.0.1">8AE70853975A75254E6D39A8DE3652AF</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1">    <digest id="MD5-CAM-1.0.1">4B2F0F6CD593E442DA9C30277A3EA9CF</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1">    <digest id="MD5-CAM-1.0.1">AD8DFB192E67715F0D9E11E95E674AD0</digest>  </integrity_checks>  <integrity_checks xsi:type="ArchetypeIntegrity" archetype_id="openEHR-EHR-CLUSTER.service_request_information.v1">    <digest id="MD5-CAM-1.0.1">230A8D964017E1FD7AC82EBDEE0202A6</digest>  </integrity_checks></template>