<template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">  <id>752ecf36-882c-4e31-b807-1a9b7f368e5b</id>  <name>DI PT Brown Fat Supression Pediatric Order Panel</name>  <description>    <lifecycle_state>Initial</lifecycle_state>    <details>      <purpose/>      <use/>      <misuse/>    </details>    <other_details>      <item>        <key>MetaDataSet:Sample Set </key>        <value>Template metadata sample set </value>      </item>      <item>        <key>Acknowledgements</key>        <value/>      </item>      <item>        <key>Business Process Level</key>        <value/>      </item>      <item>        <key>Care setting</key>        <value/>      </item>      <item>        <key>Client group</key>        <value/>      </item>      <item>        <key>Clinical Record Element</key>        <value/>      </item>      <item>        <key>Copyright</key>        <value/>      </item>      <item>        <key>Issues</key>        <value/>      </item>      <item>        <key>Owner</key>        <value/>      </item>      <item>        <key>Sign off</key>        <value/>      </item>      <item>        <key>Speciality</key>        <value/>      </item>      <item>        <key>User roles</key>        <value/>      </item>    </other_details>  </description>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;Perform baseline vital signs.&lt;/p&gt;&lt;p&gt;If HR less than 5th percentile per age or less than 60 bpm, review with NM physician before proceeding.&lt;/p&gt;&lt;p&gt;If patient greater than or equal to 10 years old, and SBP less than 90 mmHg, review with NM physician before proceeding.&lt;/p&gt;&lt;p&gt;If patient less than 10 years old, and SBP less than 70+2(age) mmHg, review with NM physician before proceeding&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;propranolol tablet&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;0.5 mg/kg, oral, once in imaging.&lt;/p&gt;&lt;p&gt;If patient is diabetic, review with NM physician before proceeding.&lt;/p&gt;&lt;p&gt;Do not use propranolol liquid oral formulation (contains sugar); use tablets only.&lt;/p&gt;&lt;p&gt;Maximum dose 40 mg. Give at least 60-75 minutes prior to FDG injection.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">    <items>      <item>        <key>CKT.Clinical Guidance</key>        <value>&lt;p&gt;&lt;strong&gt;OR&lt;/strong&gt;&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;diazePAM tablet&apos;]/activities[at0001]">    <items>      <item>        <key>CKT.Content Details</key>        <value>&lt;p&gt;0.10 mg/kg po at least 30-60 min prior FDG injection&lt;/p&gt;</value>      </item>    </items>  </annotations>  <annotations path="[openEHR-EHR-SECTION.adhoc.v1]">    <items>      <item>        <key>Technical.ï»© Technical Traceability</key>        <value>{~AHSID~752ecf36-882c-4e31-b807-1a9b7f368e5b~NAME~DI PT Brown Fat Supression Pediatric Order Panel}</value>      </item>    </items>  </annotations>  <definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="DI PT Brown Fat Supression Pediatric Order Panel">    <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="30