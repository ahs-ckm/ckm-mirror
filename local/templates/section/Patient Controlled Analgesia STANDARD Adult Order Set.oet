               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; PCA Standard Options (Single Select)&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;HYDROmorphone PCA Bolus Only&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                 <items>
                   <item>
                     <key>CKT.Clinical Guidance</key>
                     <value>&lt;p&gt;Suggest starting HYDROmorphone PCA dose: 0.2 mg with 6 min lockout and 6 mg four hour dose limit.&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; PCA Standard Options (Single Select)&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;HYDROmorphone PCA Bolus Only&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                 <items>
                   <item>
                     <key>CKT.Content Details</key>
                     <value>&lt;p&gt;PCA Bolus Only&lt;/p&gt;&lt;p&gt;PCA Dose: 0.2 mg (default) [speed buttons:&amp;nbsp;0.1 mg, 0.2 mg, 0.4 mg, 0.4 mg]&lt;/p&gt;&lt;p&gt;PCA Lockout: 6 minutes (default)&amp;nbsp; [speed buttons:&amp;nbsp;10 minutes, 8 minutes, 6 minutes]&lt;/p&gt;&lt;p&gt;Four Hour Dose Limit: 6 mg (default)&amp;nbsp;&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; PCA Standard Options (Single Select)&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Morphine PCA Bolus Only&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                 <items>
                   <item>
                     <key>CKT.Clinical Guidance</key>
                     <value>&lt;p&gt;Suggest starting morphine PCA dose 1 mg with 6 min lockout and 30 mg four hour dose limit.&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; PCA Standard Options (Single Select)&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Morphine PCA Bolus Only&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                 <items>
                   <item>
                     <key>CKT.Content Details</key>
                     <value>&lt;p&gt;PCA Bolus Only&lt;/p&gt;&lt;p&gt;PCA Dose: 1&amp;nbsp;mg (default) [speed buttons:&amp;nbsp;0.5&amp;nbsp;mg, 1&amp;nbsp;mg, 1.5&amp;nbsp;mg, 2 mg]&lt;/p&gt;&lt;p&gt;PCA Lockout: 6 minutes (default)&amp;nbsp; [speed buttons:&amp;nbsp;10 minutes, 8 minutes, 6 minutes]&lt;/p&gt;&lt;p&gt;Four Hour Dose Limit: 30&amp;nbsp;mg (default)&amp;nbsp;&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; PCA Standard Options (Single Select)&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;FentaNYL PCA Bolus Only&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                 <items>
                   <item>
                     <key>CKT.Clinical Guidance</key>
                     <value>&lt;p&gt;Suggest starting fentanyl PCA dose 15 mcg with 6 min lockout&amp;nbsp;&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; PCA Standard Options (Single Select)&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;FentaNYL PCA Bolus Only&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">
                 <items>
                   <item>
                     <key>CKT.Content Details</key>
                     <value>&lt;p&gt;PCA Bolus Only&lt;/p&gt;&lt;p&gt;PCA Dose: 15 mcg (default) [speed buttons:&amp;nbsp;10&amp;nbsp;mcg, 15 mcg, 20&amp;nbsp;mcg, 25&amp;nbsp;mcg]&lt;/p&gt;&lt;p&gt;PCA Lockout: 6 minutes (default)&amp;nbsp; [speed buttons: 10 minutes, 8 minutes, 6 minutes, 5 minutes]&lt;/p&gt;&lt;p&gt;Four Hour Dose Limit:&amp;nbsp;&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; Adjuvants (Single Select)&apos;]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">
                 <items>
                   <item>
                     <key>CKT.Clinical Guidance</key>
                     <value>&lt;p&gt;&lt;em&gt;Recommend starting at low doses and/or increased time intervals for elderly and opioid naïve patients.&lt;/em&gt;&lt;/p&gt;&lt;p&gt;Recommended standard dosing: 325 to 1000 mg.&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; Adjuvants (Single Select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;acetaminophen Tablet&apos;]/activities[at0001]">
                 <items>
                   <item>
                     <key>CKT.Content Details</key>
                     <value>&lt;p&gt;650 mg, oral, every 6 hours&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
               <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Medications&apos;]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Analgesics and Antipyretics; Adjuvants (Single Select)&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;acetaminophen Tablet #1&apos;]/activities[at0001]">
                 <items>
                   <item>
                     <key>CKT.Content Details</key>
                     <value>&lt;p&gt;1,000 mg, oral, every 6 hours&lt;/p&gt;</value>
                   </item>
                 </items>
               </annotations>
