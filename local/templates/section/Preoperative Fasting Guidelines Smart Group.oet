<?xml version="1.0"?> <template xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="openEHR/v1/Template">   <id>44ce6cb6-894d-4986-b608-7ca527304b36</id>   <name>Preoperative Fasting Guidelines Smart Group</name>   <description>     <lifecycle_state>Initial</lifecycle_state>     <details>       <purpose/>       <use/>       <misuse/>     </details>     <other_details>       <item>         <key>MetaDataSet:Sample Set </key>         <value>Template metadata sample set </value>       </item>       <item>         <key>Acknowledgements</key>         <value/>       </item>       <item>         <key>Business Process Level</key>         <value/>       </item>       <item>         <key>Care setting</key>         <value/>       </item>       <item>         <key>Client group</key>         <value/>       </item>       <item>         <key>Clinical Record Element</key>         <value/>       </item>       <item>         <key>Copyright</key>         <value/>       </item>       <item>         <key>Issues</key>         <value/>       </item>       <item>         <key>Owner</key>         <value/>       </item>       <item>         <key>Sign off</key>         <value/>       </item>       <item>         <key>Speciality</key>         <value/>       </item>       <item>         <key>User roles</key>         <value/>       </item>     </other_details>   </description>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/[openEHR-EHR-INSTRUCTION.service_request.v1]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Screen for nutrition risk: use Canadian Nutrition Screening Tool (CNST)&lt;/p&gt;&lt;p&gt;Refer to Registered Dietitian if CNST score equals 2 Yes responses&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;&lt;em&gt;Refer to AHS Pre-Operative Fasting and Carbohydrate Loading for Adult Patients Undergoing Surgical Interventions Requiring Anesthesia Care Guideline (link to be added once available).&lt;/em&gt;&lt;/p&gt;&lt;p&gt;&lt;em&gt;The minimum duration of pre-operative fasting prior to the administration of anesthesia should be 8 hours after a meal that includes meat or fried or fatty foods, 6 hours after a light meal (such as toast and a clear fluid), 2 hours after clear fluids.&lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-operative eating and drinking&apos;]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Clinical Communication: Final snack 8 hours prior to scheduled surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-operative eating and drinking&apos;]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication #1&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Clinical Communication: Clear Fluids until 3 hours prior to scheduled surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-operative eating and drinking&apos;]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;NPO&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;NPO 2 hours prior to scheduled surgery&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #1&apos;]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;&lt;em&gt;Refer to AHS Perioperative Management of Patients with Diabetes Mellitus, Adult – Inpatient Clinical Knowledge Topic (link to be added once available). The recommendation is to avoid carbohydrate loading&lt;/em&gt;&amp;nbsp;&lt;em&gt;patients with diabetes mellitus until prospective trials have been completed. &lt;/em&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.clinical_guidance.v1 and name/value=&apos;Clinical Guidance #2&apos;]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;&lt;strong&gt;&lt;em&gt;Clinicians should use independent medical judgement in the context of individual clinical circumstances and delete the following order if required.&lt;/em&gt;&lt;/strong&gt;&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-operative carbohydrate loading&apos;]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Apple juice or cranberry cocktail 500 mL PO 3 hours prior to scheduled surgery. Must be consumed by 2 hours prior to scheduled surgery&lt;/p&gt;&lt;p&gt;If patient’s admission is greater than 3 hours prior to scheduled&amp;nbsp;surgery, provide carbohydrate load&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.adhoc.v1 and name/value=&apos;Pre-operative carbohydrate loading&apos;]/[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication #1&apos;]/activities[at0001]">     <items>       <item>         <key>Transform.Clinical Notes</key>         <value>&lt;p&gt;Assess and document last consumption of food and fluids (including carbohydrate load&lt;/p&gt;</value>       </item>     </items>   </annotations>   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]"><items><item><key>Technical.ﻩ Technical Traceability</key><value>{~AHSID~44ce6cb6-894d-4986-b608-7ca527304b36~NAME~Preoperative Fasting Guidelines Smart Group.oet}</value></item></items></annotations><definition xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" name="Preoperative Fasting Guidelines Smart Group">     <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ddd49576-2a7e-42d7-b258-bcb0b9b1a420" concept_name="PC Nursing Communication Order" max="1" path="/items" name="Nursing Communication"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.clinical_guidance.v1" concept_name="Clinical Guidance" path="/items"/>     <Item xsi:type="SECTION" archetype_id="openEHR-EHR-SECTION.adhoc.v1" concept_name="Ad hoc heading" max="1" path="/items" name="Pre-operative eating and drinking">       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ddd49576-2a7e-42d7-b258-bcb0b9b1a420" concept_name="PC Nursing Communication Order" max="1" path="/items" name="Nursing Communication"/>       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="ddd49576-2a7e-42d7-b258-bcb0b9b1a420" concept_name="PC Nursing Communication Order" max="1" path="/items" name="Nursing Communication #1"/>       <Item xsi:type="INSTRUCTION" archetype_id="openEHR-EHR-INSTRUCTION.service_request.v1" template_id="4b26d247-8d30-4f36-907c-d6dc48d4b6cf" concept_name="NFS NPO Order" max="1" 