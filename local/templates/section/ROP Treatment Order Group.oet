   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;sucrose Oral liquid 24%; &apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;0.2 mL, buccal, once, as needed, for procedural pain as tolerated by the patient, Apply only one drop at a time onto the tongue OR apply drops of the solution onto soother and allow infant non-nutritive sucking on pacifier. Maximum 5 doses per day. Maximum dose only as tolerated by the patient&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;midazolam injection for nasal use 5 mg/mL&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;0.1 mg/kg/dose, intranasal, every 10 minutes, as needed, Pain management, Give 10 minutes prior to procedure&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;PLACEHOLDER; cyclopentolate-phenylephrine 0.2%-1% drop eye; 2 drops&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;1 drop, BOTH eyes, every 5 minutes, as needed, ROP Exam, Administer x 2 doses for FUNDOSCOPY - Administer 30 to 60 minutes prior&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;PLACEHOLDER; cyclopentolate-phenylephrine 0.2%-1% drop eye; 3 drops&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;1 drop, BOTH eyes, every 5 minutes, as needed, ROP Exam, Administer x 3&amp;nbsp;doses for FUNDOSCOPY - Administer 30 to 60 minutes prior&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tropicamide Eye drop 1%&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;1 drop, BOTH eyes, every 5 minutes, as needed, ROP Exam, Administer x 3 doses for FUNDOSCOPY - Administer 30 to 60 minutes prior&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;tetracaine Eye drop 0.5%; preservative free&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;1 drop, BOTH eyes, once, as needed, ROP Exam, Have available at bedside for Ophthalmology to administer&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-SECTION.orderable_section.v1]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;ranibizumab injection; 0.5 mg/0.05 mL injection 0.2 mg&apos;]/activities[at0001]">
