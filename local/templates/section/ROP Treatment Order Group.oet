   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Preprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;Nursing Communication; Ensure 2 Betadine 5% minims are available at bedside prior to ophthalmologist appointment&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;Once &amp;nbsp;for 1 occurrence, Ensure 2 Betadine 5% minims are available at bedside prior to ophthalmologist appointment. &amp;nbsp;NOT provided by pharmacy. Available in NICU medication fridge&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Postprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;PLACEHOLDER; moxifloxacin 0.5% drop eye&apos;]/activities[at0001]">
     <items>
       <item>
         <key>CKT.Content Details</key>
         <value>&lt;p&gt;&amp;nbsp;1 drop, BOTH eyes, every 6 hours, &amp;nbsp;for 7 days, Administer first dose following return from operating room., STAT&lt;/p&gt;</value>
       </item>
     </items>
   </annotations>
   <annotations path="[openEHR-EHR-SECTION.adhoc.v1]/items[openEHR-EHR-SECTION.orderable_section.v1 and name/value=&apos;Postprocedure&apos;]/items[openEHR-EHR-INSTRUCTION.service_request.v1 and name/value=&apos;atropine Eye drop 1%; preservative free&apos;]/activities[at0001]">
